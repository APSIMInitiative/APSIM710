<Type name="PatchData">
   <MetaData>
      <UItype>CSUserInterface.FileUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <LargeIcon>%apsim%\UserInterface\Images\thermometer32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\thermometer16.png</SmallIcon>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <dll>%apsim%/Model/PatchData.%dllext%</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[PatchData.name]" executable="[dll]" class="PatchData">
            <executable name="[dll]" version="1.0"/>
            <initdata>
                 [PatchData.xml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>