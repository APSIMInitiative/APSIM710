<Type name="accum">
   <MetaData>
      <UItype>VBUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\accum.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\debug32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\debug16.png</SmallIcon>
      <dll>%apsim%/Model/Accum.%dllext%</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[accum.name]" executable="[dll]">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               <accum_variables>[accum.variable]</accum_variables>
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>