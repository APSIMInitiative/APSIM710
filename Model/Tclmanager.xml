<Type name="tclmanager">
   <MetaData>
      <UItype>VBUserInterface.AreaUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\TclLink.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\folder_gear32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\folder_gear16.png</SmallIcon>
      <dll>%apsim%/Model/TclLink.%dllext%</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
         <drop>factor</drop>
      </drops>
      <ApsimToSim>
         <component name="[tclmanager.name]" executable="[dll]" class="TclLink.Tclmanager">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               <master>yes</master>
               [Children]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>