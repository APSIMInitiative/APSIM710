<Type name="tclmanager">
   <MetaData>
      <UItype>VBUserInterface.AreaUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation name="Science documentation">../apsim/tcllink/docs/tclink_science.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\folder_gear32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\folder_gear16.png</SmallIcon>
      <dll>%apsim%\Model\TclLink.dll</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[tclmanager.name]" executable="[dll]">
            <executable>[dll]</executable>
            <initdata>
               <master>yes</master>
               [Children]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>