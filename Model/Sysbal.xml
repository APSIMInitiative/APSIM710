<Type name="sysbal">
   <MetaData>
      <UItype>VBUserInterface.EmptyUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation name="Science documentation">../apsim/sysbal/docs/sysbal science.txt</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\debug32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\debug16.png</SmallIcon>
      <dll>%apsim%\Model\SysBal.dll</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[sysbal.name]" executable="[dll]">
            <executable name="[dll]" version="1.0"/>
            <initdata>
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>