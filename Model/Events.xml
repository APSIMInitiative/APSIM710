<Type name="events">
   <MetaData>
      <UItype>VBUserInterface.OutputFileDescUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <LargeIcon>%apsim%\UserInterface\Images\elements132.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\elements116.png</SmallIcon>
      <drops>
         <drop name="outputfile"/>
         <drop name="folder"/>
      </drops>
      <ApsimToSim>
         [foreach events.event]
         <outputfrequency>[if "[event.name]" = "Daily"]post[else][event.name][endif]</outputfrequency>
         [endfor]
      </ApsimToSim>
   </MetaData>
</Type>