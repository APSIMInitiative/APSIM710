<Type name="potato">
   <MetaData>
      <UItype>VBUserInterface.EmptyUI</UItype>
      <IsCrop>Yes</IsCrop>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\potato.jpg</Image>
      <Description>Most APSIM crop or plant modules do not require extra user input.  Crop specific parameters are either provided for your chosen soil, or are captured as part of the standard model validation process. NOTE THIS MODULE IS A PROTOTYPE ONLY.</Description>
      <Documentation name="potato science documentation">../apsim/potato/docs/potato_science.htm</Documentation>
      <Documentation name="Generic plant" >../apsim/plant/docs/plant_science.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\leaf32.bmp</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\leaf16.bmp</SmallIcon>
      <dll>%apsim%\Model\Plant.dll</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <class    name="plant"/>
      <cultivar name="MRPotatoHead"/>
      <variables link="%apsim%\Documentation\ModelInfo\potato.xml"/>
      <events link="%apsim%\Documentation\ModelInfo\potato.xml"/>
      <ApsimToSim>
         <component name="[Potato.name]" executable="[dll]">
            <executable>[dll]</executable>
            <initdata>
               [Model]
               <ll>[soil.ll]</ll>
               <kl>[soil.kl]</kl>
               <xf>[soil.xf]</xf>
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
   <Model>
      <!-- ***********************************************************************************************************
      CONSTANTS  - SPECIES-SPECIFIC PARAMETERS
      ***********************************************************************************************************
      -->
      <options>
         <Environment/>
         <Population/>
         <Fixation/>
         <Phenology/>
         <Root/>
         <GenericArbitratorXY/>
         <CohortingLeaf name="leaf"/>
         <Stem/>
         <Floret/>
         <Storage name="Tuber"/>
         <Parts name="Tops">
            <Leaf/>
            <Stem/>
            <Floret/>
         </Parts>
         <Parts name="Grain1">
            <Floret/>
         </Parts>    </options>
         <RadiationPartitioningOrder>floret leaf</RadiationPartitioningOrder>

         <crop_type>Potato</crop_type>
         <default_crop_class>plant</default_crop_class>
         <default_cultivar>russet</default_cultivar>
         <grain_part_type>harvest_index</grain_part_type>
         <stress_determinants>leaf stem meal</stress_determinants>
         <yield_parts>meal oil</yield_parts>
         <retrans_parts>leaf stem</retrans_parts>
         <!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
         SECTION - VALUE LIMITS - MAX AND MINS
         %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
         ______________________________________________________________________________
         OTHER VARIABLES
         limits set to check inputs
         -->
         <sw_ub>1.0</sw_ub>
         <sw_lb>0.0</sw_lb>
         <year_ub description="year upper limit">2000</year_ub>
         <year_lb description="year lower limit">1800</year_lb>
         <latitude_ub units="oL" description="latitude upper limit">90</latitude_ub>
         <latitude_lb units="oL" description="latitude lower limit">-90</latitude_lb>
         <maxt_ub units="oC" description="max. temperature upper limit">55</maxt_ub>
         <maxt_lb units="oC" description="max. temperature lower limit">0</maxt_lb>
         <mint_ub units="oC" description="min. temperature upper limit">40</mint_ub>
         <mint_lb units="oC" description="min. temperature lower limit">-10</mint_lb>
         <radn_ub units="MJ/m^2" description="radiation upper limit">50</radn_ub>
         <radn_lb units="MJ/m^2" description="radiation lower limit">1</radn_lb>
         <dlayer_ub units="mm" description="layer depth upper limit">1000</dlayer_ub>
         <dlayer_lb units="mm" description="layer depth lower limit">0</dlayer_lb>
         <dul_dep_ub units="mm" description="drained upper limit upper limit">1000</dul_dep_ub>
         <dul_dep_lb units="mm" description="drained upper limit lower limit">0</dul_dep_lb>
         <sw_dep_ub units="mm" description="soil water upper limit">1000</sw_dep_ub>
         <sw_dep_lb units="mm" description="soil water lower limit">0</sw_dep_lb>
         <no3_ub units="kg/ha" description="soil nitrate upper limit">10000</no3_ub>
         <no3_lb units="kg/ha" description="soil nitrate lower limit">0</no3_lb>
         <no3_min_ub units="kg/ha" description="min. soil nitrate upper limit">10000</no3_min_ub>
         <no3_min_lb units="kg/ha" description="min. soil nitrate lower limit">0</no3_min_lb>
         <nh4_ub units="kg/ha" description="soil ammonium upper limit">10000</nh4_ub>
         <nh4_lb units="kg/ha" description="soil ammonium lower limit">0</nh4_lb>
         <nh4_min_ub units="kg/ha" description="min. soil ammonium upper limit">10000</nh4_min_ub>
         <nh4_min_lb units="kg/ha" description="min. soil ammonium lower limit">0</nh4_min_lb>
         <!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
         SECTION 1 - CROP PHENOLOGY: Stage and Phase names
         %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
         -->
         <stage_code_list>1            2             3               4                    5                   6                7</stage_code_list>
         <stage_names>sowing    germination    emergence     end_of_juvenile    start_tuber_fill     end_tuber_fill       maturity</stage_names>
         <phase_names>sowing    germination    emergence     end_of_juvenile    start_tuber_fill     end_tuber_fill       maturity</phase_names>
         <phase_type>sowing     emergent       fixed              photo             fixed                fixed             fixed</phase_type>
         <!-- TI/STF sensitive to photoperiod apparently - short-day plant. Other models include sensitivity. Emerg to EOJ equivalent to BVP. EOJ to STF sensitive to PP.
         No need to distinguish between TI and STF as TI taken as when stolon tip 2X diameter (ie stf started)?
         Some farmers kill crop with herbicide when weeds a problem and or senescence delayed (should be handled by the kill_crop manager command)
         Typically, crop left until total above-ground senescence occurs (end of tuber fill) and then left under ground (to avoid greening) for 2-3 weeks for skin set (harvest ripe)
         Asuming that we do not need to simulate flowering.
         -->
         <composite_phases>under_ground    above_ground    leaf_senescence  node_formation    stress_reporting  tiller_formation  n_stress</composite_phases>
         <under_ground>sowing   germination</under_ground>
         <above_ground>emergence   end_of_juvenile    start_tuber_fill    end_tuber_fill    maturity</above_ground>
         <leaf_senescence>end_tuber_fill    maturity</leaf_senescence>
         <!-- Assuming leaf senescence commences after start_tuber_fill??
         reproductive = start_tuber_fill end_tuber_fill
         Assuming this refers to vegetative reproductive growth and not flower/seed production??
         emergence2floral_initiation  =    emergence end_of_juvenile tuber_initiation
         start_grain_fill2harvest_ripe =  start_tuber_fill end_tuber_fill
         preflowering  =                   emergence end_of_juvenile
         -->
         <node_formation>emergence end_of_juvenile start_tuber_fill end_tuber_fill</node_formation>
         <tiller_formation>emergence end_of_juvenile start_tuber_fill end_tuber_fill</tiller_formation>
         <!-- hi_stress_sensitive =
         -->
         <stress_reporting>start_tuber_fill end_tuber_fill</stress_reporting>
         <n_stress>emergence     end_of_juvenile    start_tuber_fill     end_tuber_fill       maturity</n_stress>
         <plant>
            <class_action>harvest</class_action>
            <class_change>unchanged</class_change>
            <n_uptake_option>1</n_uptake_option>
            <leaf_no_pot_option>1</leaf_no_pot_option>
            <n_retrans_option>1</n_retrans_option>
            <dm_senescence_option>1</dm_senescence_option>
            <n_senescence_option>1</n_senescence_option>
            <n_stress_option>1</n_stress_option>
            <partition_option>genericxy</partition_option>
            <partitionparts>root  leaf tuber stem</partitionparts>
            <partitionrules>magic frac frac  remainder</partitionrules>
            <fruit_no_option>1</fruit_no_option>
            <parasite_option>1</parasite_option>
            <!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            SECTION 0 - CO2
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            -->
            <co2_default units="ppm">350</co2_default>
            <photosynthetic_pathway units="C3, or C4">C3</photosynthetic_pathway>
            <x_co2_te_modifier units="ppm co2">350</x_co2_te_modifier>
            <y_co2_te_modifier>1.0</y_co2_te_modifier>
            <x_co2_nconc_modifier units="ppm co2">350</x_co2_nconc_modifier>
            <y_co2_nconc_modifier>1.0</y_co2_nconc_modifier>
            <stage_names>sowing    germination    emergence     end_of_juvenile     start_tuber_fill     end_tuber_fill     maturity</stage_names>
            <stage_code>1            2             3               4                    5                   6                 7</stage_code>
            <!-- __________________________________________________________________________________________________________-
            STEM REDUCTION EFFECT ON PHENOLOGY
            sow    ger   eme   juv  st_tf end_tf mat
            -->
            <stage_code_list description="numeric code for phenological stages">1      2    3     4     5     6      7</stage_code_list>
            <stage_stem_reduction_harvest>1      2    3     4     5     6      7</stage_stem_reduction_harvest>
            <stage_stem_reduction_kill_stem>1      2    3     4     5     6      7</stage_stem_reduction_kill_stem>
            <!-- Development setback associated with grazing - disabled SNL
            __________________________________________________________________________________________________________-
            SEED GERMINATION
            Water stress factor for germination
            -->
            <pesw_germ units="mm/mm" description="plant extractable soil water in seedling layer inadequate for germination (mm/mm)">0.00</pesw_germ>
            <!-- Only plant post dormant seed so no pesw threshold requirement.
            __________________________________________________________________________________________________________-
            EMERGENCE
            -->
            <shoot_lag units="oCd" description="Time lag before linear coleoptile growth starts (deg days)">72</shoot_lag>
            <!-- Lag phase for emergence inbetween germination planting and commencement of hypocotyl elongation (below)
            Sprouts active (germinated) at time of planting but require soil exposure to start elongating.
            Estimate from IRS trial
            -->
            <shoot_rate units="oCd/mm" description="Growing deg day increase with depth for coleoptile (deg day/mm depth)">6.3</shoot_rate>
            <!-- Estimate from IRS trial
            Water stress factor for emergence
            -->
            <fasw_emerg description="Fraction of available soil water">0.0  1.0    </fasw_emerg>
            <rel_emerg_rate description="Stress factor for thermal time calculation between germination and emergence">1.0  1.0    </rel_emerg_rate>
            <!-- Temperature regarded as main driver for emergence - hence deactivate for time being
            Time to emergence has been shown to vary with 'age' but IRS trials suggest no SD between PA treatments - ignore IRS.
            __________________________________________________________________________________________________________-
            PHOTOPERIOD AND TWILIGHT
            -->
            <twilight units="o" description="Twilight is defined as the interval between sunrise or sunset and the time when the true">-6.0</twilight>
            <!--  centre of the sun is 2.2 degrees below the horizon. Sunrise or sunset is defined as when
            the true centre of the sun is 50' below the horizon.
            -->
            <end_of_juvenile_pp_inductive_cutoff>end</end_of_juvenile_pp_inductive_cutoff>
            <!-- __________________________________________________________________________________________________________-
            Vernalisation
            -->
            <x_vernal_temp>0 2  8   15</x_vernal_temp>
            <y_vernal_days>0 0  0    0</y_vernal_days>
            <!--  Potato does not have a vernalisation requirement so set to nil
            __________________________________________________________________________________________________________-
            THERMAL TIME CALCULATION
            Thermal time calculation
            -->
            <x_temp units="oC" description="temperature table for thermal time">2.0  20.0  35.0</x_temp>
            <y_tt units="oCd" description="thermal time">0.0  18.0   0.0</y_tt>
            <!--  Values are based on substor (Ritchie et al, 1995) and other refs. LINTUL (kooman and Haverkort, 1995) suggest maximum temperature as high as 35oC.
            __________________________________________________________________________________________________________-
            LEAF NUMBER DEVELOPMENT
            currently leaf number at emergence will affect LAI at early stage, leaf_no_seed has no effect on the model, but will have in future
            -->
            <leaf_no_at_emerg description="(expanded) leaf number at emergence ()">1.5     </leaf_no_at_emerg>
            <!-- Based on IRS leaf trial
            -->
            <x_node_no_app>0    17</x_node_no_app>
            <y_node_app_rate>55   55</y_node_app_rate>
            <!-- Node appearance on main stem from IRS glasshouse trial. No evidence that later nodes come faster (as for canola)
            -->
            <x_node_no_leaf>0    10    13     17</x_node_no_leaf>
            <y_leaves_per_node>1    1     17     1</y_leaves_per_node>
            <!-- Based on IRS leaf trial and leaf no counts per node
            Most nodes have one MS leaf. 3 nodes post node number 10 have branching.
            Need to incorprate stem number functions here (sensitive to PA, soil moisture and temperature at sowing).
            Conceptual model consists of positive linear relationship between potential (optimum conditions) stem number and 'age' since harvest of 'seed' (thermal time)
            The PSN is then modified downward depending on soil temp and soil moisture at sowing
            Effect of stem number (and hence age) on leaf area per stem (no effect on leaf appearance from IRS trials).
            -->
            <x_lai_ratio description="ratio table for critical leaf size below which leaf number is reduced ()">0.0  0.01 0.8 1.0</x_lai_ratio>
            <y_leaf_no_frac description="reduction in leaf appearance ()">1.0  1.0 1.0 1.0</y_leaf_no_frac>
            <!-- Not sure what this does - doesn't change much with species
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            SECTION 2 - PHOTOSYNTHESIS, BIOMASS GROWTH AND PARTITION
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            PHOTOSYNTHESIS AND RADIATION USE EFFICIENCY (RUE)
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            STAGE DEPENDENT RUE
            sow    ger   eme  end_juv   st_tf   end_tf   mat
            -->
            <x_stage_rue description="numeric code for phenological stages">1      2     3    4         5       6        7</x_stage_rue>
            <y_rue units="g/MJ">0      0    1.6   1.6      1.6     1.6      1.6</y_rue>
            <!-- Suggest 1.6g/MJ during vegetative (Allen and Scott 2001) increasing to 2.5 g/MJ (Kooman and Havertkort, 1995) after tuber initiation. Higher value attributed to 'sink' strength of tuber??
            Seems high to me - check latter (SL)
            __________________________________________________________________________________________________________-
            EXTINCTION COEFFICIENT
            ROW SPACING DETERMINED
            -->
            <skiprow_default>0</skiprow_default>
            <row_spacing_default units="mm">350</row_spacing_default>
            <x_row_spacing>200  350  1000</x_row_spacing>
            <y_extinct_coef description="From SUBSTOR">0.55 0.55 0.55  </y_extinct_coef>
            <y_extinct_coef_dead description="unchanged from other species">0.30 0.30 0.30  </y_extinct_coef_dead>
            <!-- POD EXTINCTION COEFFICIENT and RUE
            -->
            <extinct_coef_floret>0.0</extinct_coef_floret>
            <spec_floret_area units="mm^2/g">0.00</spec_floret_area>
            <rue_floret units="g/MJ">0.00</rue_floret>
            <!-- __________________________________________________________________________________________________________-
            TEMPERATURE RESPONSE OF PHOTOSYNTHESIS - RUE
            -->
            <x_ave_temp units="oC" description="mean daily temperature">0  2  19  24  35</x_ave_temp>
            <y_stress_photo description="multiplier on RUE">0  0   1  1  0</y_stress_photo>
            <!-- from literature - Kooman and Haverkort (1995)
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            BIOMASS INITIATION, PARTITION AND TRANSLOCATION
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            INITIATION
            -->
            <root_dm_init units="g/plant" description="part weight at emergence">0.2</root_dm_init>
            <leaf_dm_init units="g/plant">0.03</leaf_dm_init>
            <stem_dm_init units="g/plant">0.0</stem_dm_init>
            <tuber_dm_init units="g/plant">0.0</tuber_dm_init>
            <floret_dm_init>0.0</floret_dm_init>
            <!-- __________________________________________________________________________________________________________-
            TRANSLOCATION PARTS
            -->
            <stemGrowthStructuralFractionStage>  7 7.01</stemGrowthStructuralFractionStage>
            <stemGrowthStructuralFraction>  .75 0.0</stemGrowthStructuralFraction>
            <leafGrowthStructuralFractionStage>  7 7.01</leafGrowthStructuralFractionStage>
            <leafGrowthStructuralFraction>  .85 0.0</leafGrowthStructuralFraction>

            <!-- __________________________________________________________________________________________________________-
            BIOMASS PARTITIONING
            sow    ger   eme  juv  st_tf end_tf   mat
            stage_code            = 1      2    3     4     5      6     7  ()     ! numeric code for phenological stages
            -->
            <x_stage_no_partition>1      2    3     4     5      6     7</x_stage_no_partition>
            <y_ratio_root_shoot units="0-1">0      0    1.0   0.33  0.33   0.081  0</y_ratio_root_shoot>
            <y_frac_leaf units="0-1" description="fraction of above ground dm allocated to leaves">0      0    0.80  0.55  0.40   0.1    0</y_frac_leaf>
            <y_frac_tuber units="0-1" description="fraction of below ground dm allocated to tuber">0      0    0     0.00  0.60   1.0    0</y_frac_tuber>
            <y_frac_floret units="0-1" description="fraction of below ground dm allocated to tuber">0      0    0     0    0       0     0</y_frac_floret>
            <!-- From IRS trials
            frac_tuber needs revisiting
            __________________________________________________________________________________________________________-
            GRAIN NUMBER DETERMINATION
            Want to simulate tuber number - stem number determines stolon (underground branch)number / stolon number determines tuber no.
            IRS + Beattie results show a positive linear relationship between tuber no / plant (y axis) and stem no / plant (x axis).
            y=5.3x-5.4 for Beattie and y=3.8x+1.7 for IRS.
            Can this be incorporated to enable simulation of tuber no (based on above stem no model derived from seed age)?
            Presumably, if we know tuber no and tuber biomass we can estimate size (a useful quality attribute)
            __________________________________________________________________________________________________________-
            GRAIN FILLING
            -->
            <sw_fac_max description="soil water stress factor maximum">1.125</sw_fac_max>
            <temp_fac_min description="temperature stress factor minimum">0.69</temp_fac_min>
            <sfac_slope description="soil water stress factor slope">-0.125</sfac_slope>
            <tfac_slope description="temperature stress factor slope">0.0125</tfac_slope>
            <carbo_oil_conv_ratio description="Carbohydrate:oil conversion ratio (>= 1.0)">2.1                 </carbo_oil_conv_ratio>
            <grain_oil_conc description="fractional oil content of grain (0-1)">0.45                </grain_oil_conc>
            <!-- __________________________________________________________________________________________________________-
            GRAIN WATER CONTENT
            -->
            <grn_water_cont units="g/g" description="water content of grain g/g">0.75</grn_water_cont>
            <!-- IRS field monitoring
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            SECTION 3 - LEAF/STEM/POD AREA GROWTH
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            ______________________________________________________________________________
            LEAF SIZE AS A FUNCTION OF LEAF NUMBER
            -->
            <node_no_correction>0.0</node_no_correction>
            <x_node_no>0     7    11     17</x_node_no>
            <y_leaf_size>1000 18000 18000  5000</y_leaf_size>
            <!-- IRS trial
            ______________________________________________________________________________
            SPECIFIC LEAF AREA - SLA
            -->
            <x_lai>0    8</x_lai>
            <y_sla_max description="maximum specific leaf area for delta LAI">35000 35000    </y_sla_max>
            <sla_min description="minimum specific leaf area for delta LAI">10000           </sla_min>
            <!-- ______________________________________________________________________________
            TPLA  - USED TO INTIALISE THE INITIAL LAI
            -->
            <initial_tpla units="mm^2/plant" description="initial total plant leaf area">100.</initial_tpla>
            <x_leaf_cohort>1    5    11     15</x_leaf_cohort>
            <y_leaf_growth_period units="oCd">150  200  500    500</y_leaf_growth_period>
            <y_leaf_lag_period units="oCd">50   100  200   200</y_leaf_lag_period>
            <y_leaf_sen_period units="oCd">150   150 150    150</y_leaf_sen_period>
            <y_leaf_area_pot units="mm^2/cohort">83000 83000 83000 83000</y_leaf_area_pot>
            <!-- ______________________________________________________________________________
            HARVESTING EFFECTS ON LEAF AREA
            -->
            <fr_height_cut>0.0  0.1    1</fr_height_cut>
            <fr_stem_remain>0.0  0.2    1</fr_stem_remain>
            <!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            SECTION 4 - ROOT DEPTH AND ROOT LENGTH GROWTH
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            __________________________________________________________________________________________________________-
            ROOTING DEPTH
            sow    ger  eme   end_juv   st_tf  end_tf  mat
            stage_code            = 1      2    3     4         5      6       7       ()     ! numeric code for phenological stages
            -->
            <root_depth_rate units="mm/d">0      5.0  5.0   44.0      44.0   44.0    0.0</root_depth_rate>
            <!-- Leave unchanged for time being
            Effect of sw_supply_demand_ratio on root depth increase
            -->
            <x_ws_root description="sw_supply_demand_ratio">0   1   </x_ws_root>
            <y_ws_root_fac description="effect of supply_demand_ratio on root depth increase">1   1   </y_ws_root_fac>
            <!-- Inactivated at present for all modules
            x_sw_ratio = [sw_dep(lay)-ll_dep(lay)]/[dul_dep(lay)-ll_dep(lay)] in the deepest root layer
            -->
            <x_sw_ratio description="water availability for effects on root depth growth">0.0 0.25</x_sw_ratio>
            <y_sw_fac_root description="stress factor for root depth growth, This may be probably the same for all crops">0   1</y_sw_fac_root>
            <!--  Consistent across other species models
            __________________________________________________________________________________________________________-
            TEMPERATURE EFFECT ON ROOT ADVANCE
            -->
            <x_temp_root_advance>0 20 35</x_temp_root_advance>
            <y_rel_root_advance>0 1  0</y_rel_root_advance>
            <!--  Based on cardinal temps for potato
            __________________________________________________________________________________________________________-
            ROOT LENGTH CALCULATION
            -->
            <initial_root_depth units="mm" description="initial depth of roots (mm)">100</initial_root_depth>
            <specific_root_length units="mm/g">40000</specific_root_length>
            <!-- Unchanged
            The following can increase or decrease the root length density in a layer based on the rld per plant
            -->
            <x_plant_rld units="mm/mm3/plant" description="root branching">0   0.0001  0.0003</x_plant_rld>
            <y_rel_root_rate units="0-1">0.02  0.10  1.0</y_rel_root_rate>
            <!--  Consistent across other species models
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            SECTION 5 - WATER RELATIONS AND WATER STRESS FACTORS
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            STAGE DEPENDENT TRANSPIRATION EFFICIENCY COEFFICIENTS
            Transpiration efficiency coefficient to convert vpd to transpiration efficiency (kpa)
            The following transp_eff_cf values are
            in the form kpa/g carbo per m^2 / g water per m^2 (kpa/g carbo per m^2 / mm water) because 1g water = 1 cm^3 water
            sow    ger   eme  end_juv   st_tf   end_tf  mat
            stage_code            = 1      2     3    4         5       6       7    ()     ! numeric code for phenological stages
            -->
            <transp_eff_cf units="Pa">0      0    .005  .005      .005    .005    .00</transp_eff_cf>
            <!-- Essential for determination of daily water limited biomass production (Transpiration efficiency derived from VPD and TEC. TE X SW supply = DM)
            Estimates from literature.
            __________________________________________________________________________________________________________-
            VPD FRACTION
            Fraction of distance between svp at min temp and svp at max temp where average svp during transpiration lies.
            -->
            <svp_fract description="(0-1)">0.75  </svp_fract>
            <eo_crop_factor_default description="default crop factor for limiting soil">100.0  </eo_crop_factor_default>
            <!--  water demand to some fraction/multiple
            of atmospheric potential.
            __________________________________________________________________________________________________________-
            WATER STRESS FACTORS FOR PHOTOSYNTHESIS, EXPANSION AND DEVELOPMENT
            swdef_photo for photosynthesis is calculated in the code as = sw_supply_sum/sw_demand - function crop_swdef_photo
            For expansion growth, more sensitive than other growth processes
            x_sw_demand_ratio = sw_supply_sum/sw_demand
            -->
            <x_sw_demand_ratio description="water availability">0.1 1.1</x_sw_demand_ratio>
            <y_swdef_leaf description="stress indices">0   1</y_swdef_leaf>
            <!-- For phenology stress factor based on available soil water portion
            sw_avail_pot_sum = sum_real_array (sw_avail_pot, deepest_layer),
            sw_avail_sum     = sum_real_array (sw_avail, deepest_layer)
            sw_avail_ratio   = divide (sw_avail_sum, sw_avail_pot_sum, 1.0) !???
            -->
            <x_sw_avail_ratio description="water availability">0.0 0.16</x_sw_avail_ratio>
            <y_swdef_pheno description="stress index for phenology">1.0 1.0</y_swdef_pheno>
            <x_sw_avail_ratio_flowering description="water availability">0.2   0.50  1.0</x_sw_avail_ratio_flowering>
            <y_swdef_pheno_flowering description="stress index for flowering phenology">1.0   1.0    1.0</y_swdef_pheno_flowering>
            <x_sw_avail_ratio_start_grain_fill description="water availability">0.2   0.50   1.0</x_sw_avail_ratio_start_grain_fill>
            <y_swdef_pheno_start_grain_fill description="stress index for grainfilling phenology">1.0   1.0    1.0</y_swdef_pheno_start_grain_fill>
            <!--  Consistent across other species models
            __________________________________________________________________________________________________________-
            WATERLOGGING STRESS (OXYGEN DEFICIT) FACTOR FOR PHOTOSYNTHESIS
            -->
            <oxdef_photo_rtfr description="fraction of roots waterlogged for effect on photosynthesis">0.0  0.8   1.0</oxdef_photo_rtfr>
            <oxdef_photo units="0-1" description="stress index on photosynthesis">1.0  1.0   1.0</oxdef_photo>
            <!--  Consistent across other species models
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            SECTION 6 -NITROGEN RELATIONS, UPTAKE AND STRESS FACTORS
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            ______________________________________________________________________________
            UPTAKE PREFERENCE AND CONSTANTS
            preference for n supply - it is assumed that mass flow is always predominant, but further crop preference can
            be specified as follows:
            'active'   = diffusive flow before fixation
            'fixation' = fixed N before active uptake.
            -->
            <n_supply_preference>active</n_supply_preference>
            <!-- Time constant for N uptake by diffusion. H van Keulen &  NG Seligman. Purdoe 1987.
            This is the  time it would take to remove, by diffusion, current amount of N if it wasn't depleted between time steps
            -->
            <no3_diffn_const units="days">2.0</no3_diffn_const>
            <!--  Consistent across other species models
            ______________________________________________________________________________
            LEGUME N FIXATION RATE - potential rate of N fixation (g N fixed per g above ground biomass)
            sow    ger  eme   end_juv     st_tf   end_tf  hv_rpe  end
            stage_code            = 1      2    3     4           5       6       7       8      ()     ! numeric code for phenological stages
            -->
            <N_fix_rate units="gN/gDM">0      0    0     0           0       0       0       0</N_fix_rate>
            <x_sw_avail_fix description="soil water ratio for effect on N fixation">0.0  0.50  0.95  1.0</x_sw_avail_fix>
            <y_swdef_fix units="0-1" description="stress index on N fixation">1.0  1.0   1.0   1.0</y_swdef_fix>
            <!-- N/A
            ______________________________________________________________________________
            NITROGEN STRESS FACTORS FOR PHOTOSYNTHESIS,LEAF GROWTH AND DEVELOPMENT
            n_conc_ratio      = divide ((N_conc_stover - N_conc_stover_min),(N_conc_stover_crit-N_conc_stover_min),0.0)
            n_conc_ratio_leaf = divide ((N_conc_leaf - N_conc_leaf_min), (N_conc_leaf_crit - N_conc_leaf_min), 0.0)
            g_nfact_photo     = N_fact_photo * N_conc_ratio
            g_n_fact_pheno    = N_fact_pheno * N_conc_ratio
            g_nfact_expansion = N_fact_expansion * n_conc_ratio_leaf
            -->
            <N_fact_photo description="multipler for N deficit effect on photosynthesis">2.0     </N_fact_photo>
            <N_fact_expansion description="multipler for N deficit effect on leaf expansion">0.7     </N_fact_expansion>
            <N_fact_pheno description="multipler for N deficit effect on phenology">100     </N_fact_pheno>
            <N_fact_grain description="multipler for N deficit effect on grain">1.   </N_fact_grain>
            <!-- Set by calibration against quality dataset
            ______________________________________________________________________________
            NITROGEN CONCENTRATION LIMITS: MAX. CRITICAL AND MINIMUM N CONCENTRATIONS
            eme    juv     st_tf   end_tf  mat
            -->
            <x_stage_code description="stage code used in table">3      4       5       6       7</x_stage_code>
            <y_n_conc_min_leaf description="leaf min N conc.">0.0550 0.0500  0.0400      0.0300  0.0050</y_n_conc_min_leaf>
            <y_n_conc_crit_leaf description="leaf critical N conc.">0.0650 0.0600  0.0550      0.0500  0.0500</y_n_conc_crit_leaf>
            <y_n_conc_max_leaf description="leaf max N conc.">0.0800 0.0750  0.0650      0.0550  0.0550</y_n_conc_max_leaf>
            <y_n_conc_min_stem description="stem min N conc.">0.0550 0.0500  0.0350      0.0300  0.0030</y_n_conc_min_stem>
            <y_n_conc_crit_stem description="stem critical N conc.">0.0650 0.0600  0.0500      0.0400  0.0400</y_n_conc_crit_stem>
            <y_n_conc_max_stem description="stem max N conc.">0.0800 0.0750  0.0550      0.0450  0.0450</y_n_conc_max_stem>
            <y_n_conc_min_floret description="stem min N conc.">0.0550 0.0500  0.0350      0.0300  0.0030</y_n_conc_min_floret>
            <y_n_conc_crit_floret description="stem critical N conc.">0.0650 0.0600  0.0500      0.0400  0.0400</y_n_conc_crit_floret>
            <y_n_conc_max_floret description="stem max N conc.">0.0800 0.0750  0.0550      0.0450  0.0450</y_n_conc_max_floret>
            <y_n_conc_min_tuber description="stem min N conc.">0.0550 0.0500  0.0350      0.0300  0.0030</y_n_conc_min_tuber>
            <y_n_conc_crit_tuber description="stem critical N conc.">0.0650 0.0600  0.0500      0.0400  0.0400</y_n_conc_crit_tuber>
            <y_n_conc_max_tuber description="stem max N conc.">0.0800 0.0750  0.0550      0.0450  0.0450</y_n_conc_max_tuber>
            <!--  new additions
            -->
            <n_conc_min_root description="root minimum N conc.">0.00800</n_conc_min_root>
            <n_conc_crit_root description="root critical N conc.">0.01000</n_conc_crit_root>
            <n_conc_max_root description="root max N conc.">0.01200</n_conc_max_root>
            <!-- ______________________________________________________________________________
            INITIAL NITROGEN CONCENTRATION OF ORGANS
            -->
            <root_n_init_conc>0.010</root_n_init_conc>
            <leaf_n_init_conc>0.065</leaf_n_init_conc>
            <stem_n_init_conc>0.040</stem_n_init_conc>
            <tuber_n_init_conc>0.040</tuber_n_init_conc>
            <floret_n_init_conc>0.040</floret_n_init_conc>
            <!-- ______________________________________________________________________________
            NITROGEN CONCENTRATION IN SENESCENCED ORGANS
            This means that as soon as leaves fall off, all N except for the amount below is
            instantly transferred from leaves to stem
            -->
            <root_n_sen_conc>0.008</root_n_sen_conc>
            <leaf_n_sen_conc>0.015</leaf_n_sen_conc>
            <stem_n_sen_conc>0.000</stem_n_sen_conc>
            <tuber_n_sen_conc description="new addition">0.0 </tuber_n_sen_conc>
            <floret_n_sen_conc description="new addition">0.0 </floret_n_sen_conc>
            <!-- ______________________________________________________________________________
            GRAIN NITROGEN CONCENTRATION
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            SECTION 7 -SENESCENCE AND DETACHMENT
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            ______________________________________________________________________________
            DM SENESCENCE
            -->
            <root_die_back_fr units="0-1" description="fraction of roots lost at harvest">0.1</root_die_back_fr>
            <!--  dm fraction senesced for each fraction of canopy senesced.
            -->
            <x_dm_sen_frac_root description="root">0    1     </x_dm_sen_frac_root>
            <y_dm_sen_frac_root>0.005 0.005</y_dm_sen_frac_root>
            <x_dm_sen_frac_leaf description="leaf">0    1     </x_dm_sen_frac_leaf>
            <y_dm_sen_frac_leaf>0    1</y_dm_sen_frac_leaf>
            <x_dm_sen_frac_stem description="stem">0    1     </x_dm_sen_frac_stem>
            <y_dm_sen_frac_stem>0    0</y_dm_sen_frac_stem>
            <x_dm_sen_frac_tuber description="tuber">0    1     </x_dm_sen_frac_tuber>
            <y_dm_sen_frac_tuber>0    0</y_dm_sen_frac_tuber>
            <x_dm_sen_frac_floret description="floret">0    1     </x_dm_sen_frac_floret>
            <y_dm_sen_frac_floret>0    0</y_dm_sen_frac_floret>
            <!-- ______________________________________________________________________________
            LEAF NUMBER AND AREA SENESCENCE
            -->
            <min_tpla description="lower limit of total plant leaf area">50.0</min_tpla>
            <!--  Consistent across most other species models
            -->
            <!--  Consistent across other species models
            -->
            <node_sen_rate units="oCd/node" description="rate of node senescence on main stem">114.0</node_sen_rate>
            <!--  From IRS trial
            -->
            <fr_lf_sen_rate description="fraction of total leaf number senescing per main stem node">0.04</fr_lf_sen_rate>
            <n_fact_lf_sen_rate description="max relative increase in leaf senescence due to N stress">0.0     </n_fact_lf_sen_rate>
            <!--  Consistent across most other species models
            -->
            <lai_sen_light units="m^2/m^2" description="induced senescence occurs (m^2/m^2)">4.0</lai_sen_light>
            <sen_light_slope description="sensitivity of leaf area senescence to shading">0.008</sen_light_slope>
            <!--  Consistent across most other species models
            x_temp_senescence = -15.0 -5.0     ! minimum temperature for frost senescence table (oC)
            -->
            <x_temp_senescence description="minimum temperature for frost senescence table (oC)">-15.0 -5.0     </x_temp_senescence>
            <y_senescence_fac description="daily fraction of leaf area senesced due to frost (0-1)">1    0       </y_senescence_fac>
            <!-- Set using literature temperature figures for frost susceptibility
            -->
            <sen_rate_water description="slope in linear eqn relating soil water stress during photosynthesis to leaf senesence rate">0.10</sen_rate_water>
            <!-- Leave as is for time being
            ______________________________________________________________________________
            DETACHMENT
            -->
            <root_dead_detach_frac description="fraction detached from each part of a dead plant per day.">0</root_dead_detach_frac>
            <root_sen_detach_frac description="fraction of senesced plant part detached each day from live plants">1</root_sen_detach_frac>
            <leaf_dead_detach_frac>0.0</leaf_dead_detach_frac>
            <leaf_sen_detach_frac>0.0</leaf_sen_detach_frac>
            <stem_dead_detach_frac>0.0</stem_dead_detach_frac>
            <stem_sen_detach_frac>0.0</stem_sen_detach_frac>
            <tuber_dead_detach_frac>0.0</tuber_dead_detach_frac>
            <tuber_sen_detach_frac>0.0</tuber_sen_detach_frac>
            <floret_dead_detach_frac>0.0</floret_dead_detach_frac>
            <floret_sen_detach_frac>0.0</floret_sen_detach_frac>
            <!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            SECTION 8 - WE MIGHT PUT RETRANSLOCATION OF DM AND N IN THIS SECTION
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            SECTION 9 - CROP FAILURE AND DEATH
            %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
            ______________________________________________________________________________
            crop failure
            -->
            <leaf_no_crit description="critical number of leaves, below which portion of the crop may die due to water stress">10.0</leaf_no_crit>
            <tt_emerg_limit units="oCd" description="maximum degree days allowed for emergence to take place">300.0</tt_emerg_limit>
            <days_germ_limit units="days" description="maximum days allowed after sowing for germination to take place (days)">40.0</days_germ_limit>
            <swdf_photo_limit description="critical cumulative photosynthesis water stress, above which the crop partly fails (unitless)">99.0</swdf_photo_limit>
            <swdf_photo_rate description="rate of plant reduction with photosynthesis water stress">0.0</swdf_photo_rate>
            <swdf_pheno_limit description="critical cumulative phenology water stress, above which the crop fails (unitless)">25</swdf_pheno_limit>
            <!--  Consistent across other species models
