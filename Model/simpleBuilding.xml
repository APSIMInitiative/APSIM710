<?xml version="1.0" encoding="utf-8"?>
<Type name="simpleBuilding">
  <MetaData>
    <UItype>CSUserInterface.GenericUI</UItype>
    <ShowInMainTree>Yes</ShowInMainTree>
    <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
    <Documentation>%apsim%\Documentation\4 Module Reference\erosion.htm</Documentation>
    <LargeIcon>%apsim%\UserInterface\Images\shovel32.bmp</LargeIcon>
    <SmallIcon>%apsim%\UserInterface\Images\shovel16.bmp</SmallIcon>
    <DLL>%apsim%\Model\simpleBuilding.dll</DLL>
    <drops>
      <drop>toolbox</drop>
      <drop>area</drop>
      <drop>simulation</drop>
      <drop>folder</drop>
    </drops>
    <ApsimToSim>
      <component name="[simpleBuilding.name]" executable="[dll]"> class="simpleBuilding"&gt;
        <executable name="[dll]" version="1.0" /><initdata>
        	[Model]
			[simpleBuilding.innerxml]
        </initdata></component>
    </ApsimToSim>
  </MetaData>
  <Model>
    <simpleBuilding>
      <changeDM type="text" description="Change in DM(daily)">10000</changeDM>
      <SlurryTo type="text" description="where the slurry should go to">noWare</SlurryTo>
      <slurry_pH type="text" description="Slurry pH">7.6</slurry_pH>
      <batch type="text" description="Set to 1 if in batch mode">1</batch>
      <slurry_Ash type="text" description="slurry_Ash">7.6</slurry_Ash>
      <slurry_TAN type="text" description="slurry_TAN">7.6</slurry_TAN>
      <slurry_RP type="text" description="slurry_RP">7.6</slurry_RP>
      <slurry_NDF type="text" description="slurry_NDF">7.6</slurry_NDF>
      <slurry_RL type="text" description="slurry_RL">7.6</slurry_RL>
      <slurry_VFA type="text" description="slurry_VFA">7.6</slurry_VFA>
      <slurry_ADL type="text" description="slurry_ADL">7.6</slurry_ADL>
      <slurry_DM type="text" description="slurry_DM">0.1</slurry_DM>
      <slurry_TotalS type="text" description="slurry_TotalS">7.6</slurry_TotalS>
      <slurry_SulphS type="text" description="slurry_SulphS">7.6</slurry_SulphS>
      <slurry_fInert type="text" description="slurry_fInert">7.6</slurry_fInert>
    </simpleBuilding>
  </Model>
</Type>