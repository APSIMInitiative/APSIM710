   <Type name="ocean_data">
      <MetaData>
      <UItype>VBUserInterface.FileUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation name="Science documentation">../apsim/input/docs/input_science.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\thermometer32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\thermometer16.png</SmallIcon>
      <dll>%apsim%\Model\Input.dll</dll>
      <drops>
         <drop name="toolbox"/>
         <drop name="area"/>
         <drop name="simulation"/>
         <drop name="folder"/>
     </drops>
      <variables>
         <variable name="sst" description="Sea surface temperature anomaly (oC)"/>
         <variable name="soi_index" description="SOI Index "/>
         <variable name="soi_phase" description="SOI phase"/>
         <variable name="epi" description="epi"/>
         <variable name="soi_a" description="SOI average"/>
         <variable name="nhi" description="nhi"/>
         <variable name="qri" description="qri"/>
         <variable name="swpi" description="swpi"/>
         <variable name="bom_Pacific_SSTs" description="bom"/>

      </variables>
      <ApsimToSim>
         <component name="[ocean_data.name]" executable="[dll]">
            <executable>[dll]</executable>
            <initdata>
               [ocean_data.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
      </MetaData>
   </Type>