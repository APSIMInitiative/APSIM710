<Type name="patchinput">
   <MetaData>
      <UItype>VBUserInterface.FileUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <LargeIcon>%apsim%\UserInterface\Images\thermometer32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\thermometer16.png</SmallIcon>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation name="Science documentation">../apsim/PatchInput/docs/PatchInput_science.htm</Documentation>
      <dll>%apsim%\Model\PatchInput.dll</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[patchinput.name]" executable="[dll]">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [patchinput.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>