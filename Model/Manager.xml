<Type name="manager">
   <MetaData>
      <UItype>VBUserInterface.RuleUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\Manager.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\manager32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\manager16.png</SmallIcon>
      <dll>%apsim%/Model/Manager.%dllext%</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
         <drop>factor</drop>
      </drops>
      <ApsimToSim>
         <component name="[manager.name]" executable="[dll]" class="Manager">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [manager.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>
