<Type name="manager">
   <MetaData>
      <UItype>VBUserInterface.RuleUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation name="Science documentation">../apsim/manager/docs/manager_science.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\manager32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\manager16.png</SmallIcon>
      <dll>%apsim%\Model\Manager.dll</dll>
      <drops>
         <drop name="toolbox"/>
         <drop name="area"/>
         <drop name="simulation"/>
         <drop name="folder"/>
      </drops>
      <ApsimToSim>
         <component name="[manager.name]" executable="[dll]">
            <executable>[dll]</executable>
            <initdata>
               [manager.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>
