<Type name="erosion">
   <MetaData>
      <UItype>CSUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\erosion.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\shovel32.bmp</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\shovel16.bmp</SmallIcon>
      <dll>%apsim%/Model/SoilErosion.%dllext%</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
         <drop>factor</drop>
      </drops>
      <ApsimToSim>
         <component name="[erosion.name]" executable="[dll]" class="Erosion">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [Model]
               [erosion.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
   <Model>
      <SoilErosion>
	    <model>freebairn</model>
		<profile_reduction>off</profile_reduction>
		<profile_layer_merge>0.05</profile_layer_merge>
		<minimum_depth>100.0</minimum_depth>
		<slope>6.5</slope>
		<slope_length>60</slope_length>
		<bed_depth>1900</bed_depth>
		<cover_extra>0</cover_extra>
		<k_factor>-1.0</k_factor>
		<k_factor_bed>-1.0</k_factor_bed>
		<k_factor_susp>-1.0</k_factor_susp>
		<p_factor>-1.0</p_factor>
		<entrain_eff>-1.0</entrain_eff>
		<entrain_eff_bed>-1.0</entrain_eff_bed>
		<entrain_eff_susp>-1.0</entrain_eff_susp>
		<eros_rose_b2>-1.0</eros_rose_b2>
		<eros_rose_b2_bed>-1.0</eros_rose_b2_bed>
		<eros_rose_b2_susp>-1.0</eros_rose_b2_susp>
		<wind_model>shao</wind_model>
		<flux_model>white</flux_model>
		<rho_air>0.00123</rho_air>
		<rho_particle>2.65</rho_particle>
		<Shao_gamma>0.165</Shao_gamma>
		<Shao_An>0.0123</Shao_An>
		<moisture_option>1</moisture_option>
		<percent_clay>10.0</percent_clay>
		<smooth_roughness_length>0.0015</smooth_roughness_length>
		<aeolian_roughness_length>0.2</aeolian_roughness_length>
		<drag_partition_option>2</drag_partition_option>
		<sigma_bare>1.0</sigma_bare>
		<sigma_vegetated>1.45</sigma_vegetated>
		<beta_bare>90.0</beta_bare>
		<beta_vegetated>202.0</beta_vegetated>
		<m_bare>0.5</m_bare>
		<m_vegetated>0.16</m_vegetated>
		<c_lambda>0.35</c_lambda>
		<coeff_flux>1.0</coeff_flux>
		<shao_k_gamma>1.0</shao_k_gamma>
		<shao_c_y>0.000057</shao_c_y>
		<plastic_pressure>20250.0</plastic_pressure>
		<dust_cutoff>20.0</dust_cutoff>
		<n_classes>200</n_classes>
		<soil_type>loam</soil_type>
		<wind_height>10.0</wind_height>
      </SoilErosion>
   </Model>
 </Type>
