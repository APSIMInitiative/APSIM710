<Type name="Saltbush">
  <MetaData>
    <UItype>VBUserInterface.EmptyUI</UItype>
    <IsCrop>Yes</IsCrop>
    <ShowInMainTree>Yes</ShowInMainTree>
    <Image>%apsim%\UserInterface\Images\Saltbush.jpg</Image>
    <Description>NOTE: Prototype only.  This module is not to be used in model applications.  Most APSIM crop or plant modules do not require extra user input.  Crop specific parameters are either provided for your chosen soil, or are captured as part of the standard model validation process.</Description>
    <Documentation name="Generic plant">../apsim/plant/docs/plant_science.htm</Documentation>
    <LargeIcon>%apsim%\UserInterface\Images\leaf32.bmp</LargeIcon>
    <SmallIcon>%apsim%\UserInterface\Images\leaf16.bmp</SmallIcon>
    <dll>%apsim%\Model\Plant2.dll</dll>
    <drops>
      <drop>toolbox</drop>
      <drop>area</drop>
      <drop>simulation</drop>
      <drop>folder</drop>
    </drops>
    <variables link="%apsim%\Documentation\ModelInfo\Saltbush.xml" />
    <events link="%apsim%\Documentation\ModelInfo\Saltbush.xml" />
    <ApsimToSim>
      <component name="[InstanceName]" executable="[dll]" class="Plant2.Saltbush">
        <executable>[dll]</executable>
        <initdata>
               [Model]
               <ll>[soil.ll]</ll><kl>[soil.kl]</kl><xf>[soil.xf]</xf></initdata>
      </component>
    </ApsimToSim>
  </MetaData>
  <Model>
    <Cultivar name="simba">
      <Construct>Plant</Construct>
    </Cultivar>
    <Plant>
      <CropType>Saltbush</CropType>
      <Population>
        <Value>4</Value>
      </Population>
      <AnnualPlantArbitrator name="Arbitrator">
        <DMSink>Stem</DMSink>
        <NSink>Stem</NSink>
      </AnnualPlantArbitrator>
      <Phenology>
        <TemperatureFunction name="ThermalTime">
          <XYPairs>
            <XY> 5 1.0</XY>
            <XY>25 1.0</XY>
            <XY>35 1.0</XY>
            <XY>40 1.0</XY>
          </XYPairs>
        </TemperatureFunction>
      <FixedPhase name="Growth">
         <Constant name="Target">
           <Value>1e6</Value>
         </Constant>
         <Start>Start</Start>
         <End>End</End>
         <TemperatureFunction shortcut="/Plant/Phenology/ThermalTime"/>
      </FixedPhase>
      <EndPhase name="Finished">
         <Start>End</Start>
         <End>Unused</End>
         <TemperatureFunction shortcut="/Plant/Phenology/ThermalTime"/>
      </EndPhase>

      </Phenology>
      <Leaf>
        <CompositeBiomass name="Live">
           <Property>Leaf.Leaves[].Live</Property>
        </CompositeBiomass>
        <CompositeBiomass name="Dead">
           <Property>Leaf.Leaves[].Dead</Property>
        </CompositeBiomass>
        <PrimaryBudNo>1</PrimaryBudNo>
        <MaxCover>1.0</MaxCover>
            <Constant name="ExtinctionCoeff">
              <Value>0.5</Value>
            </Constant>
        <KDead>0.5</KDead>
        <LinearInterpolationFunction name="Height">
          <XProperty>Stem.Live.Wt</XProperty>
          <XYPairs>
            <XY> 0 100.0</XY>
            <XY>300 750.0</XY>
          </XYPairs>
        </LinearInterpolationFunction>
        <LinearInterpolationFunction name="FrostFraction">
          <XProperty>Leaf.MinT</XProperty>
          <XYPairs>
            <XY>-1 0.0</XY>
            <XY>0 0.0</XY>
          </XYPairs>
        </LinearInterpolationFunction>
        <RUEModel name="Photosynthesis">
         <FT>
            <XY>5 0.0</XY>
            <XY>15 1.0</XY>
            <XY>28 1.0</XY>
            <XY>35 1.0</XY>
            <XY>40 0.0</XY>
         </FT>
         <FVPD>
            <XY> 0 1.0</XY>
            <XY>10 1.0</XY>
            <XY>50 1.0</XY>
         </FVPD>
         <RUE>1.2</RUE>
        </RUEModel>
            <Constant name="PartitionFraction">
              <Value>0.4</Value>
            </Constant>
        <TemperatureFunction name="ThermalTime">
          <XYPairs>
            <XY> 5 1.0</XY>
            <XY>25 1.0</XY>
            <XY>37 1.0</XY>
          </XYPairs>
        </TemperatureFunction>
            <Constant name="NodeInitiationRate">
              <Value>7</Value>
            </Constant>
            <Constant name="NodeAppearanceRate">
              <Value>7</Value>
            </Constant>

        <MaxNodeNo>100</MaxNodeNo>
        <LeafCohort name="Leaves[1]">
           <Rank>1</Rank>
           <Area>100</Area>
           <Age>1</Age>
        </LeafCohort>
        <InitialiseStage>Start</InitialiseStage>
        <InitialLeafPrimordia>1</InitialLeafPrimordia>
        <LinearInterpolationFunction name="ExpansionStress">
          <XProperty>WaterSupplyDemandRatio</XProperty>
          <XYPairs>
            <XY> 0.0 0</XY>
            <XY> 1.0 1</XY>
          </XYPairs>
        </LinearInterpolationFunction>
            <Constant name="MaxArea">
              <Value>10000.</Value>
            </Constant>
            <Constant name="SpecificLeafAreaMax">
              <Value>10000.</Value>
            </Constant>
            <Constant name="GrowthDuration">
              <Value>30.</Value>
            </Constant>
            <Constant name="LagDuration">
              <Value>30.</Value>
            </Constant>
            <Constant name="SenescenceDuration">
              <Value>30.</Value>
            </Constant>
        <LinearInterpolationFunction name="BranchingRate">
          <XProperty>Leaf.NodeNo</XProperty>
          <XYPairs>
            <XY> 1 0.0</XY>
            <XY> 12 0.0</XY>
            <XY> 24 0.0</XY>
          </XYPairs>
        </LinearInterpolationFunction>
        <Constant name="MaximumNConc">
          <Value>0.0</Value>
        </Constant>        
        <Constant name="MinimumNConc">
          <Value>0.0</Value>
        </Constant>        
      </Leaf>
      <GenericOrgan name="Stem">
        <Biomass name="Live" />
        <Biomass name="Dead" />
        <Constant name="PartitionFraction">
          <Value>0.4</Value>
        </Constant>
        <Constant name="StructuralFraction">
          <Value>1.0</Value>
        </Constant>
        <Constant name="StructuralNConc">
          <Value>0.003</Value>
        </Constant>
        <Constant name="MaximumNConc">
          <Value>0.02</Value>
        </Constant>        
      </GenericOrgan>
      <Root>
        <CompositeBiomass name="Live">
           <Property>Root.LayerLive[]</Property>
        </CompositeBiomass>
        <CompositeBiomass name="Dead">
           <Property>Root.LayerDead[]</Property>
        </CompositeBiomass>
        <ll>   0.29  0.29  0.29  0.29  0.3   0.31  0.32  0.33  0.34  0.35  0.36 </ll>
        <kl>   0.07  0.07  0.07  0.07  0.05  0.05  0.04  0.04  0.04  0.04  0.04 </kl>
        <xf>   1.00  1.00  1.00  1.00  1.00  1.00  1.00  1.00  1.00  1.00  1.00 </xf>
        <InitialWt>0.2</InitialWt>
        <SpecificRootLength>40000</SpecificRootLength>
        <LinearInterpolationFunction name="KLModifier">
          <XProperty>Leaf.LAI</XProperty>
          <XYPairs>
            <XY> 0.0 1.0</XY>
            <XY> 3.0 1.0</XY>
          </XYPairs>
        </LinearInterpolationFunction>
        <Constant name="SoilWaterEffect">
          <Value>1.0</Value>
        </Constant>
        
        <Constant name="RootFrontVelocity">
          <Value>20.0</Value>
        </Constant>        
        <Constant name="PartitionFraction">
          <Value>0.2</Value>
        </Constant>
        <TemperatureFunction name="TemperatureEffect">
          <XYPairs>
            <XY> 0 1.0</XY>
            <XY>26 1.0</XY>
            <XY>35 1.0</XY>
          </XYPairs>
        </TemperatureFunction>
        <Constant name="MaximumNConc">
          <Value>0.01</Value>
        </Constant>        
            <KNO3>0.02</KNO3>
            <KNH4>0.0</KNH4>       
        <Constant name="MaxDailyNUptake">
          <Value>6.0</Value>
        </Constant> 
        <Constant name="SenescenceRate">
          <Value>0.0</Value>
        </Constant>        
      </Root>
      <Summary name="Summary" />
    </Plant>
  </Model>
  
</Type>