<Type name="pigs">
  <MetaData>
      <UItype>VBUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\erosion.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\shovel32.bmp</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\shovel16.bmp</SmallIcon>
	  <DLL>%apsim%\Model\pigs.dll</DLL>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
    <ApsimToSim>
      <component name="[pigs.name]" executable="[dll]"> class="pigs">
        <executable name="[dll]" version="1.0" />
        <initdata>
        	[Model]
			[pigs.innerxml]
        </initdata>
      </component>
    </ApsimToSim>
  </MetaData>
  <Model>
     <pigs>
        <amountFluid type="text" description="amountFluid
 a constant (unitless)">3</amountFluid >
 <amountSolid type="text" description="amountSolid a constant (unitless)">3</amountSolid>
 <P_growth type="text" description="P_growth a constant (unitless)">3</P_growth>
 <K_growth type="text" description="K_growth a constant (unitless)">3</K_growth>
 <N_growth type="text" description="N_growth a constant (unitless)">3</N_growth> 
<FE_need type="text" description="FE_needa constant (unitless)">3</FE_need>
<NumberPrYear type="text" description="NumberPrYear constant (unitless)">3</NumberPrYear>
	</pigs>
  </Model>
</Type>