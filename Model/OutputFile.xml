
<Type name="outputfile">
   <MetaData>
      <UItype>VBUserInterface.FileUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation name="Science documentation">../apsim/report/docs/report_science.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\table_selection_column32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\table_selection_column16.png</SmallIcon>
      <dll>%apsim%\Model\Report.dll</dll>
      <drops>
         <drop name="toolbox"/>
         <drop name="area"/>
         <drop name="simulation"/>
         <drop name="folder"/>
      </drops>
      <ApsimToSim>
         <component name="[outputfile.name]" executable="[dll]">
            <executable>[dll]</executable>
            <initdata>
            [Children]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>
