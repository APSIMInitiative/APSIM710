<Type name="log">
   <MetaData>
      <UItype>VBUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation name="Science documentation">../apsim/log/docs/log.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\debug32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\debug16.png</SmallIcon>
      <dll>%apsim%\Model\Log.dll</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[log.name]" executable="[dll]">
            <executable>[dll]</executable>
            <initdata>
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>