<Type name="irrigation">
   <MetaData>
      <UItype>VBUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\Irrigation.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\drink_blue32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\drink_blue16.png</SmallIcon>
      <dll>%apsim%/Model/Irrigation.%dllext%</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>folder</drop>
         <drop>factor</drop>
      </drops>
      <ApsimToSim>
         <component name="[irrigation.name]" executable="[dll]" class="Irrigation">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [irrigation.innerxml]
               <solutes>nh4 no3 cl</solutes>
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
   
</Type>