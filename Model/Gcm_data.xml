<Type name="gcm_data">
   <MetaData>
      <UItype>VBUserInterface.FileUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation name="Science documentation">../apsim/input/docs/input_science.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\thermometer32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\thermometer16.png</SmallIcon>
      <dll>%apsim%\Model\Input.dll</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[gcm_data.name]" executable="[dll]">
            <executable>[dll]</executable>
            <initdata>
               [gcm_data.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
   <Info>
      <variables>
         <variable name="cerfac" description="cerfac"/>
         <variable name="ecmwf" description="ecmwfm"/>
         <variable name="ingv" description="ingv"/>
         <variable name="lodyc" description="lodyc"/>
         <variable name="meteo" description="meteo"/>
         <variable name="mpi" description="mpi"/>
         <variable name="ukmet" description="ukmet"/>
      </variables>
   </Info>
</Type>