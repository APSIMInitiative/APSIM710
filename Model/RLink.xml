<Type name="RLink">
   <MetaData>
      <UItype>CSUserInterface.RuleUI</UItype>
      <ApsimXCompatible>Yes</ApsimXCompatible>	  
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\RLink.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\user1-32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\user1-16.png</SmallIcon>
      <DLL>%apsim%\Model\RLink.%dllext%</DLL>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[RLink.name]" executable="[dll]" class="RLink">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [RLink.innerxml]   
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>
