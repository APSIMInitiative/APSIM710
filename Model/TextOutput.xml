<!-- Configured for the .net version of the TextOut component. 4/7/2012 -->
  <Type name="TextOutput">
    <MetaData>
      <UItype>CPIUserInterface.TextOutputUI</UItype>
      <IsCrop>No</IsCrop>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <LargeIcon>%apsim%\UserInterface\Images\textoutput32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\textoutput16.png</SmallIcon>
      <dll>%apsim%\Model\Textoutput.dll</dll>
      <drops>
        <drop>toolbox</drop>
        <drop>area</drop>
        <drop>simulation</drop>
        <drop>folder</drop>
      </drops>
      <ApsimToSim>
        <component name="[TextOutput.name]" executable="[dll]" class="TextOutput">
          <executable name="[dll]" version="1.0" />
          <initdata>
          [Model]
          <![CDATA[
             [TextOutput.innerxml]
          ]]></initdata>
        </component>
      </ApsimToSim>
    </MetaData>
    <Model>
      <Textoutput/>
    </Model>
     <Info>
      <variables>
        <variable name="name" description="" read="T" write="F" kind="string" array="" units="" />
        <variable name="type" description="" read="T" write="F" kind="string" array="" units="" />
        <variable name="version" description="" read="T" write="F" kind="string" array="" units="" />
        <variable name="author" description="" read="T" write="F" kind="string" array="" units="" />
        <variable name="active" description="" read="T" write="F" kind="boolean" array="" units="" />
        <variable name="state" description="" read="T" write="T" kind="string" array="" units="" />
      </variables>
      <events> <!-- published events -->
        <event name="error" description="" />
      </events>
     </Info>

  </Type>


