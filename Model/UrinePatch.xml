<Type name="UrinePatch">
  <MetaData>
    <UItype>VBUserInterface.GenericUI</UItype>
    <IsCrop>No</IsCrop>
    <ShowInMainTree>Yes</ShowInMainTree>
    <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
    <Documentation>Urine Patch Model</Documentation>
    <LargeIcon>%apsim%\UserInterface\Images\cubes32.png</LargeIcon>
    <SmallIcon>%apsim%\UserInterface\Images\cubes16.png</SmallIcon>
    <dll>%apsim%\Model\UrinePatch.dll</dll>
	  <drops>
      <drop>toolbox</drop>
      <drop>area</drop>
      <drop>simulation</drop>
      <drop>folder</drop>
    </drops>
    <ApsimToSim>
      <component name="[UrinePatch.name]" executable="[dll]" class="UrinePatch">
        <executable>[dll]</executable>
        <initdata>
          [Model]
          [UrinePatch.innerxml]
        </initdata>
      </component>
    </ApsimToSim>
  </MetaData>
  <Model>
    <UrinePatch>
    </UrinePatch>
  </Model>
  
</Type>