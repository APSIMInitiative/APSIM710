<Type name="erosion">
   <MetaData>
      <UItype>VBUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\erosion.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\shovel32.bmp</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\shovel16.bmp</SmallIcon>
      <dll>%apsim%/Model/Erosion.%dllext%</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[erosion.name]" executable="[dll]">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [Model]
               [erosion.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
   <Info>
      <variables>
         <variable name="soil_loss" description="Soil loss (t/ha)"/>
         <variable name="soil_loss_mm" description="Soil loss (mm)"/>
         <variable name="sed_conc" description="Sediment concentration (g/l)"/>
         <variable name="bed_depth" description="Depth to bedrock (mm)"/>
      </variables>
   </Info>
</Type>