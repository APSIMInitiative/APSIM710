<Type name="paddockmanager">
   <MetaData>
      <UItype>VBUserInterface.AreaUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation name="Science documentation">../apsim/tcllink/docs/farmmanager_science.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\folder_gear32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\folder_gear16.png</SmallIcon>
      <dll>%apsim%\Model\TclLink.dll</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[paddockmanager.name]" executable="[dll]">
            <executable>[dll]</executable>
            <initdata>
               <master>no</master>
               [Children]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>