<Type name="soiltemp">
   <MetaData>
      <UItype>VBUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\SoilTemp.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\thermometer32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\thermometer16.png</SmallIcon>
      <dll>%apsim%/Model/SoilTemp.%dllext%</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
      <ApsimToSim>
         <component name="[SoilTemp.name]" executable="[dll]">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [Model]
               [SoilTemp.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
   <Model>
      <nu description="(-) control forward/backward differencing">0.6 </nu>
      <vol_spec_heat_om description="(Joules*m-3*K-1)">5.00e6 </vol_spec_heat_om>
      <vol_spec_heat_water description="(Joules*m-3*K-1)">4.18e6 </vol_spec_heat_water>
      <vol_spec_heat_clay description="(Joules*m-3*K-1)">2.39e6 </vol_spec_heat_clay>
      <maxt_time_default description="hours">14.0 </maxt_time_default>
   </Model>
   
</Type>