
<Type name="DDRules">
	<MetaData>
		<UItype>VBUserInterface.EmptyUI</UItype>
		<IsCrop>No</IsCrop>
		<ShowInMainTree>Yes</ShowInMainTree>
		<Image>%apsim%\Model\DDRules\Documents\DDRules.jpg</Image>
		<Description>NOTE: Prototype only. Dairy farm mangnager component base on simple rules created by Dawn Dally of DairyNZ</Description>
		<Documentation>%apsim%\Model\DDRules\Documents\index.html</Documentation>
		<LargeIcon>%apsim%\UserInterface\Images\cubes32.png</LargeIcon>
		<SmallIcon>%apsim%\UserInterface\Images\cubes16.png</SmallIcon>
		<dll>%apsim%\Model\DDRules.dll</dll>
		<drops>
			<drop>toolbox</drop>
			<drop>area</drop>
			<drop>simulation</drop>
			<drop>folder</drop>
		</drops>
		<ApsimToSim>
			<component name="[DDRules.name]" executable="[dll]">
				<componentinterface>%apsim%\Model\dotnetcomponentinterface.dll</componentinterface>
				<executable>[dll]</executable>
				<initdata>
					[Model]
					[DDRules.innerxml]	     
				</initdata>
			</component>
		</ApsimToSim>
	</MetaData>
	<Model>
		<DDRules>
			<DebugLevel>0</DebugLevel>
			<Cow_Interpolation>0</Cow_Interpolation>
			<BreakFeeding>0</BreakFeeding>
			<GrowthRateWindowSize>30</GrowthRateWindowSize>
		</DDRules>
	</Model>
	
</Type>