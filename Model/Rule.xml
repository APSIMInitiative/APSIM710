<Type name="rule">
   <MetaData>
      <UItype>VBUserInterface.RuleUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <LargeIcon>%apsim%\UserInterface\Images\tclmanager32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\tclmanager16.png</SmallIcon>
      <drops>
         <drop name="tclmanager"/>
         <drop name="farmmanager"/>
         <drop name="paddockmanager"/>
      </drops>
      <ApsimToSim>
         <component name="[manager.name]" executable="[dll]">
            <executable>[dll]</executable>
            <initdata>
               [manager.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>