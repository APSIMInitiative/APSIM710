<Type name="frenchbean">
  <MetaData>
    <UItype>VBUserInterface.EmptyUI</UItype>
    <IsCrop>Yes</IsCrop>
    <ShowInMainTree>Yes</ShowInMainTree>
    <Image>%apsim%\UserInterface\Images\frenchbean.jpg</Image>
    <Description>NOTE: Prototype only.  This module is not to be used in model applications.  Most APSIM crop or plant modules do not require extra user input.  Crop specific parameters are either provided for your chosen soil, or are captured as part of the standard model validation process.</Description>
    <Documentation name="Generic plant">../apsim/plant/docs/plant_science.htm</Documentation>
    <LargeIcon>%apsim%\UserInterface\Images\leaf32.bmp</LargeIcon>
    <SmallIcon>%apsim%\UserInterface\Images\leaf16.bmp</SmallIcon>
    <dll>%apsim%\Model\Plant2.dll</dll>
    <drops>
      <drop>toolbox</drop>
      <drop>area</drop>
      <drop>simulation</drop>
      <drop>folder</drop>
    </drops>
    <variables link="%apsim%\Documentation\ModelInfo\frenchbean.xml" />
    <events link="%apsim%\Documentation\ModelInfo\frenchbean.xml" />
    <ApsimToSim>
      <component name="[InstanceName]" executable="[dll]">
        <executable>[dll]</executable>
        <componentinterface>%apsim%\Model\dotnetcomponentinterface.dll</componentinterface>
        <initdata>
               [Model]
               <ll>[soil.[InstanceName] ll(mm/mm)]</ll>
               <kl>[soil.[InstanceName] kl(/day)]</kl>
               <xf>[soil.[InstanceName] xf(0-1)]</xf></initdata>
      </component>
    </ApsimToSim>
  </MetaData>
  <Model>
    <Cultivar name="simba">
      <Construct>Plant</Construct>
    </Cultivar>
    <Plant>
      <CropType>frenchbean</CropType>
      <Population>
        <Value>4</Value>
      </Population>
      <AnnualPlantArbitrator name="Arbitrator">
        <DMSink>Stem</DMSink>
        <NSink>Stem</NSink>
      </AnnualPlantArbitrator>
      <Phenology>
        <TemperatureFunction name="ThermalTime">
          <XYPairs>
            <XY> 5 0.0</XY>
            <XY>25 20.0</XY>
            <XY>35 20.0</XY>
            <XY>40 0.0</XY>
          </XYPairs>
        </TemperatureFunction>
      <GenericPhase name="Emergent">
         <Constant name="Target">
           <Value>100</Value>
         </Constant>
         <Start>Germination</Start>
         <End>Emergence</End>
         <TemperatureFunction shortcut="/FrenchBean/Model/Plant/Phenology/ThermalTime"/>
      </GenericPhase>
      <GenericPhase name="Vegetative">
         <Constant name="Target">
           <Value>529</Value>
         </Constant>
         <Start>Emergence</Start>
         <End>Flowering</End>
         <TemperatureFunction shortcut="/FrenchBean/Model/Plant/Phenology/ThermalTime"/>
      </GenericPhase>
      <GenericPhase name="Flowering">
         <Constant name="Target">
           <Value>185</Value>
         </Constant>
         <Start>Flowering</Start>
         <End>StartPodGrowth</End>
         <TemperatureFunction shortcut="/FrenchBean/Model/Plant/Phenology/ThermalTime"/>
      </GenericPhase>
      <GenericPhase name="PodFilling">
         <Constant name="Target">
           <Value>265</Value>
         </Constant>
         <Start>StartPodGrowth</Start>
         <End>Maturity</End>
         <TemperatureFunction shortcut="/FrenchBean/Model/Plant/Phenology/ThermalTime"/>
      </GenericPhase>
      <EndPhase name="HarvestRipe">
         <Start>Maturity</Start>
         <End>Unused</End>
         <TemperatureFunction shortcut="/FrenchBean/Model/Plant/Phenology/ThermalTime"/>
      </EndPhase>

      </Phenology>
      <Leaf>
        <Biomass name="Live" />
        <Biomass name="Dead" />
        <Frgr>1</Frgr>
        <PrimaryBudNo>1</PrimaryBudNo>
        <MaxCover>1.0</MaxCover>
        <GenericFunction name="ExtinctionCoeff">
          <XProperty>Leaf.LAI</XProperty>
          <XYPairs>
            <XY> 0 0.8</XY>
            <XY> 0.5 0.5</XY>
          </XYPairs>
        </GenericFunction>
        <KDead>0.5</KDead>
        <GenericFunction name="Height">
          <XProperty>Stem.Live.Wt</XProperty>
          <XYPairs>
            <XY> 0 100.0</XY>
            <XY>300 750.0</XY>
          </XYPairs>
        </GenericFunction>
        <FrostFraction>
          <XY> 0 0.0</XY>
          <XY> 2 0.0</XY>
        </FrostFraction>
        <RUEModel name="Photosynthesis">
         <FT>
            <XY>5 0.0</XY>
            <XY>15 1.0</XY>
            <XY>28 1.0</XY>
            <XY>35 1.0</XY>
            <XY>40 0.0</XY>
         </FT>
         <FVPD>
            <XY> 0 1.0</XY>
            <XY>10 1.0</XY>
            <XY>50 1.0</XY>
         </FVPD>
         <RUE>1.2</RUE>
        </RUEModel>
        <PhaseLookup name="PartitionFraction">
          <PhaseLookupValue name="Early">
            <Start>Emergence</Start>
            <End>Flowering</End>
            <Constant name="Function">
              <Value>0.7</Value>
            </Constant>
          </PhaseLookupValue>
          <PhaseLookupValue name="Late">
            <Start>Flowering</Start>
            <End>Maturity</End>
            <Constant name="Function">
              <Value>0.2</Value>
            </Constant>
          </PhaseLookupValue>
        </PhaseLookup>
        <TemperatureFunction name="ThermalTime">
          <XYPairs>
            <XY> 5 9.0</XY>
            <XY>25 16.0</XY>
            <XY>37 0.0</XY>
          </XYPairs>
        </TemperatureFunction>
        <PhaseLookupValue name="NodeInitiationRate">
          <Start>Emergence</Start>
          <End>Flowering</End>
          <Constant name="Function">
            <Value>30.0</Value>
          </Constant>
        </PhaseLookupValue>
        <MaxNodeNo>40</MaxNodeNo>
        <PhaseLookupValue name="NodeAppearanceRate">
          <Start>Emergence</Start>
          <End>Maturity</End>
          <GenericFunction name="Function">
            <XProperty>Leaf.NodeNo</XProperty>
            <XYPairs>
              <XY> 1 60</XY>
              <XY> 3 60</XY>
              <XY> 6 60</XY>
              <XY>30 60</XY>
            </XYPairs>
          </GenericFunction>
        </PhaseLookupValue>
        <InitialAreas>1000 </InitialAreas>
        <InitialAges>150  </InitialAges>
        <InitialiseStage>Emergence</InitialiseStage>
        <InitialLeafPrimordia>6</InitialLeafPrimordia>
        <FinalNodeNoEstimate type="text" description="FinalNodeNoEstimate">20</FinalNodeNoEstimate>
        <GenericFunction name="ExpansionStress">
          <XProperty>WaterSupplyDemandRatio</XProperty>
          <XYPairs>
            <XY> 0.0 0</XY>
            <XY> 1.0 1</XY>
          </XYPairs>
        </GenericFunction>
        <GenericFunction name="MaxArea">
          <XProperty>Leaf.NodeNo</XProperty>
          <XYPairs>
            <XY> 2  5000.0</XY>
            <XY> 6 30000.0</XY>
            <XY>30 30000.0</XY>
          </XYPairs>
        </GenericFunction>
        <GenericFunction name="SpecificLeafArea">
          <XProperty>Leaf.NodeNo</XProperty>
          <XYPairs>
            <XY> 1 18000.0</XY>
            <XY> 6 30000.0</XY>
          </XYPairs>
        </GenericFunction>
        <GenericFunction name="GrowthDuration">
          <XProperty>Leaf.NodeNo</XProperty>
          <XYPairs>
            <XY> 4 180.0</XY>
            <XY> 5 180.0</XY>
          </XYPairs>
        </GenericFunction>
        <GenericFunction name="LagDuration">
          <XProperty>Leaf.NodeNo</XProperty>
          <XYPairs>
            <XY> 1 150.0</XY>
            <XY> 6 300.0</XY>
          </XYPairs>
        </GenericFunction>
        <GenericFunction name="SenescenceDuration">
          <XProperty>Leaf.NodeNo</XProperty>
          <XYPairs>
            <XY> 5 150.0</XY>
            <XY>30 150.0</XY>
          </XYPairs>
        </GenericFunction>
        <GenericFunction name="BranchingRate">
          <XProperty>Leaf.NodeNo</XProperty>
          <XYPairs>
            <XY> 4 0.0</XY>
            <XY> 5 0.7</XY>
            <XY> 8 0.0</XY>
          </XYPairs>
        </GenericFunction>
        <PhaseLookup name="PartitionFractionMax">
          <PhaseLookupValue name="Early">
            <Start>Emergence</Start>
            <End>Flowering</End>
            <Constant name="Function">
              <Value>0.70</Value>
            </Constant>
          </PhaseLookupValue>
          <PhaseLookupValue name="Late">
            <Start>Flowering</Start>
            <End>Maturity</End>
            <Constant name="Function">
              <Value>0.5</Value>
            </Constant>
          </PhaseLookupValue>
        </PhaseLookup>
        <Constant name="CriticalNConc">
          <Value>0.01</Value>
        </Constant>
        <Constant name="MinimumNConc">
          <Value>0.0</Value>
        </Constant>

      </Leaf>
      <GenericOrgan name="Stem">
        <Biomass name="Live" />
        <Biomass name="Dead" />
        <PhaseLookup name="PartitionFraction">
          <PhaseLookupValue name="Early">
            <Start>Emergence</Start>
            <End>StartPodGrowth</End>
            <Constant name="Function">
              <Value>0.3</Value>
            </Constant>
          </PhaseLookupValue>
          <PhaseLookupValue name="Late">
            <Start>StartPodGrowth</Start>
            <End>Maturity</End>
            <Constant name="Function">
              <Value>0.5</Value>
            </Constant>
          </PhaseLookupValue>
        </PhaseLookup>
        <Constant name="StructuralFraction">
          <Value>1.0</Value>
        </Constant>
        <Constant name="CriticalNConc">
          <Value>0.01</Value>
        </Constant>
        <Constant name="NStructuralFraction">
          <Value>1.0</Value>
        </Constant>

      </GenericOrgan>
      <Root>
        <ll>   0.29  0.29  0.29  0.29  0.3   0.31  0.32  0.33  0.34  0.35  0.36 </ll>
