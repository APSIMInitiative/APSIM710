            </Constant>
          </PhaseLookupValue>
          <PhaseLookupValue name="PostEmergence">
            <Start>Emergence</Start>
            <End>StartPodGrowth</End>
            <Constant name="Function">
              <Value>30</Value>
            </Constant>
          </PhaseLookupValue>
          <PhaseLookupValue name="PodFilling">
            <Start>StartPodGrowth</Start>
            <End>Maturity</End>
            <Constant name="Function">
              <Value>0</Value>
            </Constant>
          </PhaseLookupValue>
        </PhaseLookup>

        <Constant name="PartitionFraction">
          <Value>0.2</Value>
        </Constant>
        <TemperatureFunction name="TemperatureEffect">
          <XYPairs>
            <XY> 0 1.0</XY>
            <XY>26 1.0</XY>
            <XY>35 1.0</XY>
          </XYPairs>
        </TemperatureFunction>
        <Constant name="CriticalNConc">
          <Value>0.01</Value>
        </Constant>
            <KNO3>0.02</KNO3>
            <KNH4>0.0</KNH4>
        <Constant name="MaxDailyNUptake">
          <Value>6.0</Value>
        </Constant>
      </Root>
      <ReproductiveOrgan name="Pod">
        <Biomass name="Live" />
        <Biomass name="Dead" />
        <MaximumSize>0.68</MaximumSize>
        <RipeStage>Ripe</RipeStage>
        <GenericFunction name="NumberFunction">
          <XProperty>Stem.Live.Wt</XProperty>
          <XYPairs>
            <XY>0 0</XY>
            <XY>200 1100</XY>
          </XYPairs>
        </GenericFunction>
        <PhaseLookup name="FillingRate">
          <PhaseLookupValue name="EarlyPodGrowth">
            <Start>Flowering</Start>
            <End>StartPodGrowth</End>
            <TemperatureFunction name="Function">
              <XYPairs>
                <XY>5 0.0</XY>
                <XY>25 0.001</XY>
                <XY>30 0.001</XY>
                <XY>35 0.0</XY>
              </XYPairs>
            </TemperatureFunction>
          </PhaseLookupValue>
          <PhaseLookupValue name="LatePodGrowth">
            <Start>StartPodGrowth</Start>
            <End>Maturity</End>
            <TemperatureFunction name="Function">
              <XYPairs>
                <XY>5 0.0</XY>
                <XY>25 0.04</XY>
                <XY>30 0.04</XY>
                <XY>35 0.0</XY>
              </XYPairs>
            </TemperatureFunction>
          </PhaseLookupValue>
        </PhaseLookup>
        <Constant name="NFillingRate">
          <Value>0.00012</Value>
        </Constant>
        <Constant name="MaxNConcDailyGrowth">
          <Value>0.03</Value>
        </Constant>
        <StageBasedInterpolation name="WaterContent">
          <Stages>Germination Emergence Flowering StartPodGrowth Maturity </Stages>
          <Codes>  0.93         0.93      0.93    0.93            0.90</Codes>
        </StageBasedInterpolation>
      </ReproductiveOrgan>
      <StageBasedInterpolation name="StageCode">
        <Stages>Germination Emergence Flowering StartPodGrowth Maturity</Stages>
        <Codes>   1               2          3            4           5      </Codes>
      </StageBasedInterpolation>
      <Summary name="Summary" />
    </Plant>
