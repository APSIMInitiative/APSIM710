<Type name="FarmSimGraze">
  <MetaData>
    <UItype>VBUserInterface.GenericUI</UItype>
    <IsCrop>No</IsCrop>
    <ShowInMainTree>Yes</ShowInMainTree>
    <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
    <Documentation>FarmSimGraze</Documentation>
    <LargeIcon>%apsim%\UserInterface\Images\cubes32.png</LargeIcon>
    <SmallIcon>%apsim%\UserInterface\Images\cubes16.png</SmallIcon>
    <dll>%apsim%\Model\FarmSimGraze.dll</dll>
	  <drops>
      <drop>toolbox</drop>
      <drop>area</drop>
      <drop>simulation</drop>
      <drop>folder</drop>
      <drop>factor</drop>
    </drops>
    <ApsimToSim>
      <component name="[FarmSimGraze.name]" executable="[dll]">
        <componentinterface>%apsim%\Model\dotnetcomponentinterface.dll</componentinterface>
        <executable>[dll]</executable>
        <initdata>
          [Model]
          [FarmSimGraze.innerxml]
        </initdata>
      </component>
    </ApsimToSim>
  </MetaData>
  <Model>
    <FarmSimGraze>
    </FarmSimGraze>
  </Model>
  
</Type>