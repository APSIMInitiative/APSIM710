<Soil>

  <Properties>
    <Country/>
    <Site/>
    <Region/>
    <LocalName/>
    <SoilType/>
    <NearestTown/>
    <NaturalVegetation/>
    <State/>
    <ApsoilNumber/>
    <Latitude/>
    <Longitude/>
    <LocationAccuracy/>
    <DataSource/>
    <Comments/>
    <MunsellColour/>
    <MunsellColourCode/>
  </Properties>

  <SoilWat>
    <Properties>
      <SummerCona/>
      <SummerU/>
      <SummerDate/>
      <WinterCona/>
      <WinterU/>
      <WinterDate/>
      <DiffusConst/>
      <DiffusSlope/>
      <Salb/>
      <Cn2Bare/>
      <CnRed/>
      <CnCov/>
      <CnCanopyFact/>
      <Slope/>
      <DischargeWidth/>
      <CatchmentArea/>
      <MaxPond/>
    </Properties>
    <Variables>
      <SWCON >
        <MapMethod>Spatial</MapMethod>
        <DefaultValue>0.0</DefaultValue>
        <Units>0-1</Units>
        <Code name="E">Estimated</Code>
        <Code name="M">Measured</Code>
      </SWCON>
      <MWCON>
        <MapMethod>Spatial</MapMethod>
        <DefaultValue>0.0</DefaultValue>
        <Units>0-1</Units>
        <Code name="E">Estimated</Code>
        <Code name="M">Measured</Code>
      </MWCON>
      <KLAT>
        <MapMethod>Spatial</MapMethod>
        <DefaultValue>0.0</DefaultValue>
        <Units>mm/d</Units>
        <Code name="E">Estimated</Code>
        <Code name="M">Measured</Code>
      </KLAT>
    </Variables>
  </SoilWat>

  <Swim>
    <Properties>
      <Salb/>
      <Cn2Bare/>
      <CnRed/>
      <CnCov/>
      <Kd/>
      <VC/>
      <dtmin/>
      <dtmax/>
      <MaxWaterIncrement/>
      <SpaceWeightingFactor/>
      <SoluteSpaceWeightingFactor/>
      <BottomBoundaryCondition/>
      <BottomBoundaryValue/>
      <WaterTableConductance/>
    </Properties>
    <Variables/>
  </Swim>
  
  <Water>
    <Variables>
      <Thickness>
        <Units>mm</Units>
        <Units>cm</Units>
      </Thickness>
      <KS>
        <MapMethod>Spatial</MapMethod>
        <Units>mm/day</Units>
      </KS>
      <BD>
        <MapMethod>Spatial</MapMethod>
        <Units>g/cc</Units>
        <Code description="Field measured and checked for sensibility">FM</Code>
        <Code description="Laboratory measured">LM</Code>
        <Code description="Calculated from gravimetric moisture when profile wet but drained (Vertosols only)">C_Grav</Code>
        <Code description="Estimated – best estimate based on local knowledge, soil texture etc">E</Code>
      </BD>
      <AirDry>
        <MapMethod>Spatial</MapMethod>
        <Units>mm/mm</Units>
        <Code description="Field measured and checked for sensibility">FM</Code>
        <Code description="Laboratory measured">LM</Code>
        <Code description="Estimated – best estimate based on local knowledge, soil texture etc">E</Code>
      </AirDry>
      <LL15>
        <MapMethod>Spatial</MapMethod>
        <Units>mm/mm</Units>
        <Code description="Field measured and checked for sensibility">FM</Code>
        <Code description="Laboratory measured">LM</Code>
        <Code description="Estimated – best estimate based on local knowledge, soil texture etc">E</Code>
      </LL15>
      <DUL>
        <MapMethod>Spatial</MapMethod>
        <Units>mm/mm</Units>
        <Code description="Field measured and checked for sensibility">FM</Code>
        <Code description="Laboratory measured">LM</Code>
        <Code description="Estimated – best estimate based on local knowledge, soil texture etc">E</Code>
      </DUL>
      <SAT>
        <MapMethod>Spatial</MapMethod>
        <Units>mm/mm</Units>
        <Code description="Field measured and checked for sensibility">FM</Code>
        <Code description="Laboratory measured">LM</Code>
        <Code description="Calculated from gravimetric moisture">C_Bd</Code>
        <Code description="Estimated – best estimate based on local knowledge, soil texture etc">E</Code>
      </SAT>
    </Variables>
  </Water>
  
  <SoilOrganicMatter>
    <Properties>
      <RootCn/>
      <RootWt/>
      <SoilCn/>
      <EnrACoeff/>
      <EnrBCoeff/>
    </Properties>
    <Variables>
      <OC>
        <MapMethod>Mass</MapMethod>
        <DefaultValue>0.02</DefaultValue>
        <Units>Total %</Units>
        <Units>Walkley Black %</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </OC>
      <FBiom>
        <MapMethod>Spatial</MapMethod>
        <Units>0-1</Units>
      </FBiom>
      <FInert>
        <MapMethod>Spatial</MapMethod>
        <Units>0-1</Units>
      </FInert>
    </Variables>
  </SoilOrganicMatter>
  
  <Lab>
    <Variables>
      <Rocks>
        <MapMethod>Spatial</MapMethod>
        <Units>%</Units>
        <Code description="Volumetric measured">Vol</Code>
        <Code description="Mass measured">Mass</Code>
      </Rocks>
      <Texture>
        <Code description="Calculated from PSA">Calc</Code>
        <Code description="Field Texture">FT</Code>
      </Texture>
      <EC>
        <MapMethod>Spatial</MapMethod>
        <Units>1:5 dS/m</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </EC>
      <PH>
        <MapMethod>Spatial</MapMethod>
        <Units>1:5 water</Units>
        <Units>CaCl2</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </PH>
      <CL>
        <MapMethod>Spatial</MapMethod>
        <Units>mg/kg</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </CL>
      <Boron>
        <MapMethod>Spatial</MapMethod>
        <Units>Hot mg/kg</Units>
        <Units>Hot CaCl2</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </Boron>
      <CEC>
        <MapMethod>Spatial</MapMethod>
        <Units>cmol+/kg</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </CEC>
      <Ca>
        <MapMethod>Spatial</MapMethod>
        <Units>cmol+/kg</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </Ca>
      <Mg>
        <MapMethod>Spatial</MapMethod>
        <Units>cmol+/kg</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </Mg>
      <Na>
        <MapMethod>Spatial</MapMethod>
        <Units>cmol+/kg</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </Na>
      <K>
        <MapMethod>Spatial</MapMethod>
        <Units>cmol+/kg</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </K>
      <ESP>
        <MapMethod>Spatial</MapMethod>
        <Units>%</Units>
        <Code description="Calculated (Na/sum of cations)*100">M</Code>
        <Code description="Estimated (CEC (Exch Na/CEC*100)">E</Code>
      </ESP>
      <Mn>
        <MapMethod>Spatial</MapMethod>
        <Units>mg/kg</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </Mn>
      <Al>
        <MapMethod>Spatial</MapMethod>
        <Units>meq/100g</Units>
        <Code description="Measured as Exch Al in cmol(+)/kg">M</Code>
        <Code description="Estimated">E</Code>
      </Al>
      <ParticleSizeSand>
        <MapMethod>Spatial</MapMethod>
        <Units>%</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </ParticleSizeSand>
      <ParticleSizeSilt>
        <MapMethod>Spatial</MapMethod>
        <Units>%</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </ParticleSizeSilt>
      <ParticleSizeClay>
        <MapMethod>Spatial</MapMethod>
        <Units>%</Units>
        <Code description="Measured">M</Code>
        <Code description="Estimated">E</Code>
      </ParticleSizeClay>
    </Variables>    
  </Lab>

  <Sample>
    <Variables>
      <NO3>
        <MapMethod>Mass</MapMethod>
        <DefaultValue>1.0</DefaultValue>
        <Units>ppm</Units>
        <Units>kg/ha</Units>
      </NO3>
      <NH4>
        <MapMethod>Mass</MapMethod>
        <DefaultValue>0.2</DefaultValue>
        <Units>ppm</Units>
        <Units>kg/ha</Units>
      </NH4>
      <SW>
        <MapMethod>Spatial</MapMethod>
        <DefaultValue>LL15</DefaultValue>
        <Units>mm/mm</Units>
        <Units>grav. mm/mm</Units>
      </SW>
       <OC>
          <MapMethod>Mass</MapMethod>
          <DefaultValue>0.02</DefaultValue>
          <Units>Total %</Units>
          <Units>Walkley Black %</Units>
          <Code description="Measured">M</Code>
          <Code description="Estimated">E</Code>
       </OC>
       <EC>
          <MapMethod>Spatial</MapMethod>
          <Units>1:5 dS/m</Units>
          <Code description="Measured">M</Code>
          <Code description="Estimated">E</Code>
       </EC>
       <PH>
          <MapMethod>Spatial</MapMethod>
          <Units>1:5 water</Units>
          <Units>CaCl2</Units>
          <Code description="Measured">M</Code>
          <Code description="Estimated">E</Code>
       </PH>
       <CL>
          <MapMethod>Spatial</MapMethod>
          <Units>mg/kg</Units>
          <Code description="Measured">M</Code>
          <Code description="Estimated">E</Code>
       </CL>
       <ESP>
          <MapMethod>Spatial</MapMethod>
          <Units>%</Units>
          <Code description="Calculated (Na/sum of cations)*100">M</Code>
          <Code description="Estimated (CEC (Exch Na/CEC*100)">E</Code>
       </ESP>

    </Variables>
  </Sample>

  <SoilCrop>
    <Variables>
      <LL>
        <MapMethod>Spatial</MapMethod>
        <Units>mm/mm</Units>
        <Code description="Field measured">FM</Code>
        <Code description="Laboratory measured">LM</Code>
        <Code description="Estimated">E</Code>
      </LL>
      <KL>
        <MapMethod>Spatial</MapMethod>
        <Units>/day</Units>
      </KL>
      <XF>
        <MapMethod>Spatial</MapMethod>
        <Units>0-1</Units>
      </XF>
    </Variables>
  </SoilCrop>
  
  <Phosphorus>
    <Properties>
      <RootCP/>
      <RateDissolRock/>
      <RateLossAvail/>
    </Properties>
    <Variables>
      <LabileP>
        <MapMethod>Spatial</MapMethod>
        <Units>mg/kg</Units>
      </LabileP>
      <BandedP>
        <Location>Phosphorus</Location>
        <MapMethod>Spatial</MapMethod>
        <Units>kg/ha</Units>
      </BandedP>
      <RockP>
        <Location>Phosphorus</Location>
        <MapMethod>Spatial</MapMethod>
        <Units>kg/ha</Units>
      </RockP>
      <Sorption>
        <Location>Phosphorus</Location>
        <MapMethod>Spatial</MapMethod>
        <Units>-</Units>
      </Sorption>    
    </Variables>
  </Phosphorus>


</Soil>
