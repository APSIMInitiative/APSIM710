<Type name="SlurryTank">
  <MetaData>
      <UItype>CSUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\erosion.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\shovel32.bmp</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\shovel16.bmp</SmallIcon>
	  <DLL>%apsim%\Model\SlurryTank.dll</DLL>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
      </drops>
    <ApsimToSim>
      <component name="[SlurryTank.name]" executable="[dll]"> class="SlurryTank">
        <executable name="[dll]" version="1.0" />
        <initdata>
        	[Model]
			[SlurryTank.innerxml]
        </initdata>
      </component>
    </ApsimToSim>
  </MetaData>
  <Model>
     
  <SlurryTank>
        <ThetaA type="text" description="ThetaA:Temperature constant in decay function">-3.432</ThetaA>
        <ThetaB type="text" description="ThetaB:Temperature constant in decay function">0.181</ThetaB>
        <ThetaC type="text" description="ThetaC:Temperature constant in decay function">36.9</ThetaC>
        <ThetaAM type="text" description="ThetaAM:Temperature constant in decay function">-3.432</ThetaAM>
        <ThetaBM type="text" description="ThetaBM:Temperature constant in decay function">0.186</ThetaBM>
        <ThetaCM type="text" description="ThetaCM:Temperature constant in decay function">36.9</ThetaCM>
        <GInert type="text" description="Proportion of C partitioned to inert pool during decomposition of Fast pool">0.05</GInert>
        <b type="text" description="b a constant (unitless)">200</b>
        <CCH4 type="text" description="Total mass of C partitioned to CH4 (kr pr day)">1</CCH4>

    <dayOfEmpty type="text" description="the day where the tank should be empty(day)">400</dayOfEmpty>
<temperatureInKelvin type="text" description="temperature In Kelvin(degree)">1</temperatureInKelvin>

<k1 type="text" description="Optimum decomposition rate of the Slow pool(pr day)">0.01</k1>
<k2 type="text" description="Optimum decomposition rate of the Fast pool (pr day)">0.1</k2>
<k3 type="text" description="Optimum decomposition rate of the VFA pool in the absence of sulphate (pr day)">0.1</k3>
<k4 type="text" description="Optimum decomposition rate of the VFA pool in the presence of sulphate (pr day)">0.1</k4>
<pHmin type="text" description="pH below which microbial activity ceases">4</pHmin>
<pHopt_lo type="text" description="optimum pH(unitless)">6</pHopt_lo>
<pHmax type="text" description="pH above which microbial activity ceases">9</pHmax>
<pHopt_hi type="text" description="Optimum pH for microbial activity">8</pHopt_hi>
<surfaceArea type="text" description="surfaceArea  (unitless)">300</surfaceArea>
<lag_alpha type="text" description="lag_alpha (pr day)">0.06</lag_alpha>
<lag_beta type="text" description="lag_beta(unitless)">5</lag_beta>
      </SlurryTank>	
  </Model>
</Type>


