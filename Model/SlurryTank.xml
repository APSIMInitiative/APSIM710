<?xml version="1.0" encoding="utf-8"?>
<Type name="SlurryTank">
  <MetaData>
    <UItype>CSUserInterface.GenericUI</UItype>
    <ShowInMainTree>Yes</ShowInMainTree>
    <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
    <Documentation>%apsim%\Documentation\4 Module Reference\erosion.htm</Documentation>
    <LargeIcon>%apsim%\UserInterface\Images\shovel32.bmp</LargeIcon>
    <SmallIcon>%apsim%\UserInterface\Images\shovel16.bmp</SmallIcon>
    <DLL>%apsim%\Model\SlurryTank.dll</DLL>
    <drops>
      <drop>toolbox</drop>
      <drop>area</drop>
      <drop>simulation</drop>
      <drop>folder</drop>
    </drops>
    <ApsimToSim>
      <component name="[SlurryTank.name]" executable="[dll]"> class="SlurryTank"&gt;
        <executable name="[dll]" version="1.0" /><initdata>
        	[Model]
			[SlurryTank.innerxml]
        </initdata></component>
    </ApsimToSim>
  </MetaData>
  <Model>
    <SlurryTank>
      <HeatTransferCoefficient type="text" description="HeatTransferCoefficient a Coefficient">0</HeatTransferCoefficient>
      <ThetaA type="text" description="ThetaA a constant (unitless)">-3.432</ThetaA>
      <ThetaB type="text" description="ThetaB a constant (unitless)">6</ThetaB>
      <ThetaC type="text" description="ThetaC a constant (unitless)">36.9</ThetaC>
      <ThetaAM type="text" description="ThetaAM a constant (unitless)">-3.432</ThetaAM>
      <ThetaBM type="text" description="ThetaBM a constant (unitless)">0.186</ThetaBM>
      <ThetaCM type="text" description="ThetaCM constant (unitless)">36.9</ThetaCM>
      <GInert type="text" description="GInert a constant (unitless)">0.05</GInert>
      <b type="text" description="b a constant (unitless)">200</b>
      <CCH4 type="text" description="CCH4 a constant (unitless)">1</CCH4>
      <dayOfEmpty type="text" description="the day where the tank should be empty(day)">-1</dayOfEmpty>
      <temperatureInKelvin type="text" description="temperature In Kelvin (unitless)">1</temperatureInKelvin>
      <k1 type="text" description="k1 (unitless)">0.01</k1>
      <k2 type="text" description="k2 (unitless)">0.1</k2>
      <k3 type="text" description="k3 (unitless)">0.1</k3>
      <k4 type="text" description="k4 (unitless)">0.1</k4>
      <pHmin type="text" description="pHmin(unitless)">0</pHmin>
      <pHmin_m type="text" description="pHmin_m(unitless) methanogenesis">0</pHmin_m>
      <pHopt_lo type="text" description="pHopt_lo(unitless)">0</pHopt_lo>
      <pHopt_m_lo type="text" description="pHopt_m_lo(unitless) methanogenesis">0</pHopt_m_lo>
      <pHmax type="text" description="pHmax  (unitless)">0</pHmax>
      <pHmax_m type="text" description="pHmax_m  (unitless) methanogenesis">0</pHmax_m>
      <pHopt_hi type="text" description="pHopt_hi(unitless)">0</pHopt_hi>
      <pHopt_m_hi type="text" description="pHopt_m_hi(unitless) methanogenesis">0</pHopt_m_hi>
      <surfaceArea type="text" description="surfaceArea  (unitless)">300</surfaceArea>
      <lag_alpha type="text" description="lag_alpha (pr day)">0.06</lag_alpha>
      <lag_beta type="text" description="lag_beta(unitless)">5</lag_beta>
      <treatment_no type="text" description="treatment no">0</treatment_no>
      <RA type="text" description="aerodynamic resistance (s/m)">0.2</RA>
      <measurement_day type="text" description="measurement day">0.2</measurement_day>
      <measured_pH type="text" description="measured pH">0.2</measured_pH>
<Diameter type="text" description="Diameter">0.2</Diameter>
<HeightSoil type="text" description="HeightSoil ">0.2</HeightSoil>
<tempCommon type="text" description="tempCommon">0.2</tempCommon>
<tempAir type="text" description="tempAir">0.2</tempAir>
<tempSoil type="text" description="tempSoil">0.2</tempSoil>
    </SlurryTank>
  </Model>
</Type>