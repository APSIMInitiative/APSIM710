   <!--
   ORGANIC MATTER Types
   Event: IncorpFOM (type: FOMLayer) is sent by the crops to incorporate
          fresh organica matter (usually roots) into the soil. SoilN and SoilP subscribe
          to this event. NB: If a CNR is specified (non zero) then it will be used to
          calculate an N content in the FOM. LabileP is also optional and can be zero.
          The C field in the embedded FOM type is not used.
   Event: IncorpFOMPool (type: FOMPool) sent by SurfaceOM
          to SoilN and SoilP to add fresh organic matter into soil layers. This
          event contains multiple pools of matter by layer. NB: The Amount field of
          FOM isn't used by SoilN and SoilP.
   Event: SurfaceOrganicMatterState (type: SurfaceOrganicMatter) is sent by SurfaceOM.
   Event: Remove_SurfaceOM (type: SurfaceOrganicMatter) is sent by SurfaceOM.
     Get: Surface_organic_matter variable (type: SurfaceOrganicMatter) is sent by SurfaceOM
          when another component (e.g. StockScienceConverter - ResidueHerbage) does a get for
          it.
   Event: PotentialResidueDecompositionCalculated (type: SurfaceOrganicMatterDecomp) sent by
          SurfaceOM to SoilN.
   Event: ActualResidueDecompositionCalculated (type: SurfaceOrganicMatterDecomp) sent by
          SoilN and received by SurfaceOM and SoilP.
   -->

   <type name="FOM">
      <field name="amount"  kind="single" unit="kg/ha"/>
      <field name="C"       kind="single" unit="kg/ha"/>
      <field name="N"       kind="single" unit="kg/ha"/>
      <field name="P"       kind="single" unit="kg/ha"/>
      <field name="AshAlk"  kind="single" unit="kg/ha"/>
   </type>

   <type name="FOMLayer">
      <field name="Type" kind="string"/>
      <field name="Layer" array="T">
         <field name="FOM" type="FOM" />
         <field name="CNR" kind="single" />
         <field name="LabileP" kind="single" />
      </field>
   </type>

   <type name="FOMPool">
      <field name="Layer" array="T">
         <field name="thickness" kind="single" units = "mm"/>
         <field name="no3" kind="single" unit="kg/ha"/>
         <field name="nh4" kind="single" unit="kg/ha"/>
         <field name="po4" kind="single" unit="kg/ha"/>
         <field name="Pool" array="T" type="FOM" />
      </field>
   </type>

   <type name="SurfaceOrganicMatter">
      <field name="Pool" array="T">
         <field name="Name" kind="string"/>
         <field name="OrganicMatterType" kind="string"/>
         <field name="PotDecompRate" kind="single" unit="day^-1"/>
         <field name="no3" kind="single" unit="kg/ha"/>
         <field name="nh4" kind="single" unit="kg/ha"/>
         <field name="po4" kind="single" unit="kg/ha"/>
         <field name="StandingFraction" array="T" type="FOM" />
         <field name="LyingFraction" array="T" type="FOM" />
      </field>
   </type>

   <type name="SurfaceOrganicMatterDecomp">
      <field name="Pool" array="T">
         <field name="Name" kind="string"/>
         <field name="OrganicMatterType" kind="string"/>
         <field name="FOM" type="FOM" />
      </field>
   </type>


   <type name="SoilOrganicMatter">
      <field name="OrganicMatterType" kind="string"/>
      <field name="layer" array = "T">
          <field name="OrganicMatterFraction" array="T" type="FOM" />
      </field>
   </type>


