 <Type name="pond">
   <MetaData>
      <UItype>VBUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\images\pond.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\WaterSupply.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\drink_blue32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\drink_blue16.png</SmallIcon>
      <dll>%apsim%/Model/Pond.%dllext%</dll>
      <drops>
         <drop>simulation</drop>
         <drop>area</drop>
         <drop>folder</drop>
         <drop>toolbox</drop>
     </drops>
      <ApsimToSim>
         <component name="[pond.name]" executable="[dll]">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [Model]
               <algae_present>[pond.algae_present]</algae_present>
               <CEC>[pond.CEC]</CEC>
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
   <Model>
      <constants>

      </constants>
   </Model>
   <Info>
     <variables>
         <variable name="pond_active" description ="Status of the pond - present or not"/> 
         <variable name="pond_no3" description ="Pond nitrate"/>
         <variable name="pond_nh4" description ="Pond ammonium"/>
         <variable name="pond_nh3" description ="Pond urea"/>
         <variable name="pond_urea" description ="Pond urea"/>
         <variable name="pond_no3_conc" description ="Pond nitrate"/>
         <variable name="pond_nh4_conc" description ="Pond ammonium"/>
         <variable name="pond_nh3_conc" description ="Pond urea"/>
         <variable name="ftmax" description ="Maximum floodwater temperature today"/>
         <variable name="ftmin" description ="Minimum floodwater temperature today"/>
         <variable name="ftmean" description ="Mean floodwater temperature today"/>
         <variable name="ftmax_yest" description ="Maximum daily floodwater temperature yesterday"/>
         <variable name="ftmin_yest" description ="Minimum daily floodwater temperature yesterday"/>
         <variable name="ftmean_yest" description ="Mean daily floodwater temperature yesterday"/>
         <variable name="pab_mass" description ="Algal biomass"/>
         <variable name="algal_turnover" description ="algal production in excess of maximum production"/>
         <variable name="pond_hum_c" description ="pond_hum_c"/>
         <variable name="pond_biom_c" description ="pond_biom_c"/>
         <variable name="pond_depth" description ="pond_depth"/>
         <variable name="pond_evap" description ="pond_evap"/>
         <variable name="rlai_from_pondmodule" description ="rlai"/>
         <variable name="rep" description ="rice evapotranspiration"/>
         <variable name="oc1" description ="organic carbon layer 1"/>
         <variable name="dlayer1" description ="dlayer1"/>
         <variable name="bd1" description ="bd1"/>
         <variable name="algact" description ="algact"/>
         <variable name="fni" description ="nitrogen factor affecting algal growth"/>
         <variable name="fpi" description ="phosphorus factor affecting algal growth"/>
         <variable name="fti" description ="temperature factor affecting algal growth"/>
         <variable name="fph" description ="floodwater pH"/>
         <variable name="ali" description ="light factor affecting algal growth"/>
         <variable name="yalga" description ="yalga"/>
         <variable name="algfix" description ="algfix"/>
         <variable name="tmpfon" description ="tmpfon"/>
         <variable name="totuh" description ="total urea hydrolysed"/>
         <variable name="pot_hydrolysis" description ="pot_hydrolysis"/>
         <variable name="totaml" description ="total ammonia loss"/>
         <variable name="amloss" description ="daily ammonia loss"/>
         <variable name="dlt_res_nh4_min" description ="dlt_res_nh4_min"/>
         <variable name="dlt_res_no3_min" description ="dlt_res_no3_min"/>
         <variable name="kill" description ="algal kill"/>
         <variable name="dlt_res_C_decomp" description ="dlt_res_C_decomp"/>
         <variable name="dlt_res_N_decomp" description ="dlt_res_N_decomp"/>
         <variable name="dlt_res_C_atm" description ="dlt_res_C_atm"/>
         <variable name="dlt_res_C_biom" description ="dlt_res_C_biom"/>
         <variable name="dlt_res_C_hum" description ="dlt_res_C_hum"/>
         <variable name="infiltration_pond_calc" description ="infiltration"/>
         <variable name="no3_infiltrated" description ="no3_infiltrated"/>
         <variable name="nh4_infiltrated" description ="nh4_infiltrated"/>
         <variable name="urea_infiltrated" description ="urea_infiltrated"/>
     </variables>
   </Info>

</Type>
