 <Type name="pond">
   <MetaData>
      <UItype>CSUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\images\pond.jpg</Image>
      <Documentation>%apsim%\Documentation\4 Module Reference\WaterSupply.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\drink_blue32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\drink_blue16.png</SmallIcon>
      <dll>%apsim%/Model/Pond.%dllext%</dll>
      <drops>
         <drop>simulation</drop>
         <drop>area</drop>
         <drop>folder</drop>
         <drop>toolbox</drop>
     </drops>
      <ApsimToSim>
         <component name="[pond.name]" executable="[dll]" class="Pond">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [Model]
               <algae_present>[pond.algae_present]</algae_present>
               <CEC>[pond.CEC]</CEC>
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
   <Model>
      <constants>

      </constants>
   </Model>
   

</Type>
