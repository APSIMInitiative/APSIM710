<Type name="SiloInput">
   <MetaData>
      <UItype>VBUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Documentation>%apsim%\Documentation\4 Module Reference\SiloInput.htm</Documentation>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <LargeIcon>%apsim%\UserInterface\Images\thermometer32.png</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\thermometer16.png</SmallIcon>
      <dll>%apsim%/Model/SiloInput.%dllext%</dll>
      <drops>
         <drop>simulation</drop>
         <drop>folder</drop>
		 <drop>factor</drop>		 
      </drops>
      <ApsimToSim>
         <component name="[SiloInput.name]" executable="[dll]" class="SiloInput">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [SiloInput.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
</Type>