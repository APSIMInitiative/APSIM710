      </Job>
      <Job name="Compile JobScheduler Tools">
         <DependsOn>Move RunTime files into model directory</DependsOn>
         <WorkingDirectory>%APSIM%/Model/JobScheduler</WorkingDirectory>
         <CommandLine>make --file=MakefileTools</CommandLine>
      </Job>
   </Target>
