<Type name="TreeMicroClimate">
  <MetaData>
    <UItype>CSUserInterface.GenericUI</UItype>
    <ShowInMainTree>Yes</ShowInMainTree>
    <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
    <Documentation>TreeMicroClimate.htm</Documentation>
    <LargeIcon>%apsim%\UserInterface\Images\fertiliser32.png</LargeIcon>
    <SmallIcon>%apsim%\UserInterface\Images\fertiliser16.png</SmallIcon>
    <DLL>%apsim%/Model/TreeMicroClimate.%dllext%</DLL>
    <drops>
      <drop>toolbox</drop>
      <drop>folder</drop>
	  <drop>simulation</drop>
	  <drop>factor</drop>
    </drops>
    <ApsimToSim>
      <component name="[TreeMicroClimate.name]" executable="[dll]">
        <executable name="[dll]" version="1.0" />
        <initdata>
        [Model]
		[TreeMicroClimate.innerxml]
        </initdata>
      </component>
    </ApsimToSim>
  </MetaData>
  <Model>
     <TreeMicroClimate>
		<inputFilename />
	 </TreeMicroClimate>
  </Model>
</Type>
