<Type name="graz">
   <MetaData>
      <UItype>VBUserInterface.GenericUI</UItype>
      <IsCrop>No</IsCrop>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Description>Rickert &amp; McKeon (1981) grazing module</Description>
      <Documentation></Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\stock32.bmp</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\stock16.bmp</SmallIcon>
      <dll>%apsim%/Model/Graz.%dllext%</dll>
      <drops>
         <drop>toolbox</drop>
         <drop>area</drop>
         <drop>simulation</drop>
         <drop>folder</drop>
         <drop>factor</drop>
      </drops>
      <ApsimToSim>
         <component name="[graz.name]" executable="[dll]">
            <executable name="[dll]" version="1.0"/>
            <initdata>
               [Model]
               [graz.innerxml]
            </initdata>
         </component>
      </ApsimToSim>
   </MetaData>
   
</Type>