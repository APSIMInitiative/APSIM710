             -->
          <fasw_emerg>.0 .7</fasw_emerg>
          <rel_emerg_rate>.3  1</rel_emerg_rate>
          <!--      crop_leaf_appearance
             -->
          <leaf_no_at_emerg description="leaf number at emergence ()">1.0</leaf_no_at_emerg>
