version = 7.3
[APSIM.fababean]
title = Sample Simulation
module = Clock   fababean.par [sample] %apsim%/Model/Clock.xml [standard]
module = Report     fababean.par[orobanche] fababean.par[orobanche0]
module = Input(met)                       %apsim%/Examples/MetFiles/Dalby.met[weather]
module = Manager    fababean.par[fababean]
module = SoilTemp   fababean.par [sample] %apsim%/Model/SoilTemp.xml [standard]
module = SoilWat   %apsim%/Examples/Soils/Soil.par [black_earth] %apsim%/Model/Soil.xml [SoilWat]
module = SurfaceOM   %apsim%/Examples/Soils/Soil.par [legume_residue] %apsim%/Model/SurfaceOM.xml [standard]
module = SoilN   %apsim%/Examples/Soils/Soil.par [black_earth] %apsim%/Model/Soil.xml [SoilN]
module = Plant(fababean)   fababean.par [sample] %apsim%/Model/Fababean.xml [standard]
module = Parasite(orobanche)   %apsim%/Model/Orobanche.xml [standard]


