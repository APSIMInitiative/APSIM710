      <manager2 name="Script(C#)">
        <text>
using System;
using ModelFramework;

public class Script 
{      
   [Link] Paddock MyPaddock;
   [Link] Plant Wheat2; 
   [Input] DateTime Today;   
   [Input] string CurrentPhaseName; 
   
   // The following event handler will be called once at the beginning of the simulation
   [EventHandler] public void OnInitialised()
   {
      MyPaddock.Deactivate("wheat2");
   }
      
   // The following event handler will be called each day at the beginning of the day
   [EventHandler] public void OnPrepare()
   {
      if (Today.Day == 1 &amp;&amp; Today.Month == 6) 
      {
         MyPaddock.Activate("wheat2");
         SowPlant2Type Sow = new SowPlant2Type {Cultivar = "Hartog", Population = 150, Depth = 50, RowSpacing = 500, MaxCover=1, BudNumber=1};
         Wheat2.OnSow(Sow);
      }
      
      if (CurrentPhaseName == "ReadyForHarvesting")
      {
         Wheat2.OnHarvest();
         Wheat2.OnEndCrop();
         MyPaddock.Deactivate("wheat2");
      }
      
   }
   
}
       </text>
