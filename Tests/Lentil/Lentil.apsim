   lentil sow cultivar = '[lentil_cv]', plants = [lentil_density] (/m2), sowing_depth = 40 (mm)
   MaxLAI = 0
   under_crop = 1
endif

! if maturity harvest the crop
if (lentil.StageName = 'harvest_ripe') or (lentil.plant_status = 'dead') then
   lentil harvest
