          <ui>
            <Dummy1 type="category" description="Setup cut rotation for AgPasture" />
            <CutPasture_StartDate type="ddmmmdate" description="Enter date to start rotation (dd/mm/yyyy): ">01/01/1900</CutPasture_StartDate>
