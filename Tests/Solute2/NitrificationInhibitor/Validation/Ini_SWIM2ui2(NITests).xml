         <minimum_conductance>0.02</minimum_conductance>
         <initial_conductance>4.0</initial_conductance>
         <precipitation_constant>25</precipitation_constant>
      </top_boundary>
      <bypass_flow>
         <depth>11</depth>
         <conductance>0.10</conductance>
         <storage>0.10</storage>
      </bypass_flow>
      <crop>
