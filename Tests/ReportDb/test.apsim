          </ui>
          <script>
            <text>
        if (today = date('[date]') then
              [crop] sow population =[density], depth = [depth], cultivar = [cultivar], MaxCover = 1.0, BudNumber = 1, row_spacing = [row_spacing], crop_class = [class]
         endif
               </text>
            <event>start_of_day</event>
