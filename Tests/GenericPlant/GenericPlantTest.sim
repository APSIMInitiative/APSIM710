        </C4grassType>
        <!--  might be best to make the input Ra50 rather than R50
      -->
        <TreeType>
          <albedo>0.15</albedo>
          <gsmax>0.005</gsmax>
        </TreeType>
        <TreeType2>
          <albedo>0.15</albedo>
          <gsmax>0.010</gsmax>
