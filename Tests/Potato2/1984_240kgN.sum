                endif
     END of rules
     

------- Diagnostic variables Initialisation -----------------------------------
     Manager rules:
     
     SECTION:- init
     simday = 1
     accumplantn = 0
     accummineralisation = 0
     accumdenit = 0
     accumfert = 0
     accumleach = 0
     accumdetach = 0
     deltasoilomn = 0
     deltasurfaceomn = 0
     deltasoilmineraln = 0
     accumeo = 0
     accumep = 0
     accumes = 0
     accumdrainage = 0
     accumrunoff = 0
     accumoutflowlat = 0
     accumirrigation = 0
     accumrainfall = 0
     soilwaterdeficit = 0
     grainnpercent = 0
     floretnpercent = 0
     tubernpercent = 0
     stemnpercent = 0
     leafnpercent = 0
     accumttsowing = 0
     
     SECTION:- start_of_day
     if simday = 1 then
     initialsoilomn = biom_n() + fom_n() + hum_n()
     initialsurfaceomn = surfaceom_n
     initialmineraln = no3() + nh4() + urea()
     endif
     accumplantn =  (0 - potato2.leafliven2 - potato2.leafdeadn2 - potato2.stemliven - potato2.stemdeadn - potato2.tuberliven - potato2.tuberdeadn ) * 10
     accummineralisation = accummineralisation + dlt_n_min() + dlt_n_min_res()
     accumdenit = accumdenit - dnit()
     accumfert = accumfert + fertiliser
     accumleach = accumleach - leach_urea - leach_no3 - leach_nh4
     accumdetach = 0
     deltasoilomn = (biom_n() + fom_n() + hum_n()) - initialsoilomn
     deltasurfaceomn = surfaceom_n - initialsurfaceomn
     deltasoilmineraln = (no3() + nh4() + urea()) - initialmineraln
     accumeo = accumeo - eo
     accumep = accumep - potato2.leaftranspiration
     accumes = accumes - es
     accumdrainage = accumdrainage - drain
     accumrunoff = accumrunoff - runoff
     accumoutflowlat = accumoutflowlat - outflow_lat()
     accumirrigation = accumirrigation + irrigation
     accumrainfall = accumrainfall + rain
     soilwaterdeficit =  sw_dep() - dul_dep()
     grainnpercent = potato2.grainlivenconc * 100
     floretnpercent = potato2.floretlivenconc * 100
     tubernpercent = potato2.tuberlivenconc * 100
     stemnpercent = potato2.stemlivenconc * 100
     if potato2.leaflivewt2 > 0 then
     leafnpercent = (potato2.leafliven2/potato2.leaflivewt2) * 100
     endif
     grainmaximumn = potato2.grainmaximumnconcvalue * 100
     floretmaximumn = potato2.floretmaximumnconcvalue * 100
     leafmaximumn = potato2.leafmaximumnconcvalue * 100
     stemmaximumn = potato2.stemmaximumnconcvalue * 100
     tubermaximumn = potato2.tubermaximumnconcvalue * 100
     grainminimumn = potato2.grainminimumnconcvalue * 100
     floretminimumn = potato2.floretminimumnconcvalue * 100
     leafminimumn = potato2.leafminimumnconcvalue * 100
     stemminimumn = potato2.stemminimumnconcvalue * 100
     tuberminimumn = potato2.tuberminimumnconcvalue * 100
     if potato2.stagecodevalue >= 3 then
     accumttsowing = accumttsowing + potato2.phenologythermaltimevalue
     endif
     
     SECTION:- end_of_day
     if simday = 1 then
     simday = 2
     else simday = 2
     endif
     END of rules
     
