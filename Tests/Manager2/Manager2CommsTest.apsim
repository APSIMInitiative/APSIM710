        <manager2 name="SecondVB_Manager">
          <ui />
          <text>
Imports System
Imports ModelFramework

Public Class Script 
      
   &lt;Link&gt; Dim MyPaddock As Paddock ' Can be used to dynamically get access to simulation structure and variables

   &lt;Link(NamePath := "Met")&gt; Dim MyMet As MetFile 
   &lt;Link(NamePath := "FirstVB_Manager")&gt; Dim MyOtherManager As Manager2

   &lt;Input&gt; Dim Today As DateTime   ' Equates to the value of the current simulation date - value comes from CLOCK
   &lt;Output&gt; Private AddRain As Double    
   &lt;Output&gt; Private AddVar1 As Double    
   &lt;Output&gt; Private AddVar2 As String    

   
   ' The following event handler will be called once at the beginning of the simulation
   &lt;EventHandler()&gt; Public Sub OnInitialised()

      
   End Sub

   
   ' The following event handler will be called each day at the beginning of the day
   &lt;EventHandler()&gt; Public Sub OnPrepare()
      
      AddRain = MyMet.Rain + 1.0
      MyOtherManager.Get("DoubleFromFirstManager", AddVar1): AddVar1 += 100.0
      
      MyOtherManager.Get("StringFromFirstManager", AddVar2): AddVar2 = AddVar2 &amp; "World"
      
      
      
   End Sub
   
End Class
   </text>
