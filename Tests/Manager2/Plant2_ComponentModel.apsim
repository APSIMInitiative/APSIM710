      <manager2 name="PhenMod">
        <ui>
          <Vn_Lag_Rate_b type="text" description="Base rate of progression through Vn Lag at high temperature">.4</Vn_Lag_Rate_b>
          <Vn_Lag_Rate_a type="text" description="Maximum rate of progression through Vn Lag at Low temperature">.4</Vn_Lag_Rate_a>
          <Vn_Lag_Rate_r type="text" description="Curvature of the change of rate of progression through Vn Lag with temperature">.55</Vn_Lag_Rate_r>
          <VnER_Slope_b type="text" description="Minimum Coefficient for the rate of Vn expression (/HS) at high temperature">.07</VnER_Slope_b>
          <VnER_Slope_a type="text" description="Maximum Coefficient for the rate of Vn expression (/HS) at low temperature">4</VnER_Slope_a>
          <VnER_Slope_r type="text" description="Curvature of the change of rate of Vn expression (/HS) with temperature">-.14</VnER_Slope_r>
          <VnER_Slope_int type="text" description="Minimum rate of VN expression following Vn Lag">0.018</VnER_Slope_int>
          <VnBaseTarget type="text" description="Amount of Vn expression needed to satisify vernalisation in short photoperiods">0.235</VnBaseTarget>
        </ui>
        <text>
using System;
using ModelFramework;

public class Script 
   {      
   [Link] Paddock MyPaddock; // Can be used To dynamically Get access To simulation Structure And variables
  
   //Set up Parameter values
   [Param] double Vn_Lag_Rate_b;
   [Param] double Vn_Lag_Rate_a;
   [Param] double Vn_Lag_Rate_r;
   [Param] double VnER_Slope_a;
   [Param] double VnER_Slope_b;
   [Param] double VnER_Slope_r;
   [Param] double VnER_Slope_int;
   [Param] double VnBaseTarget;
   
   //Set up data from other modules
   [Input] double MaxT;
   [Input] double MinT;
   
   //Set up links to Plant.NET functions
   [Link] Function PhotoperiodFunction;
   [Link] Function ThermalTime;
   [Link] Function HaunStage;   
   [Link] Function Vrn2rate;
   [Link] Function Vrn3rate;
   
   //Set up class variables
   [Output] double AccumTt = 0;
   [Output] double Vrn1Lag = 0;
   [Output] double Vrn1 = 0;
   [Output] double Vrn1Target = 0;
   [Output] double Pp = 0;
   [Output] double Tt = 0;
   [Output] double HS = 0;
   [Output] double Vrn2 = 0;
   [Output] double FT1 = 0;
   [Output] double VnER_coefficient = 0;
   [Output] double MeanT = 0;
   [Output] double HaunStageYesterday = 0;
   [Output] double DeltaHaunStage = 0;
   [Output] double FIHS = 0;
   [Output] double TSHS = 0;
   [Output] double FLN = 0;
   [Output] bool IsGerminated = false;
   [Output] bool LagIsFinished = false;
   [Output] bool IsVernalised = false;
   [Output] bool IsInduced = false;
   [Output] bool TerminalSpikeletReached = false;
   
   //Event procedures
   [EventHandler] public void OnInitialised()
   {
   Vrn1Target = VnBaseTarget;
   }
      
   [EventHandler] public void OnPrepare()
   {
      MeanT = (MaxT + MinT) / 2;
      Pp = PhotoperiodFunction.Value;
      Tt = ThermalTime.Value;
      HS = HaunStage.Value;
      if (IsGerminated)
         DeltaHaunStage = HS - HaunStageYesterday;
      HaunStageYesterday = HS;
      AccumTt += Tt; 
      VnER_coefficient = VnER_Slope_b + VnER_Slope_a * Math.Exp(VnER_Slope_r * MeanT);
      IsGerminated = true; //Germinated on 1st day
      
      //Determine progress through Vernalisation lag
      if (LagIsFinished == false)
      {
         Vrn1Lag += (Vn_Lag_Rate_b + Vn_Lag_Rate_a * Math.Exp(Vn_Lag_Rate_r * VnER_coefficient)) * DeltaHaunStage;
         Vrn1Lag = Math.Min(1.0, Vrn1Lag);
         if (Vrn1Lag == 1.0)
            LagIsFinished = true;
      }
      //If Lag complete determine extent of Vrn expression
      if ((LagIsFinished) &amp;&amp; (IsVernalised == false))
      {
         Vrn1 += (VnER_Slope_int + VnER_coefficient * Math.Pow(Vrn1, 1 - 0.06 * VnER_coefficient)) * DeltaHaunStage;
         Vrn1 = Math.Min(1.0, Vrn1);
      }
      //Update Vernalisation target to reflect photoperiod conditions and determine Vernalisation status
      if ((IsVernalised == false) &amp;&amp; (Vrn1Target &lt;= 1.0))
      { 
         Vrn1Target += Vrn2rate.Value;
         Vrn1Target = Math.Min(1.0, Vrn1Target);
         if (Vrn1 &gt;= Vrn1Target)
            IsVernalised = true;
      }
      //If Vernalisation is complete begin expressing FT1
      if ((IsVernalised) &amp;&amp; (TerminalSpikeletReached == false))
      {
         FT1 += Vrn3rate.Value * DeltaHaunStage;
         FT1 = Math.Min(1.0, FT1);
      }
      
      //Set timings of floral initiation, terminal spiklet and FLN in response to FT1 expression
      if ((FT1 &gt;= 0.3) &amp;&amp; (IsInduced == false))
      {
         IsInduced = true;
         FIHS = HS;
      }
      if ((FT1 &gt;= 1.0) &amp;&amp; (TerminalSpikeletReached == false))
      {
         TerminalSpikeletReached = true;
         TSHS = HS + 1.0;
         FLN = 2.86 + 1.1 * TSHS;
      }
   }
   
}
       </text>
