      <manager2 name="Script">
        <ui />
        <text><![CDATA[Imports System
Imports ModelFramework

Public Class Script
   Inherits Instance

   <Input()> Private Day As Integer

   <EventHandler()> Public Sub OnPost()
      Dim MyPaddock As New PaddockType(Me)
      
      Dim dlt(0) As Double
      dlt(0) = 100
      
      Dim part(0) As String
      part(0) = "Leaf"
      
      Dim dms(0) As RemoveCropDmdmType
      dms(0) = New RemoveCropDmdmType
      dms(0).dlt = dlt
      dms(0).part = part
      dms(0).pool = "Pool"
      
      
      Dim MyRemoveCropDM As New RemoveCropDmType
      MyRemoveCropDM.dm = dms
      
      
      If Day = 300 Then
         MyPaddock.ComponentByName("lucerne").Publish("remove_crop_biomass", MyRemoveCropDM)
      End If
   End Sub
End Class
    ]]></text>
