               ! ----------------------------


! ------------------ swim calculation parameters ---------------------
[sample.SWIM2.calc]
dtmin =  0.0   ! min timestep (min)
dtmax =  1440.   ! max timestep (min)
ersoil =  0.000001   !
ernode =  0.000001   !
errex =  0.01   !
dppl =  2   !
dpnl =  1   !
