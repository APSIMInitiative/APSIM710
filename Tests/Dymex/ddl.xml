<?xml version="1.0"?>
<typedef>
   <type name="error">
      <field name="fatal" kind="boolean"/>
      <field name="message" kind="string"/>
   </type>

	<type kind="integer4" name="my_int_array"/>

	<type array="T" name="temperature_array">
	      <element kind="double" array="T" unit="oC"/>
	</type>

   <type name="tickType">
      <field name="startday" kind="integer4"/>
      <field name="startsec" kind="integer4"/>
      <field name="startsecpart" kind="double"/>
      <field name="endday" kind="integer4"/>
      <field name="endsec" kind="integer4"/>
      <field name="endsecpart" kind="double"/>
   </type>"

   <type name="a_record">
      <field kind="double" name="height" unit="mm">
      </field>
      <field kind="integer4" name ="apples" array="T">
      </field>
      <field kind="single" name="conductivity" array="T" unit="dS/m">
      </field>
   </type>
	<type kind="string" name="my_string"/>

	<type kind="char" name="char_array" array="T"/>

	<type array="T" name="supplements">
		<field kind="string" name="fld_name">
		</field>
		<field kind="double" name="dmd" unit="%">
		</field>
		<field kind="double" name="me2dm" unit="MJ/kg">
		</field>
		<field kind="double" name="cp" unit="%">
		</field>
	</type>

	<type kind="predefined" name="my_predefined" predefname="layers"/>

</typedef>
