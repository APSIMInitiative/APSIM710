version = 7.1
[APSIM.test1_millet]
title=Test sowing before out
module = clock   test.par [all] %apsim%/Model/clock.xml [standard]
module = report     test.par[test1]  test.par[all]
module = input(met)        %apsim%/Examples/MetFiles/dalby.met[weather]
module = manager    test.par[test1]   test.par[all]
module = SoilWat   %apsim%/Examples/Soils/soil.par [black_earth] %apsim%/Model/Soil.xml [SoilWat]
module = SurfaceOM   %apsim%/Examples/Soils/soil.par [legume_residue] %apsim%/Model/SurfaceOM.xml [standard]
module = SoilN   %apsim%/Examples/Soils/soil.par [black_earth] %apsim%/Model/Soil.xml [SoilN]
module = canopy   test.par[all]
module = millet   test.par [all] %apsim%/Model/millet.xml [standard]
module = millet(millet1)   test.par [all] %apsim%/Model/millet1.xml [standard]
module = millet(millet2)   test.par [all] %apsim%/Model/millet2.xml [standard]
module = millet(millet3)   test.par [all] %apsim%/Model/millet3.xml [standard]
module = millet(millet4)   test.par [all] %apsim%/Model/millet4.xml [standard]
module = millet(millet5)   test.par [all] %apsim%/Model/millet5.xml [standard]

