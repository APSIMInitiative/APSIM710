[test3.SummaryFile.parameters]
summaryfile=test3.sum
[test3.report.parameters]
outputfile  =test3.out
[test3.manager.start_of_day]
if harvest = 1 then
   millet sow population =  7.5, sowing_depth = 30, cultivar = wrajpop
   harvest = 2
endif

if millet.StageName = 'harvest_ripe' or millet.PlantStatus = 'dead' then
   millet harvest
   millet1 harvest
   millet2 harvest
   millet3 harvest
   millet4 harvest
   millet5 harvest
   millet end_crop
   millet1 end_crop
   millet2 end_crop
   millet3 end_crop
   millet4 end_crop
   millet5 end_crop
   harvest = 1
endif


[all.report.parameters]
variable = clock.day
variable = clock.year
variable = millet.LAI
variable = millet.Biomass
variable = millet.green_biomass

[all.canopy.parameters]
intercrop =  millet millet1 millet2 millet3 millet4 millet5

[all.clock.parameters]
! Start and end date of run (day number of year and year)
start_date=1/1/1988
end_date=26/12/1988

[all.manager.init]
harvest = 0

[all.manager.start_of_day]

if day = 166 and year  = 1988 then
   millet sow population =  7.5, sowing_depth = 30, cultivar = wrajpop
endif

[all.manager.end_of_day]
report do_end_day_output


