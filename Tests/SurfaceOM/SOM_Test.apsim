      <outputfile name="report">
        <filename output="yes">SurfaceOM_Test7 report.out</filename>
        <title>SurfaceOM_Test7 report</title>
        <variables>
          <variable>dd/mmm/yyyy</variable>
          <variable>day</variable>
          <variable>year</variable>
          <variable>rice.wagt as rice_biomass</variable>
