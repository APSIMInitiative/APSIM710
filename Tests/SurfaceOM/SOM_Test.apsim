            endif
            if day = 20 then
               surfaceom add_surfaceom name=residue1, type = inert, mass=2000, cnr = 10, cpr = 100
            endif
            if day = 25 then
               surfaceom add_surfaceom name=residue3, type = inert, mass=2000, cnr = 10, cpr = 100
            endif
            </text>
          <event>start_of_day</event>
