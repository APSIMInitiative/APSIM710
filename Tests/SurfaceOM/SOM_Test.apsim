      <manager>
        <script>
          <text>! **************** Rice crop 2005 ***********************

! estaablish paddy bunds on 1 June (152) 

if day = 152 and year = 2005 then
   max_pond = 200
endif

! sow crop on 11 June 2005
if day = 163 and year = 2005 then 
   rice sow cultivar = IR72, establishment = transplant, sbdur = 25, nplh = 2, nh = 25, nplsb = 1000
   fertiliser apply amount = 5, depth = 50, type = urea_N  
endif

!Fertilizer application at transplanting
if day = 188 and year = 2005 then
     fertiliser apply amount = 40, depth = 50, type = urea_N  
endif

! irrigate as soon as there is no standing water on the surface, if the rice crop stage is less than late grain-filling
if day &gt; 163 and rice.dvs &lt; 2.2 and year = 2005 then 
     if pond = 0 then
        irrigation apply amount = 60
        count = 0
     endif
  endif

! ***** EXAMPLE OF AWD LOGIC *********
!if day &gt; 163 and day &lt; 295 then 
!     if pond = 0 and count = 7 and irri_count &lt; 25 then
!        irrigation apply amount = 60
!        irri_count = irri_count + 1
!        count = 0
!     endif
!     if pond = 0 and count &lt;7 then
!        count = count +1
!     endif
!  endif
! ***********************************

! top dress 1 21 DAT
if day = 209 and year = 2005 then        
   fertiliser apply amount = 40, depth = 0, type = urea_N   
endif

! top dress 2 35 DAT
if day = 244 and year = 2005 then        
   fertiliser apply amount = 40, depth = 0, type = urea_N   
endif

! open up rice bunds and drain pond at after maturity stage
if rice.dvs &gt; 2.2 then
   max_pond = 0
endif

! harvest rice 
if dvs &gt; 2.4 and year = 2005 then
     rice end_crop
endif

! two weeks after rice harvest, remove 100% of rice residue for livestock feed, leave the algae
if day = 293 and year = 2005 then
     SurfaceOM tillage_single name = rice, type = burn  ! remove 100% of rice residues from the field, but leave all the algae 
!     SurfaceOM tillage type = burn  ! this is what we don't want to do....remove 100% of both rice residues and algae from the field 
endif
    </text>
          <event>start_of_day</event>
        </script>
