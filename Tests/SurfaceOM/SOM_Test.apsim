        <automatic_irrigation type="list" listvalues="on,off" description="Automatic irrigation">off</automatic_irrigation>
        <asw_depth type="text" description="Depth to which ASW is calculated. (mm)">600</asw_depth>
        <crit_fr_asw type="text" description="Fraction of ASW below which irrigation is applied (0-1.0)">0.5</crit_fr_asw>
        <irrigation_efficiency type="text" description="Efficiency of the irrigation. (0-1.0)">1</irrigation_efficiency>
        <irrigation_allocation type="list" listvalues="on,off" description="Allocation limits">off</irrigation_allocation>
        <allocation type="text" description="Allocation in mm">0</allocation>
        <default_no3_conc type="text" description="Nitrate concentration (ppm N)">0.0</default_no3_conc>
        <default_nh4_conc type="text" description="Ammonium concentration (ppm N)">0.0</default_nh4_conc>
        <default_cl_conc type="text" description="Chloride concentration (ppm Cl)">0.0</default_cl_conc>
      </irrigation>
      <manager>
        <script>
          <text>
            !**** wheat *********
            if day = 169 and year = 1986 then
               wheat sow cultivar = hartog, plants = 120, sowing_depth = 30 (mm)
            endif
            if wheat.stagename = 'harvest_ripe' or wheat.plant_status = 'dead' then
               wheat harvest
               wheat end_crop
            endif
            !**** sorghum *********
               if day = 305 and year = 1986 then
                  sorghum sow cultivar = early, plants = 10, sowing_depth = 30 (mm), tiller_no_fertile = 0 (), row_spacing = 1.0 (m)
                            ! optional skiprow arguments are: skip = solid; skip = single; skip = double
               irrigation apply amount = 150 (mm)
               fertiliser apply amount = 100 (kg/ha), depth = 50 (mm), type = no3_n
               endif
               if (sorghum.stage = 11) then
                  sorghum harvest
                  sorghum end_crop
               endif
            ! if crop killed due to stress, end simulation
            if (sorghum.plant_status = 'dead')  then
                  sorghum harvest
                  sorghum kill_crop
                  sorghum end_crop
            endif
            !**** canola *********
            if day = 130 and year  = 1989 then
               fertiliser apply amount = 100 (kg/ha), depth = 50 (mm), type = no3_n
               canola sow cultivar = hyola42, plants = 40 (/m2), sowing_depth = 20 (mm), row_spacing = 300 (mm)
            endif
            if canola.stagename = 'harvest_ripe' or canola.plant_status = 'dead' then
               canola harvest
               canola end_crop
            endif
            !**** wheat again *********
            if day = 169 and year = 1990 then
               wheat sow cultivar = hartog, plants = 120, sowing_depth = 30 (mm)
            endif
            if wheat.stagename = 'harvest_ripe' or wheat.plant_status = 'dead' then
               wheat harvest
               wheat end_crop
            endif
            </text>
          <event>start_of_day</event>
        </script>
