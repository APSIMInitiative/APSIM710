version = 7.2
[apsim.Test1]

title= Surface Organic Matter Module Test 1
module = clock   Test.par [sample] %apsim%/Model/clock.xml [standard]
module = report   Test.par [test1]
module = input(Met)      optimum.MET [weather]
module = manager  Test.par [test1]
module = SoilWat   %apsim%/Examples/Soils/Soil.par [black_earth] %apsim%/Model/Soil.xml [SoilWat]
module = SurfaceOM   Test.par [test1] %apsim%/Model/SurfaceOM.xml [standard]
module = SoilN   %apsim%/Examples/Soils/Soil.par [black_earth] %apsim%/Model/Soil.xml [SoilN]

