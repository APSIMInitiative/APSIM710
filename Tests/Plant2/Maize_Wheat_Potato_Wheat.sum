------- Roto-rainer_irrigation Initialisation ---------------------------------
     Manager rules:
     
     SECTION:- init
     irrigation_period = 1
     number_of_paddocks = 7
     paddock = 0
     days_since_rain_event = 0
     rain_event_trigger = 30
     rain_event_pause_time = 3
     irrigation_amount = 50
     trigger_deficit = 0.5
     
     SECTION:- start_of_day
     if        (date_within('1-jun,1-aug') = 1) then
     irrigation_period=0
     paddock=0
     endif
     if       (swd_top<trigger_deficit) then
      irrigation_period=1
     endif
     if        (rain>=rain_event_trigger) then
       days_since_rain_event = 0
     else     days_since_rain_event = days_since_rain_event +1
     endif
     if           (irrigation_period =1) and (paddock = 0) and (days_since_rain_event > rain_event_pause_time) then
        paddock = paddock +2
           if  (swd_top<trigger_deficit) and (paddock_is_fallow()=0) then
                  irrigation apply amount = irrigation_amount
                  paddock = paddock -1
           endif
     elseif  (irrigation_period =1) and (paddock = number_of_paddocks) and (days_since_rain_event > rain_event_pause_time) then
        paddock = 0
     elseif  (irrigation_period =1) and (paddock > 0) and (days_since_rain_event > rain_event_pause_time) then
        paddock = paddock +1
     endif
     
     SECTION:- end_of_day
     END of rules
     
     Manager creating a new local real variable : irrigation_period = 1.00000
     Manager creating a new local real variable : number_of_paddocks = 7.00000
     Manager creating a new local real variable : paddock = 0.00000
     Manager creating a new local real variable : days_since_rain_event = 0.00000
     Manager creating a new local real variable : rain_event_trigger = 30.00000
     Manager creating a new local real variable : rain_event_pause_time = 3.00000
     Manager creating a new local real variable : irrigation_amount = 50.00000
     Manager creating a new local real variable : trigger_deficit = 0.50000

------- Full nitrogen rule Initialisation -------------------------------------
     Manager rules:
     
     SECTION:- init
     runningmeantemp = 15
     fertilising_window = 1
      trigger_level = 45
      n_application_amount = 50
      n_available = 0
     
     SECTION:- start_of_day
     n_available = no3(1) + no3(2) + no3(3) + nh4(1) + nh4(2) + nh4(3)  + urea(1) + urea(2) + urea(3)
     deep_n = no3(4) + no3(5) + no3(6) + no3(7) + nh4(4) + nh4(5) + nh4(6) + nh4(7) + urea(4) + urea(5) + urea(6) + urea(7)
     runningmeantemp = (runningmeantemp * 13 + mean) / 14
     if           (date_within('1-jan, 20-jul') = 1) and
      runningmeantemp < 10 then
      fertilising_window = 0
     elseif  (date_within('21-jul, 31-dec') = 1) and
      runningmeantemp < 8 then
      fertilising_window = 0
     else    fertilising_window = 1
     endif
     if               (wheat.plant_status = 'alive') then
       if               (n_available < trigger_level)          and
          (wheat.stage > 3)                                 and
          fertilising_window = 1                        and
          (wheat.stage < 6)                                          then
     fertiliser apply amount = n_application_amount (kg/ha), depth = 50 (mm), type = urea_n ()
       endif
     elseif      (maize.plant_status = 'alive')           and
          (n_available < trigger_level)          and
          (maize.stage > 3)                                and
          fertilising_window = 1                        and
          (maize.stage < 7)                                          then
     fertiliser apply amount = n_application_amount (kg/ha), depth = 50 (mm), type = urea_n ()
     elseif      (potato2.plant_status = 'alive') then
       if               (n_available < trigger_level)           and
          (potato2.stagecodevalue > 3)                                 and
          fertilising_window = 1                        and
          (potato2.stagecodevalue < 6.3)                                      then
     fertiliser apply amount = n_application_amount (kg/ha), depth = 50 (mm), type = urea_n ()
       endif
     endif
     
     SECTION:- end_of_day
     END of rules
     
     Manager creating a new local real variable : runningmeantemp = 15.00000
     Manager creating a new local real variable : fertilising_window = 1.00000
     Manager creating a new local real variable : trigger_level = 45.00000
