------- HawkesBay base Rotation Initialisation --------------------------------
     Manager rules:
     
     SECTION:- init
     rotation_counter = 0
     days_after_harvest = 0
     reset_pt = 0
     swd = 0
     swd_top = 0
     ptirrig = 0
     ptes = 0
     pteo = 0
     ptep = 0
     ptrain = 0
     ptrunoff = 0
     ptdrain = 0
     ptfert = 0
     ptleach = 0
     ptmin = 0
     ptmin_res = 0
     ptdnit = 0
      ptcropn = 0
     
     SECTION:- start_of_day
     swd = ((sw(1) +sw(2) +sw(3)) - (ll15(1) + ll15(2) +ll15(3)))/((dul(1) + dul(2) + dul(3)) - (ll15(1) + ll15(2) +ll15(3)))
     swd_top = (sw(1) - ll15(1))/(dul(1) - ll15(1))
     days_after_harvest = days_after_harvest+1
     reset_pt = 0
     if (date_within('1-oct,1-nov') = 1) and
     (rotation_counter = 0) and (days_after_harvest > 2) and (swd_top<=1)  then
        maize sow plants =10, sowing_depth =30, cultivar = pioneer_3153, row_spacing = 0.762, crop_class = plant
        outputfile do_output
        rotation_counter = 1
        reset_pt = 1
     elseif (date_within('2-nov,1-dec') = 1) and
     (rotation_counter = 0) and (days_after_harvest > 2) and (swd_top<=1)then
        maize sow plants =10, sowing_depth =30, cultivar  pioneer_3153, row_spacing = 0.762, crop_class = plant
        outputfile do_output
        rotation_counter = 1
        reset_pt = 1
     elseif (today=date('2-dec') and
     (rotation_counter = 0) then
        outputfile do_output
        rotation_counter = 2
        reset_pt = 1
     endif
     if maize.plant_status <> 'out' then
     if           (rotation_counter = 1) and (maize.stagename = 'end_grain_fill' or maize.plant_status = 'dead' or today=date('31-may')) then
        outputfile do_output
        maize harvest height = 0.1 (mm) , remove = 0.98 ()
        maize end_crop
        rotation_counter = 2
        days_after_harvest = 0
        reset_pt = 1
     endif
     endif
     if           today<date('1-jul') and
      (rotation_counter = 2) and (days_after_harvest > 2) and (swd_top<=1)  then
          wheat sow plants =400, sowing_depth =30, cultivar = avalon, row_spacing = 150, crop_class = plant
        outputfile do_output
        rotation_counter = 3
        reset_pt = 1
     elseif  (today=date('2-jul') and
      (rotation_counter = 2) then
        outputfile do_output
        rotation_counter = 4
        reset_pt = 1
     endif
     if wheat.plant_status <> 'out' then
     if           (rotation_counter = 3) and (wheat.stagename = 'harvest_ripe' or wheat.plant_status = 'dead' or today = date('23-jul')) then
        outputfile do_output
        wheat  harvest height = 0.1 (mm) , remove = 0.98 ()
        wheat  end_crop
        rotation_counter = 4
        days_after_harvest = 0
        reset_pt = 1
     endif
     endif
     if          (date_within('1-sep,1-dec') = 1) and
     (rotation_counter = 4) and (days_after_harvest > 2) and (swd_top<=20)  then
      potato2 sow population = 4, depth = 150, cultivar = russetburbank, maxcover = 1.0, budnumber = 3, row_spacing = 800, crop_class = plant
        outputfile do_output
        rotation_counter = 5
        reset_pt = 1
     elseif (today=date('2-dec') and
     (rotation_counter = 4) then
        outputfile do_output
     rotation_counter = 6
        reset_pt = 1
     endif
     if potato2.plant_status <> 'out' then
     if           ((rotation_counter = 5) and (potato2.phenologycurrentphasename = 'maturity' or potato2.plant_status = 'dead')) then
        outputfile do_output
        potato2 harvest height = 0.1 (mm) , remove = 0.98 ()
        potato2 endcrop
      days_after_harvest = 0
     rotation_counter = 6
        reset_pt_variables = 1
        reset_pt = 1
     endif
     endif
     if          (today<date('1-jun') and
     (rotation_counter = 6) and (days_after_harvest > 2) and (swd_top<=1)  then
        wheat sow plants =400, sowing_depth =30, cultivar = avalon, row_spacing = 150, crop_class = plant
        outputfile do_output
        rotation_counter = 7
        reset_pt = 1
     elseif  (today=date('2-may') and
      (rotation_counter = 6) then
        outputfile do_output
        rotation_counter = 0
        reset_pt = 1
     endif
     if wheat.plant_status <> 'out' then
     if           (rotation_counter = 7) and (wheat.stagename = 'harvest_ripe' or wheat.plant_status = 'dead' or today = date('1-may')) then
        outputfile do_output
        wheat  harvest height = 0.1 (mm) , remove = 0.98 ()
        wheat  end_crop
        rotation_counter = 0
        reset_pt = 1
        days_after_harvest = 0
     endif
     endif
     
     SECTION:- end_of_day
     ptirrig = ptirrig + irrigation
     ptes = ptes + es
     pteo = pteo + eo
     ptrain = ptrain + rain
     ptrunoff = ptrunoff + runoff
     ptdrain = ptdrain + drain
     ptfert = ptfert + fertiliser
     ptleach = ptleach + no3_leach(7) + nh4_leach(7)
     ptmin = ptmin + dlt_n_min()
     ptmin_res = ptmin_res + dlt_n_min_res()
     ptdnit = ptdnit + dnit()
     if wheat.plant_status <> 'out' then
       ptep = ptep + wheat.ep
       ptcropn = ptcropn + wheat.n_uptake
     endif
     if maize.plant_status <> 'out' then
       ptep = ptep + maize.ep
       ptcropn = ptcropn + maize.no3_uptake
     endif
     if potato2.plant_status <> 'out' then
       ptep = ptep + potato2.leaftranspiration
       ptcropn = ptcropn + potato2.rootnuptake
     endif
     if          (reset_pt = 1) then
        ptirrig = 0
        ptes = 0
        pteo = 0
        ptep = 0
        ptrunoff = 0
        ptdrain = 0
        ptfert = 0
        ptleach = 0
        ptmin = 0
        ptmin_res = 0
       ptdnit = 0
      ptcropn = 0
      ptcropn = 0
     endif
     END of rules
     
