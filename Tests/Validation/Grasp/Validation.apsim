      <outputfile name="Daily">
        <variables name="Variables">
          <variable name="dd/mm/yyyy as Date" description="Date (dd/mm/yyyy)" array="?" />
          <variable name="tsdm" description="" array="" units="kg/ha" />
          <variable name="cover_tot" description="" array="" units="kg/ha" />
          <variable name="crop_status" description="" array="" units="kg/ha" />
        </variables>
        <events name="Reporting Frequency">
          <event name="Daily" description="End of day" array="No" />
        </events>
      </outputfile>
      <outputfile name="Annual">
        <variables name="Variables">
          <variable name="year as Year" description="Date (dd/mm/yyyy)" array="?" />
          <variable name="grasp.n_uptake" description="" array="" units="kg/ha" />
          <variable name="tracker.dlt_dm as total_growth" description="" array="" units="kg/ha" />
        </variables>
        <events name="Reporting Frequency">
          <event name="End_year" description="End of year" array="No" />
        </events>
      </outputfile>
      <tracker>
        <variable name="sum of Scratch.grasp_dlt_dm on end_of_day from Annual.reported to now as dlt_dm" />
      </tracker>
      <operations>
        <operation condition="end_of_day">
          <date>11-Oct-1991</date>
          <action>grasp set green_pool= 702 !(kg/ha)</action>
        </operation>
        <operation condition="end_of_day">
          <date>11-Oct-1991</date>
          <action>grasp set dead_pool = 0 !(kg/ha)</action>
        </operation>
        <operation condition="end_of_day">
          <date>15-Oct-1991</date>
          <action>'Site67 Water' set sw_dep = 5 39.5 42.4 3 !(mm)</action>
        </operation>
        <operation condition="end_of_day">
          <date>10-Nov-1992</date>
          <action>grasp set green_pool= 80 !(kg/ha)</action>
        </operation>
        <operation condition="end_of_day">
          <date>10-Nov-1992</date>
          <action>grasp set dead_pool= 0 !(kg/ha)</action>
