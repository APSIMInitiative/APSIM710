                <density type="text" description="Enter sowing density  (plants/m2) : ">4</density>
                <depth type="text" description="Enter sowing depth  (mm) : ">30</depth>
                <cultivar type="cultivars" description="Enter cultivar : ">Buster</cultivar>
                <class type="classes" description="Enter crop growth class : ">plant</class>
                <row_spacing type="text" description="Enter row spacing (mm) : ">500</row_spacing>
                <skiprow type="list" listvalues="solid, single, double" description="Skip row : ">solid</skiprow>
                <ftn type="text" description="Enter Fertile Tiller Number :">3.27</ftn>
              </ui>
              <script>
                <text>if (today = date('[date]') then
[crop] sow plants =[density], sowing_depth = [depth], cultivar = [cultivar], row_spacing = [row_spacing], crop_class = [class], skip = [skiprow], tiller_no_fertile = [ftn]
endif
</text>
                <event>start_of_day</event>
              </script>
            </manager>
