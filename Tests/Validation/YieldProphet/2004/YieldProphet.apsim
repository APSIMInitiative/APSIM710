   elseif (Z >= 55 and Zadok55Das = 0) then
      Zadok55Das = DaysAfterSowing
   elseif (Z >= 65 and Zadok65Das = 0) then
      Zadok65Das = DaysAfterSowing
   elseif (Z >= 75 and Zadok75Das = 0) then
      Zadok75Das = DaysAfterSowing
   endif

]]></text>
