   elseif (Z >= 31 and Zadok31Das = 0) then
      Zadok31Das = DaysAfterSowing
   elseif (Z >= 32 and Zadok32Das = 0) then
      Zadok32Das = DaysAfterSowing
   elseif (Z >= 37 and Zadok37Das = 0) then
