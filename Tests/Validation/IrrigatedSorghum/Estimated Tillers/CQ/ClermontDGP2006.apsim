              [crop]  end_crop
           endif
 
            </text>
            <event>end_of_day</event>
          </script>
          <ui>
            <category name="Harvesting criteria" />
            <crop type="modulename" description="Enter name of crop to harvest when ripe : ">Sorghum</crop>
          </ui>
        </manager>
