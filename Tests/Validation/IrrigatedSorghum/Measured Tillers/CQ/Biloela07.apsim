          </ui>
          <script name="start_of_day">
            <text><![CDATA[if (today = date('[date]') then
    if (day > 152) then
        season  = year
    else
        season  = year -1
    endif

    SowDay = day
     SowYear = year
    [crop] sow population =[density], sowing_depth = [depth], cultivar = [cultivar], row_spacing = [row_spacing], skip = [RowConfiguration], tiller_no_fertile = [ftn]
endif
               ]]></text>
            <event>start_of_day</event>
          </script>
        </manager>
        <manager name="Harvesting rule">
          <ui>
            <category description="Harvesting criteria" type="category" />
            <crop type="crop" description="Enter name of crop to harvest when ripe : ">sorghum</crop>
