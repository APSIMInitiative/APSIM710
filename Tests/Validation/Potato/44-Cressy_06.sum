------- Sowing rule Initialisation --------------------------------------------
     Manager rules:
     
     SECTION:- start_of_day
           if (paddock_is_fallow() = 1 and fallowin <> 'yes' and (nextcrop = 0 or nextcrop = 'potato')) then
              if (date_within('27-nov, 28-nov') = 1) then
     if (rain[7] >= 0 and esw >= 0) or
         ('yes' = 'yes' and today = date('28-nov'))) then
        choosenextcrop = 'yes'
       stem_per_seed = -100+(0.3*150)
       if stem_per_seed > 10 then
          stem_per_seed = 10
       elseif stem_per_seed < 2 then
          stem_per_seed = 5.4
       endif
       stf = st(1)/10
       if (stf < 0) then
           stf = 0
        endif
        if (stf > 1) then
           stf = 1
        endif
        swf = (sw(1) - ll(1))/(dul(1) - ll(1))
        if (swf < 0) then
           swf = 0
        endif
        if (swf > 1) then
           swf = 1
        endif
        density = 6 * stem_per_seed * stf * swf
        potato sow plants = density, sowing_depth = 20, cultivar = russet, row_spacing = 815, crop_class = plant
     endif
     if today = date('28-nov') then
        choosenextcrop = 'yes'
     endif
              endif
           endif
     END of rules
     

------- Harvesting rule Initialisation ----------------------------------------
     Manager rules:
     
     SECTION:- end_of_day
                if potato.stagename = 'maturity' or potato.plant_status = 'dead' then
       potato  harvest
       potato  end_crop
                endif
     END of rules
     

------- Sowing fertiliser Initialisation --------------------------------------
     Manager rules:
     
     SECTION:- potato.sowing
             fertiliser apply amount = 150 (kg/ha), depth = 50 (mm), type = no3_n
     END of rules
     

------- Fertilise on fixed date Initialisation --------------------------------
     Manager rules:
     
     SECTION:- start_of_day
              if (today = date('5-jan') then
     n_topsoil = no3(1) + nh4(1) + no3(2) + nh4(2)
     if (n_topsoil < 1000) then
        fertiliser apply amount = 58 (kg/ha), depth = 50 (mm), type = urea_n ()
     endif
              endif
     END of rules
     

------- Logic Initialisation --------------------------------------------------
     Manager rules:
     
     SECTION:- init
      irriginterval = 0
     applied_irr_tot = 0
     
     SECTION:- start_of_day
     irriginterval = irriginterval + 1
      if potato.plant_status = 'alive'  and (potato.stage > 3) and (potato.stage < 6) and (irriginterval > 11) then
     'radford water' set dlt_sw_dep = 20.0 0.0 0.0 0.0 0.0
                irriginterval = 0
                applied_irr_tot = applied_irr_tot + 20
      endif
     
     SECTION:- end_of_day
     END of rules
     
