                  stf = 0
               endif
               if (stf > 1) then
                  stf = 1
               endif        
       
               swf = (sw(1) - ll(1))/(dul(1) - ll(1))
               if (swf < 0) then
                  swf = 0
               endif
               if (swf > 1) then
