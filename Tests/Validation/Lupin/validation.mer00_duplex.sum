     SECTION:- end_of_day
          if lupin.daysaftersowing > 0 then
             report do_output
          endif
     sw_70 = sw_dep(1) + sw_dep(2) + sw_dep(3) + sw_dep(4) + sw_dep(5) + sw_dep(6) + sw_dep(7) + sw_dep(8)
     sw_150 = sw_dep(1) + sw_dep(2) + sw_dep(3) + sw_dep(4) + sw_dep(5) + sw_dep(6) + sw_dep(7) + sw_dep(8)+ sw_dep(9)+ sw_dep(10) )+ sw_dep(9)+ sw_dep(10)  )+ sw_dep(9)+ sw_dep(10)  )+ sw_dep(9)+ sw_dep(10)+ sw_dep(11)+ sw_dep(12)
     sw_70 = sw_dep(1) + sw_dep(2) + sw_dep(3) + sw_dep(4) + sw_dep(5) + sw_dep(6) + sw_dep(7) + sw_dep(8)
     sw_40 = sw_dep(1) + sw_dep(2) + sw_dep(3) + sw_dep(4) + sw_dep(5)
     swc_40 = (( (sw(1) + sw(2) )/2) + sw(3) + sw(4) + sw(5))/4
     swc_10 = (sw(1) + sw(2))/2
     swc_20 = sw(3)
     swc_30 = sw(4)
     swc_50 = (sw(5) + sw(6))/2
     swc_70 = (sw(7) + sw(8))/2
     swc_90 = (sw(9)
     swc_120 = (2/3*sw(10)) + (1/3*sw(11))
     swc_150 = (1/3*sw(11)) + (2/3*sw(12))
     avgt   = (met.maxt + met.mint) / 2
     eta = ep + es
     eta_cum = eta_cum + eta
     etp_cum =etp_cum + eo
     if (lupin.phenologystage>=3 and lupin.phenologystage <= 8) then
        hi = yield / biomass
        hi_green = grainwt / abovegroundlivewt
        sla_tot = lai/leaflivewt
        yield_in_t = yield/1000
     else
        hi = 0
        hi_green = 0
        sla_tot = 0
        yield_in_t = 0
     endif
     if (lupin.stagename = 'harvest_ripe' or lupin.plantstatus = 'dead') then
        lupin harvest
        lupin kill_crop
        lupin end_crop
        dateset = 0
     endif
     END of rules
     
