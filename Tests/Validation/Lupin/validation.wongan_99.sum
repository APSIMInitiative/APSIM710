------- Manager Initialisation ------------------------------------------------
     Manager rules:
     
     SECTION:- init
          dateset = 0
          emer_date = 0
          endjuv_date = 0
          anth_date = 0
          mat_date = 0
          event = 0
     hi = 0
     hi_green = 0
     sla_tot=0
     eta=0
     eta_cum = 0
     etp_cum=0
     apoc_rain = 0
     dlt_lai = 0
     lai_yesterday = 0
     lai_today = 0
     sla = 0
     day_cnt = 0
     
     SECTION:- start_of_day
     if day>90 and day<305 then
        apoc_rain = apoc_rain + rain
     endif
     
     SECTION:- end_of_day
          if lupin.daysaftersowing > 0 then
             report do_output
          endif
     sw_70 = sw_dep(1) + sw_dep(2) + sw_dep(3) + sw_dep(4) + sw_dep(5) + sw_dep(6) + sw_dep(7) + sw_dep(8)
     sw_110 = sw_dep(1) + sw_dep(2) + sw_dep(3) + sw_dep(4) + sw_dep(5) + sw_dep(6) + sw_dep(7) + sw_dep(8)+ sw_dep(9)+ sw_dep(10)
     swc_10 = (sw(1) + sw(2))/2
     sw_40 = sw_dep(1) + sw_dep(2) + sw_dep(3) + sw_dep(4) + sw_dep(5)
     swc_40 = (( (sw(1) + sw(2) )/2) + sw(3) + sw(4) + sw(5))/4
     avgt   = (met.maxt + met.mint) / 2
     eta = ep + es
     eta_cum = eta_cum + eta
     etp_cum =etp_cum + eo
     if (lupin.phenologystage>=3 and lupin.phenologystage <= 9) then
        hi = yield / biomass
        hi_green = grainwt / abovegroundlivewt
        sla_tot = lai/leaflivewt
        yield_in_t = yield/1000
     else
        hi = 0
        hi_green = 0
        sla_tot = 0
        yield_in_t = 0
     endif
     if clock.today = clock.simulation_start_day then
        lai_yesterday = laitotal
     else
        lai_today = laitotal
        dlt_lai = (lai_today - lai_yesterday)
        if (dlt_lai <> 0) and (dlt_dm_green_leaf <> 0) then
           sla = ((dlt_lai )/dlt_dm_green_leaf)
        else
           sla = 0
        endif
        lai_yesterday = lai_today
     endif
     day_cnt = day_cnt + 1
     if (lupin.stagename = 'harvest_ripe' or lupin.plantstatus = 'dead') then
        lupin harvest
        lupin kill_crop
        lupin end_crop
        dateset = 0
     endif
     END of rules
     
