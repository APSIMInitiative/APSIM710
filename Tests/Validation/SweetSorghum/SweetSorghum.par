     endif


[all.manager.end_of_day]
     if sweetsorghum.PlantStatus = 'alive' then
        report do_output
     endif

     if sweetsorghum.StageName = 'harvest_ripe' or sweetsorghum.PlantStatus = 'dead' then
        sweetsorghum harvest
        sweetsorghum end_crop
     endif


