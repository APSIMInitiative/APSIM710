------- manager Initialisation ------------------------------------------------
     Manager rules:
     
     SECTION:- init
          dateset = 0
          emer_date = 0
          endjuv_date = 0
          anth_date = 0
          mat_date = 0
          event = 0
          cum_temp = 0
          cum_swdef = 0
          cum_days = 0
     
     SECTION:- start_of_day
        if (daysaftersowing = 1) then
           cum_temp = 0
           cum_swdef = 0
           cum_days = 0
        elseif (stagename = 'start_grain_fill' or
                      stagename = 'end_grain_fill' or
                      stagename = 'maturity' or
                      stagename = 'harvest_ripe') then
           cum_temp = cum_temp + (maxt + mint) / 2
           cum_swdef = cum_swdef + (1 - waterstressphoto)
           cum_days = cum_days + 1
        endif
        if (cum_days > 0) then
           avg_temp = cum_temp / cum_days
           avg_swdef = cum_swdef / cum_days
           if (avg_temp <= 14) then
              oil_conc =  47
           else
              oil_conc = (47 - ((avg_temp - 14)*1.21)) - (1.91*(1 - avg_swdef^2))
           endif
        else
           oil_conc = 0
        endif
     if (canola.stagename = 'harvest_ripe' or canola.plantstatus = 'dead') then
        canola harvest
        canola kill_crop
        canola end_crop
        dateset = 0
     endif
     END of rules
     
