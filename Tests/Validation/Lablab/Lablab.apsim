          <ui>
            <category description="Sowing criteria" />
            <date1 type="ddmmmdate" description="Enter sowing window START date (dd-mmm) : ">24-Jan-2000</date1>
            <date2 type="ddmmmdate" description="Enter sowing window END date (dd-mmm) : ">24-Jan-2000</date2>
            <must_sow type="yesno" description="Must sow? : ">yes</must_sow>
            <raincrit type="text" description="Amount of rainfall : ">30</raincrit>
            <rainnumdays type="text" description="Number of days of rainfall : ">3</rainnumdays>
            <esw_amount type="text" description="Enter minimum allowable available soil water (mm) : ">200</esw_amount>
            <category description="Sowing parameters" />
            <crop type="crop" description="Enter name of crop to sow : ">lablab</crop>
            <density type="text" description="Enter sowing density  (plants/m2) : ">10</density>
            <depth type="text" description="Enter sowing depth  (mm) : ">30</depth>
            <cultivar type="cultivars" description="Enter cultivar : ">highworth</cultivar>
            <class type="classes" description="Enter crop growth class : ">plant</class>
            <row_spacing type="text" description="Enter row spacing (mm) : ">500</row_spacing>
          </ui>
          <script name="start_of_day">
            <text><![CDATA[
      if (paddock_is_fallow() = 1 and FallowIn <> 'yes' and (NextCrop = 0 or NextCrop = '[crop]')) then
         if (date_within('[date1], [date2]') = 1) then
            if (rain[[rainnumdays]] >= [raincrit] AND esw >= [esw_amount]) OR
                ('[must_sow]' = 'yes' AND today = date('[date2]'))) THEN
               ChooseNextCrop = 'yes'   ! for rotations
               [crop] sow plants =[density], sowing_depth = [depth], cultivar = [cultivar], row_spacing = [row_spacing], crop_class = [class]
            endif
            if today = date('[date2]') then
               ChooseNextCrop = 'yes'
            endif
         endif
      endif
            ]]></text>
            <event>start_of_day</event>
          </script>
        </manager>
        <manager name="Harvesting rule">
          <ui>
            <category description="Harvesting criteria" />
            <crop type="crop" description="Enter name of crop to harvest when ripe : ">lablab</crop>
          </ui>
          <script name="end_of_day">
            <text>

           if [crop].StageName = 'harvest_ripe' or [crop].plant_status = 'dead' or day = 79 then
              [crop]  harvest
              [crop]  end_crop
           endif

            </text>
            <event>end_of_day</event>
          </script>
        </manager>
        <manager name="Manager">
          <script name="init">
            <text>cuts = 0
 total_minN = 0
biom = 0

green_leaves = 0

tt_ = 0
acc_TT = 0
</text>
            <event>init</event>
          </script>
          <script name="start_of_day">
            <text><![CDATA[ biom = leafgreenwt + stemgreenwt

tt_ = ((maxt+mint)/2) -10
acc_TT = tt_ + acc_tt
 if leaf_no >= 1 then
      green_leaves = leaf_no - leaf_no_sen 
 else
 endif

  if day = 358 and year = 2007 then        
      irrigation apply amount = 10
  endif

  if day = 362  and year = 2007 then        
      irrigation apply amount = 38
  endif

 
  if day = 97 and year = 2008 then        
      irrigation apply amount = 40
  endif

  if day = 9114 and year = 2008 then        
      irrigation apply amount = 50
  endif





]]></text>
            <event>start_of_day</event>
          </script>
          <script name="end_of_day">
            <text>  total_minN = total_minN + dlt_n_min_tot()
</text>
