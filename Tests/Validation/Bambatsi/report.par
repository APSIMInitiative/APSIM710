! ================================= P3 =====================================
[p3rep.report.parameters]
variable = clock.dd/mm/yyyy as date
variable = clock.day
variable = clock.year
variable = tracker1.sum@soilwat2_runoff
variable = tracker1.sum@soilwat2_drain
variable = tracker1.sum@soilwat2_es
variable = SoilWat.eo
variable = met.rain
variable = micromet.interception
variable = SoilN.nh4()
variable = SoilN.no3()
variable = tracker1.sum@soiln2_dnit[]
variable = tracker1.sum@soilwat2_flow_no3[16]
variable = manager.surf_biomn
variable = manager.subsurf_biomn
variable = SoilN.fom_n()
variable = manager.surf_humn
variable = manager.subsurf_humn
variable = surfaceom.surfaceom_wt
variable = surfaceom.surfaceom_cover
variable = SoilWat.cn2_new as curve_no
variable = bambatsi.lai as b_lai
variable = bambatsi.biomass as b_biomass
variable = bambatsi.biomass_n as b_biomass_n
variable = bambatsi.Fw as b_fw
variable = bambatsi.Fn as b_fn
variable = bambatsi.Fvpd as b_fvpd
variable = bambatsi.Ft as b_ft
variable = bambatsi.root_depth as b_root_depth
variable = bambatsi.ep as b_ep
variable = bambatsi.adm_sen()
variable = bambatsi.adm_green()
variable = bambatsi.adm_dead()
variable = bambatsi.foliage_mass
variable = bambatsi.foliage_mass_sen
variable = bambatsi.height as b_height
variable = manager.average_t
variable = met.maxt
variable = met.mint
variable = bambatsi.ffasw
variable = clock.month
variable = manager.sw1
variable = manager.sw2
variable = manager.sw3
variable = manager.swtot
variable = manager.cum_production
variable = manager.cum_runoff
variable = SoilWat.cn2_new
!variable = SoilWat.sw
!variable = SoilWat.sw_dep()



[p3_stdrep.report.parameters]
outputfile =p3.out
[p3_stdrep.SummaryFile.parameters]
summaryfile = p3.sum

[log.log.parameters]
debug_output = off
logfile=hudson.log
