   [EventHandler] public void OnPrepare()
   {
     
      for (int i = 0; i &lt; hdates.Length; i++)
      {         
         if (hdates[i] != null &amp;&amp; Today == hdates[i])   
         {         
            doHarvest();                
         }                    
      }
              
      DateTime hDate = new DateTime(2010, 8, 1);
      dayCount++;
      if (Today.CompareTo(hDate) &gt;= 0 &amp;&amp; dayCount &gt; 28 )
      {
         dayCount = 0;
         doHarvest();
      }
        
      //reset TT during winter dormancy
      if (Today.DayOfYear == 152 &amp;&amp; stage &gt;= 4.0)
      {         
         HarvestType hSet = new HarvestType();
         hSet.Remove = 0;   
         hSet.Height = 800;   
         //MyPaddock.Publish(Crop2Defoliate + ".harvest", hSet);
         
         resetPhenoByRemoval();
      }
         
      //tracing vernalisation
      DateTime sday = new DateTime(2008, 10, 23);
      if (Today &gt; sday)
      {
         if ( meant &gt;= 0 &amp;&amp; meant &lt; 5)
            vd = 1;
         else if (meant &gt; -5 &amp;&amp; meant &lt; 0)
            vd = (meant + 5) / 5;
         else if (meant &gt;= 5 &amp;&amp; meant &lt; 15) 
            vd = (15 - meant) / 10;
         else
            vd = 0;            
         vernalDays += vd;
      }
   }
   
   public void doHarvest()
   {
      MyPaddock.Get(Crop2Defoliate + ".leafgreenwt", out GreenLeaf);
      MyPaddock.Get(Crop2Defoliate + ".stemgreenwt", out GreenStem);
      

      dlt_GreenLeaf = Math.Max(0, GreenLeaf * (1.0 - ResidualLeaf));
      dlt_GreenStem = Math.Max(0, GreenStem * (1.0 - ResidualStem));
    
      RemoveCropBiomassdmType GreenRemoveCropDmDm = new RemoveCropBiomassdmType();
            
      GreenRemoveCropDmDm.pool = "green";
      GreenRemoveCropDmDm.part = new string[]
         {"leaf","stem"};
      GreenRemoveCropDmDm.dlt = new double[] 
         {dlt_GreenLeaf, dlt_GreenStem};
            
      RemoveCropBiomassType MyRemoveCropDM = new RemoveCropBiomassType();
           
      MyRemoveCropDM.dm = new RemoveCropBiomassdmType[] { GreenRemoveCropDmDm };            
       
      MyPaddock.Publish(Crop2Defoliate + ".remove_crop_biomass", MyRemoveCropDM);
            
      Console.WriteLine(Today.ToShortDateString() + " - " + "Plant1 Defoliation Rule Invoked");
      Console.WriteLine("          - " + Crop2Defoliate + " has been defoliated");
      Console.WriteLine("          - Removed " + dlt_GreenLeaf + " g/m2 of Leaf");
      Console.WriteLine("          - Removed " + dlt_GreenStem + " g/m2 of Stem");
      Console.WriteLine(" "); 
     
      DMharvestLeaf = dlt_GreenLeaf * 10;     //in kgDM/ha
      DMharvestStem = dlt_GreenStem * 10; 
      DMharvest = DMharvestLeaf + DMharvestStem;  
      
      //Reducing population 
      if (Today.CompareTo(popStart) &gt;= 0 &amp;&amp; Today.CompareTo(popEnd)&lt; 0 )
      {
         KillCropType reducePopn = new KillCropType();
         reducePopn.KillFraction = (float) (1.0 - popDecline);
         MyPaddock.Publish(Crop2Defoliate + ".kill_crop", reducePopn);
      }
   }
   
   public void  resetPhenoByRemoval()
   {
      MyPaddock.Get(Crop2Defoliate + ".leafgreenwt", out GreenLeaf);
      MyPaddock.Get(Crop2Defoliate + ".stemgreenwt", out GreenStem);
      
      dlt_GreenLeaf = Math.Max(0, GreenLeaf * 0.95);  //large removal for windback phenology
      dlt_GreenStem = Math.Max(0, GreenStem * 0.95);
      RemoveCropBiomassdmType GreenRemoveCropDmDm = new RemoveCropBiomassdmType();
            
      GreenRemoveCropDmDm.pool = "green";
      GreenRemoveCropDmDm.part = new string[] {"leaf","stem"};
      GreenRemoveCropDmDm.dlt = new double[]  {dlt_GreenLeaf, dlt_GreenStem};            
   
      RemoveCropBiomassType MyRemoveCropDM = new RemoveCropBiomassType();
      MyRemoveCropDM.dm = new RemoveCropBiomassdmType[] { GreenRemoveCropDmDm };            
      MyPaddock.Publish(Crop2Defoliate + ".remove_crop_biomass", MyRemoveCropDM);            
      Console.WriteLine(Today.ToShortDateString() + " - " + "To reset Pheno");
   }
   
   [EventHandler] public void OnPost()
   {
      DMharvestLeaf = 0.0;    //in kgDM/ha
      DMharvestStem = 0.0; 
      DMharvest = 0.0;  
          
   }
