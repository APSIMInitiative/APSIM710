        <manager name="Chicory Dormancy">
          <ui>
            <category type="category" description="Harvesting criteria" />
            <crop type="text" description="Enter name of crop to harvest when ripe : ">chicory</crop>
            <criticalMint type="text" description="cretical minimum T for darmancy">2</criticalMint>
            <Days type="text" description="number of days for averaging minimum temperature">10</Days>
          </ui>
          <script>
            <text>critT = [criticalmint]
accumDays = [days]

!Calculate creteria for dormancy        
if ([crop].plant_status = 'alive' and dormant = 0 and day &gt; 120 and day &lt; 240 and mint &lt; critT) then
   dayCount1 = dayCount1 + 1
   if (dayCount1 &lt; accumDays) then
       dormAccumT = dormAccumT + mint
   elseif (dayCount1 &gt; accumDays) then
       dormAccumT = (accumDays-1)/accumDays*dormAccumT + mint
   endif
endif

! Calculate creteria for ending dormancy
if ([crop].plant_status = 'alive' and dormant = 1 and day &gt; 180 and mint &gt;=critT ) then
   dayCount2 = dayCount2 + 1
   if (dayCount2 &lt; accumDays) then
       regrowAccumT = regrowAccumT + mint
   elseif (dayCount2 &gt; accumDays) then
       regrowAccumT = (accumDays-1)/accumDays*regrowAccumT + mint
   endif
endif

! Starting/Ending dormancy 
if (dayCount1 &gt;= accumDays and dormAccumT &lt; critT*accumDays) then
    chicory dormancy
!   reset TT              
    dormant = 1
    dayCount1 = 0
elseif (dayCount2 &gt;= accumDays and regrowAccumT &gt; critT*accumDays) then
   chicory spring
   dormant = 0
   dayCount2 = 0
endif</text>
            <event>end_of_day</event>
          </script>
          <script>
            <text>dayCount1 = 0
dayCount2 = 0
dormAccumT = 0
dormant = 0
regrowAccumT = 0
</text>
            <event>init</event>
          </script>
        </manager>
