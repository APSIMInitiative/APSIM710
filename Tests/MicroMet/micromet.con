version = 7.3
[apsim.sample_micromet]
title=Micromet Sample Simulation
module = Clock   micromet.par [sample] %apsim%/Model/Clock.xml [standard]
module = Report   micromet.par [sample]
module = Input(met)  %apsim%/Examples/MetFiles/Dalby.met [weather]
module = MicroMet micromet.par [sample] %apsim%/Model/MicroMet.xml[standard]
module = Manager  micromet.par [sample]
module = SoilWat   %apsim%/Examples/Soils/Soil.par [black_earth] %apsim%/Model/Soil.xml [SoilWat]
module = %apsim%/Model/Plant2.dll(grass)  Grass.xml [standard]
module = %apsim%/Model/Plant2.dll(tree)   Tree.xml [standard]
module = %apsim%/Model/Root.dll(GrassRoot)  micromet.par [sample] %apsim%/Model/Root.xml [standard]
module = %apsim%/Model/Root.dll(TreeRoot)  micromet.par [sample] %apsim%/Model/Root.xml [standard]