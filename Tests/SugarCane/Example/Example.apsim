            <NoRatoons type="text" description="Number of ratoons (max = 5): ">4</NoRatoons>
            <ratoonlen type="text" description="Duration of a ratoon crop (days) (30 days/month * 13 months) : ">390</ratoonlen>
            <ratoon_stalks type="text" description="Stalk density of ratoon crop (/m2) :">10</ratoon_stalks>
          </ui>
          <script>
            <text>
            ! -----------------------------------------------------------------
            ! Book keeping to round days to integer values
            plantlen = nearest_int(plantlen)
            ratoonlen = nearest_int(ratoonlen)

            ! Initialise and set a time counter for entire run
            dah = 0

            if (SugarCane.crop_status = 'alive') then
               dah = 0

               ! Set harvest dates for each crop phase
               if (SugarCane.DaysAfterSowing = [plantlen] and ratoon_no = 0) or
                  (SugarCane.DaysAfterSowing = [ratoonlen] and ratoon_no &gt; 0) then

                  SugarCane harvest

                  if (ratoon_no = [NoRatoons] + 1) then
                     ! plough the crop out
                     SugarCane kill_crop
                     SugarCane end_crop

                  else
                     ! Set stalk density
                     SugarCane.plants = [ratoon_stalks]
                  endif
               else
                  ! We have not reached the harvest date for this ratoon.
               endif

            elseif (SugarCane.crop_status = 'dead') then
               SugarCane harvest
               SugarCane end_crop

            elseif (SugarCane.crop_status = 'out') and (today = date('[planting_day]') then
               SugarCane sow plants = 10 (), cultivar = [variety], sowing_depth = [sowing_depth]
               SugarCane.plants = [plant_stalks]
               dah = 0

            else
               dah = dah + 1
            endif
               </text>
            <event>start_of_day</event>
          </script>
          <script>
            <text>
            nextcrop = 0
            fallowin = 0
            plantlen = 0
            ratoonlen = 0
            </text>
            <event>init</event>
          </script>
        </manager>
        <manager name="Fertilise on fixed date" shortcut="/simulations/Sugar/paddock/Manager folder/Fertilise on fixed date" />
      </folder>
      <outputfile shortcut="/simulations/Sugar/paddock/outputfile">
        <filename output="yes">Cane.out</filename>
        <title>Cane</title>
        <variables name="Variables" shortcut="/simulations/Sugar/paddock/outputfile/Variables" />
        <events name="Reporting Frequency" shortcut="/simulations/Sugar/paddock/outputfile/Reporting Frequency" />
      </outputfile>
    </area>
  </simulation>
</folder>