    <simulation name="Sim08-SoilCNPatch_Pasture+OrgN+MinN+Urea" shortcut="/SoilNitrogen_PatchTests/SoilCNPatch-init/Sim01-SoilCNPatch_Bare+NO3+NH4">
      <clock shortcut="/SoilNitrogen_PatchTests/SoilCNPatch-init/Sim01-SoilCNPatch_Bare+NO3+NH4/clock" />
      <metfile name="met" shortcut="/SoilNitrogen_PatchTests/SoilCNPatch-init/Sim01-SoilCNPatch_Bare+NO3+NH4/met" />
      <memo name="Info">
++++++++++++++++++  TEST 01 ++++++++++++++++++

   Description:
      - Artificial climate, smooth mild temperate;
      - Using a generic soil, medium texture free draining;
      - Pasture under cutting rotation;
      - Initial mineral N = 30kg as NO3 + 30kg as NH4;
      - Initial organic matter = about 125 tonnes hum, 10ton N;
      - Initial surface residues = 1000kg, 27kg N;
      - No additions of residues (besides crop senescence);
      - Apply 2×50kg urea_N;

      &gt; Paddock1 has 2 patches, exactly equal
      &gt; Paddock 2 has one patch only (the base)


   Expected output:
      - All outputs with valid values (numeric), including those related to plants (may differ between wheat and pasture);
      - No differences between the results from the two paddocks (very, very small diffs might happen due to rounding);
      - Include changes in plant variables;

+++++++++++++++++++++++++++++++++++++++++++++</memo>
      <summaryfile shortcut="/SoilNitrogen_PatchTests/SoilCNPatch-init/Sim01-SoilCNPatch_Bare+NO3+NH4/summaryfile" />
      <area name="Paddock1-wPatch" shortcut="/SoilNitrogen_PatchTests/SoilCNPatch-init/Sim01-SoilCNPatch_Bare+NO3+NH4/Paddock1-wPatch">
        <manager2 name="AddPatch" shortcut="/SoilNitrogen_PatchTests/SoilCNPatch-init/Sim02-SoilCNPatch_Bare+Hum+SurfOM/Paddock1-wPatch/AddPatch" />
        <manager name="CutGraze_AgPasture">
          <ui>
            <category type="category" description="Cut or graze rotation" />
            <CutPasture_start_date type="ddmmmdate" description="Enter date to start rotation (dd/mm/yyyy): ">01/01/1900</CutPasture_start_date>
            <CutPasture_end_date type="ddmmmdate" description="Enter date to end rotation (dd/mm/yyyy): ">31/12/2100</CutPasture_end_date>
            <AgPasture_type type="list" listvalues="AgPasture,ryegrass_clover,ryegrass,clover" description="Enter pasture type:">AgPasture</AgPasture_type>
            <rotation_type type="list" listvalues="Fixed intervals,Target mass" description="Enter the rotation type: ">Fixed intervals</rotation_type>
            <cut_interval type="text" description="Enter the interval between cuts/grazings (days):">21</cut_interval>
            <upper_threshold type="text" description="Enter the sward DM to trigger cut (kg/ha):">2000</upper_threshold>
            <lower_threshold type="text" description="Enter the residual DM after cut (kg/ha):">1500</lower_threshold>
            <category type="category" description="Management of cut/grazed material" />
            <treatment_type type="list" listvalues="Return as residue,Return as dung &amp; urine" description="Enter the treatment type for the cut/grazed material:">Return as residue</treatment_type>
            <remove_dm_frac type="text" description="Enter fraction of DM that is removed from field (0-1):">0.98</remove_dm_frac>
            <remove_n_frac type="text" description="Enter fraction of N that is removed from field (0-1):">0.98</remove_n_frac>
            <dung_fraction type="text" description="Enter proportion of N returned in dung (0-1):">0.0</dung_fraction>
          </ui>
          <script>
            <text>
! --&gt;   Last updated in: 11/July/2011  ..........
! . Author: Rogerio Cichota

days_after_cut = [cut_interval] - 1
Do_Cut = 'FALSE'
Has_Cut = 'FALSE'
harvested_dm =0
harvested_n = 0
removed_dm = 0.0
removed_n = 0.0
return_dm = 0.0
return_n = 0.0
GrowthRateCalc = 0
n_conc_total = 0
</text>
            <event>init</event>
          </script>
          <script>
            <text>if today &gt;= date('[CutPasture_start_date]') and today &lt;= date('[CutPasture_end_date]') then

   preharvest_dm = [AgPasture_type].StemWt + [AgPasture_type].LeafWt
   preharvest_n = [AgPasture_type].StemN + [AgPasture_type].LeafN
   harvested_dm = 0.0
   harvested_n = 0.0
   removed_dm = 0.0
   removed_n = 0.0
   return_dm = 0.0
   return_n = 0.0
   GrowthRateCalc = 0.0
   n_conc_removed = 0.0
   days_after_cut = days_after_cut + 1
   Has_Cut = 'FALSE'

   if '[rotation_type]' = 'Fixed intervals' then
      if days_after_cut = [cut_interval] then
         Do_Cut = 'TRUE'
      else
         Do_Cut = 'FALSE'
      endif
   elseif '[rotation_type]' = 'Target mass' then
      if preharvest_dm &gt;= [upper_threshold] then
         Do_Cut = 'TRUE'
      else
         Do_Cut = 'FALSE'
      endif
   endif

endif   ! of today

If Do_Cut = 'TRUE' then
   
!- Cut or graze ---------------------------------------------------------------------------------------

   if preharvest_dm &gt; [lower_threshold] then
      [AgPasture_type] graze type = 'residue', amount = [lower_threshold]
      Has_Cut = 'TRUE'
      harvested_dm = [AgPasture_type].HarvestWt
      harvested_n = [AgPasture_type].HarvestN
      GrowthRateCalc = harvested_dm/days_after_cut
      if harvested_dm &gt; 0 then
         n_conc_removed = harvested_n*100/harvested_dm
      endif
      days_after_cut = 0

!- Residue management -------------------------------------------------------------------------
      removed_dm =  harvested_dm* [remove_dm_frac]
      removed_n = harvested_n* [remove_n_frac]
      return_dm = harvested_dm - removed_dm 
      return_n = harvested_n - removed_n 

      if '[treatment_type]' = 'Return as residue' and [remove_dm_frac] &lt; 1 then
         'SurfaceOrganicMatter' add_surfaceom name = grass, type = grass, mass =  return_dm (kg/ha), n = return_n (kg/ha), cpr = 0
      elseif '[treatment_type]' = 'Return as dung &amp; urine' then
         ! It is advised that dung and urine return should be done using other manager, 'Urine_patch' or 'Apply or return N and water'
         N_on_dung = return_n * [dung_fraction]
         N_on_urine = return_n - N_on_dung
         'SurfaceOrganicMatter' add_surfaceom name = dung, type = RuminantDung_PastureFed, mass =  return_dm (kg/ha), n = N_on_dung (kg/ha), cpr = 0
         if N_on_urine &gt; 0 then
            fertiliser apply amount = N_on_urine (kg/ha), depth = 10 (mm), type = urea_n ()
         endif
      endif
   endif

endif  ! of Do_Cut
</text>
            <event>start_of_day</event>
          </script>
        </manager>
        <operations name="OperationsSchedule">
          <operation condition="start_of_day">
            <date>10-Jan</date>
            <action>Fertiliser apply amount=50 (kg/ha), depth=100 (mm), type=urea_n</action>
          </operation>
          <operation condition="start_of_day">
            <date>10-Feb</date>
            <action>Fertiliser apply amount=50 (kg/ha), depth=100 (mm), type=urea_n</action>
          </operation>
        </operations>
        <micromet name="Micromet">
          <soilalbedo type="text" description="soilalbedo">0.23</soilalbedo>
          <a_interception type="text" description="a_interception">0</a_interception>
          <b_interception type="text" description="b_interception">0</b_interception>
          <c_interception type="text" description="c_interception">0</c_interception>
          <d_interception type="text" description="d_interception">0</d_interception>
        </micromet>
        <fertiliser name="Fertiliser" shortcut="/SoilNitrogen_PatchTests/SoilCNPatch-init/Sim01-SoilCNPatch_Bare+NO3+NH4/Paddock1-wPatch/Fertiliser" />
        <surfaceom name="SurfaceOrganicMatter">
          <PoolName type="text" description="Organic Matter pool name">Residue1</PoolName>
