   &lt;Param&gt; Dim AddCNPatch_UreaN As Double          'Amount of urea N to add (to first layer for now)
   
   &lt;[Event]()&gt; Public Event AddSoilCNPatch As AddSoilCNPatchDelegate

   &lt;Input&gt; Dim Today As DateTime
   &lt;Input&gt; Dim dlayer As Double()

   Private Date2Add As DateTime
   Private Amount2Add As Double()
   
   &lt;EventHandler()&gt; Public Sub OnInitialised()
      
      Date2Add = DateTime.Parse(AddCNPatch_Date &amp; " 06:00:00 am")
      ReDim Amount2Add(dlayer.Length - 1)
      Amount2Add(0) = AddCNPatch_UreaN
      Console.WriteLine("   Initialised")
      
   End Sub

   &lt;EventHandler()&gt; Public Sub OnPrepare()

      If today.Date = Date2Add.Date Then

         Dim NewPatch As AddSoilCNPatchType = New AddSoilCNPatchType
         Dim AffectedPatches As Integer() = {0}
         
         NewPatch.Sender = "Manager"
         NewPatch.AreaFraction = AddCNPatch_AreaFraction
         NewPatch.DepositionType = "ToNewPatch"
         NewPatch.AffectedPatches_id = AffectedPatches   '  Add to 'base'
         NewPatch.Urea = Amount2Add
      
         RaiseEvent AddSoilCNPatch(NewPatch)
         
      End If
   End Sub

End Class
