      Array.Clear(dlt_n_min_tot_p2, 0, nLayers)
      Array.Clear(n2o_atm_p2, 0, nLayers)

      ' 2.Accumulate values
      
      es_p1 = SoilWs(0).es
      drain_p1 = SoilWs(0).drain
      leach_no3_p1 = SoilWs(0).Variable("leach_no3").ToSingle()      
      surfaceom_n_p1 = SurfOMs(0).surfaceom_n
      Try
         PlantN_p1 = Plants(0).totaln
      Catch e1 As Exception
         Try
            PlantN_p1 = Pastures(0).TotalPlantN
         Catch e2 As Exception
            'do nothing
         End Try
      End Try

      es_p2 = 0.5 * (SoilWs(1).es + SoilWs(2).es)
      drain_p2 = 0.5 * (SoilWs(1).drain + SoilWs(2).drain)
      surfaceom_n_p2 = 0.5 * (SurfOMs(1).surfaceom_n + SurfOMs(2).surfaceom_n)
      Try
         PlantN_p2 = 0.5 * (Plants(1).totaln + Plants(2).totaln)
      Catch e1 As Exception
         Try
            PlantN_p2 = 0.5 * (Pastures(1).TotalPlantN + Pastures(2).TotalPlantN)
         Catch e2 As Exception
            'do nothing
         End Try
      End Try
      leach_no3_p2 = 0.5 * (SoilWs(1).Variable("leach_no3").ToDouble + SoilWs(2).Variable("leach_no3").ToDouble)

      For z As Integer = 0 To nLayers - 1
         
         dlayer_ps(z) = SoilWs(0).dlayer(z)
         Try
            sw_uptake_p1(z) = Plants(0).sw_uptake(z)
         Catch e1 As Exception
            Try
               sw_uptake_p1(z) = Pastures(0).WaterUptakeLayers(z)
            Catch e2 As Exception
               'do nothing
            End Try
         End Try
         
         urea_p1(z) = SoilNs(0).urea(z)
         nh4_p1(z) = SoilNs(0).nh4(z)
         no3_p1(z) = SoilNs(0).no3(z)
         fom_n_p1(z) = SoilNs(0).fom_n(z)
         biom_n_p1(z) = SoilNs(0).biom_n(z)
         hum_n_p1(z) = SoilNs(0).hum_n(z)
         fom_c_p1(z) = SoilNs(0).fom_c(z)
         biom_c_p1(z) = SoilNs(0).biom_c(z)
         hum_c_p1(z) = SoilNs(0).hum_c(z)
         oc_p1(z) = SoilNs(0).oc(z)
         dlt_urea_hydrol_p1(z) = SoilNs(0).dlt_urea_hydrol(z)
         nitrification_p1(z) = SoilNs(0).nitrification(z)
         dnit_p1(z) = SoilNs(0).dnit(z)
         dlt_n_min_res_p1(z) = SoilNs(0).dlt_n_min_res(z)
         dlt_n_min_tot_p1(z) = SoilNs(0).dlt_n_min_tot(z)
         n2o_atm_p1(z) = SoilNs(0).n2o_atm(z)

         Try
            sw_uptake_p2(z) = 0.5 * (Plants(1).sw_uptake(z) + Plants(2).sw_uptake(z))
         Catch e1 As Exception
            Try
               sw_uptake_p1(z) = 0.5 * (Pastures(1).WaterUptakeLayers(z) + Pastures(2).WaterUptakeLayers(z))
            Catch e2 As Exception
               'do nothing
            End Try
         End Try

         urea_p2(z) = 0.5 * (SoilNs(1).urea(z) + SoilNs(2).urea(z))
         nh4_p2(z) = 0.5 * (SoilNs(1).nh4(z) + SoilNs(2).nh4(z))
         no3_p2(z) = 0.5 * (SoilNs(1).no3(z) + SoilNs(2).no3(z))
         fom_n_p2(z) = 0.5 * (SoilNs(1).fom_n(z) + SoilNs(2).fom_n(z))
         biom_n_p2(z) = 0.5 * (SoilNs(1).biom_n(z) + SoilNs(2).biom_n(z))
         hum_n_p2(z) = 0.5 * (SoilNs(1).hum_n(z) + SoilNs(2).hum_n(z))
         fom_c_p2(z) = 0.5 * (SoilNs(1).fom_c(z) + SoilNs(2).fom_c(z))
         biom_c_p2(z) = 0.5 * (SoilNs(1).biom_c(z) + SoilNs(2).biom_c(z))
         hum_c_p2(z) = 0.5 * (SoilNs(1).hum_c(z) + SoilNs(2).hum_c(z))
         oc_p2(z) = 0.5 * (SoilNs(1).oc(z) + SoilNs(2).oc(z))
         dlt_urea_hydrol_p2(z) = 0.5 * (SoilNs(1).dlt_urea_hydrol(z) + SoilNs(2).dlt_urea_hydrol(z))
         nitrification_p2(z) = 0.5 * (SoilNs(1).nitrification(z) + SoilNs(2).nitrification(z))
         dnit_p2(z) = 0.5 * (SoilNs(1).dnit(z) + SoilNs(2).dnit(z))
         dlt_n_min_res_p2(z) = 0.5 * (SoilNs(1).dlt_n_min_res(z) + SoilNs(2).dlt_n_min_res(z))
         dlt_n_min_tot_p2(z) = 0.5 * (SoilNs(1).dlt_n_min_tot(z) + SoilNs(2).dlt_n_min_tot(z))
         n2o_atm_p2(z) = 0.5 * (SoilNs(1).n2o_atm(z) + SoilNs(2).n2o_atm(z))
      Next

   End Sub
   
End Class
