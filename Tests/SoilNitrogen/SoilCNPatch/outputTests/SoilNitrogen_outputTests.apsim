            <text>
using System;
using ModelFramework;

public class Script 
{      

   [Param] DateTime AddPatch_DateToAdd;
   [Param] int AddPatch_NumAdditions;
   [Param] int AddPatch_Interval;
   [Param] string AddPatch_DepositionType;
   [Param] string AddPatch_PatchToAdd;
   [Param] double AddPatch_NewArea;
   [Param] string AddPatch_SoluteType;
   [Param] double AddPatch_SoluteAmount;
   [Param] string AddPatch_FOMtype;
   [Param] double AddPatch_FOMamount;
   [Param] double AddPatch_FOMcnr;
      
   [Link] private Paddock myPaddock;
   [Link] private Clock myClock;
   
   [Event] public event AddSoilCNPatchDelegate AddSoilCNPatch;
   [Event] public event AddSoilCNPatchwithFOMDelegate AddSoilCNPatchwithFOM;
   
   [Input] int PatchCount;
      
   [Output] string SoilName1;

   private double[] SoluteToAdd;
   private double[][] FOMCToAdd = new double[3][];
   private double[][] FOMNToAdd = new double[3][];

   private int[] PatchToAddTo;
   private int nPatches;
   private int nPatchesAdded;
   private int DaysAfterAdding;
   
   private string myFOMtype;
   private double[] FOMfraction = new double[3];
   
   [EventHandler] public void OnInitialised()
   {
      SoluteToAdd = new double[1];
      SoluteToAdd[0] = AddPatch_SoluteAmount;
      PatchToAddTo = new int[1];
      nPatchesAdded = 0;
      DaysAfterAdding = AddPatch_Interval;
      
      
      if (AddPatch_FOMtype == "Grass")
      {
         myFOMtype = "grass";
         FOMfraction[0] = 0.2;
         FOMfraction[1] = 0.7;
         FOMfraction[2] = 0.1;
      }
      else if (AddPatch_FOMtype == "Dung")
      {
         myFOMtype = "RuminantDung_PastureFed";
         FOMfraction[0] = 0.3;
         FOMfraction[1] = 0.5;
         FOMfraction[2] = 0.2;
      }
      else if (AddPatch_FOMtype == "Manure")
      {
         myFOMtype = "manure";
         FOMfraction[0] = 0.3;
         FOMfraction[1] = 0.3;
         FOMfraction[2] = 0.4;
      }
      else
      {
         myFOMtype = "none";
      }
      
      for (int pool = 0; pool &lt; 3; pool++)
      {
         FOMCToAdd[pool] = new double[1];
         FOMNToAdd[pool] = new double[1];
         FOMCToAdd[pool][0] = AddPatch_FOMamount * 0.40 * FOMfraction[pool];
         FOMNToAdd[pool][0] = FOMCToAdd[pool][0] / AddPatch_FOMcnr;
      }
      
      
      Console.WriteLine();
      Console.WriteLine("     Addition of CNPatches initialised");
      
   }

   [EventHandler] public void OnPrepare()
   {
      if(myClock.Today &gt;= AddPatch_DateToAdd.Date)
      {
         if ((DaysAfterAdding == AddPatch_Interval) &amp;&amp; (nPatchesAdded &lt; AddPatch_NumAdditions))
         {
            if (myFOMtype == "none")
            {
               AddSoilCNPatchType NewPatch = new AddSoilCNPatchType();
               NewPatch.Sender = "manager";
               NewPatch.DepositionType = AddPatch_DepositionType;
               NewPatch.AreaFraction = AddPatch_NewArea;
               if (AddPatch_PatchToAdd == "base")
                  PatchToAddTo[0] = 0;
               else if (AddPatch_PatchToAdd == "last")
                  PatchToAddTo[0] = PatchCount - 1;
               NewPatch.AffectedPatches_id = PatchToAddTo;
               if (AddPatch_SoluteType == "Urea")
                  NewPatch.Urea = SoluteToAdd;
               if (AddPatch_SoluteType == "NH4")
                  NewPatch.NH4 = SoluteToAdd;
               if (AddPatch_SoluteType == "NO3")
                  NewPatch.NO3 = SoluteToAdd;
      
               AddSoilCNPatch.Invoke(NewPatch);
            }
            else
            {
               AddSoilCNPatchwithFOMType NewPatch = new AddSoilCNPatchwithFOMType();
               NewPatch.Sender = "manager";
               NewPatch.DepositionType = AddPatch_DepositionType;
               NewPatch.AreaNewPatch = AddPatch_NewArea;
               if (AddPatch_PatchToAdd == "base")
                  PatchToAddTo[0] = 0;
               else if (AddPatch_PatchToAdd == "last")
                  PatchToAddTo[0] = PatchCount - 1;
               NewPatch.AffectedPatches_id = PatchToAddTo;
               if (AddPatch_SoluteType == "Urea")
                  NewPatch.Urea = SoluteToAdd;
               if (AddPatch_SoluteType == "NH4")
                  NewPatch.NH4 = SoluteToAdd;
               if (AddPatch_SoluteType == "NO3")
                  NewPatch.NO3 = SoluteToAdd;
      
               NewPatch.FOM = new AddSoilCNPatchwithFOMFOMType();
               NewPatch.FOM.Pool = new SoilOrganicMaterialType[3];
               for (int pool = 0; pool &lt; 3; pool++)
               {
                  NewPatch.FOM.Pool[pool] = new SoilOrganicMaterialType();
                  NewPatch.FOM.Pool[pool].C = FOMCToAdd[pool];
                  NewPatch.FOM.Pool[pool].N = FOMNToAdd[pool];
               }
               
                  
               AddSoilCNPatchwithFOM.Invoke(NewPatch);
            }
            nPatchesAdded += 1;
            DaysAfterAdding = 1;
         }
         else
            DaysAfterAdding += 1;
      }
   }
      
   [EventHandler] public void OnPost()
   {

   }
}
