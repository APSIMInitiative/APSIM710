               patch01NH4[z] = myPatchNH4.Patch[k].Value[z];
               patch01NO3[z] = myPatchNO3.Patch[k].Value[z];
               patch01FOMN[z] = myPatchFOMN.Patch[k].Pool[0].Value[z] + myPatchFOMN.Patch[k].Pool[1].Value[z] + myPatchFOMN.Patch[k].Pool[2].Value[z];
               patch01BiomN[z] = myPatchBiomN.Patch[k].Value[z];
               patch01HumN[z] = myPatchHumN.Patch[k].Value[z];

               patch01Hydrolysis[z] = myPatchHydrolysis.Patch[k].Value[z];
               patch01Nitrification[z] = myPatchNitrification.Patch[k].Value[z];
               patch01Denitrification[z] = myPatchDenitrification.Patch[k].Value[z];
               patch01NMinerResidues[z] = myPatchNMinerResidues.Patch[k].Value[z];
               patch01NMinerSoilOM[z] = myPatchNMinerFOM.Patch[k].Value[z] + myPatchNMinerBiomass.Patch[k].Value[z] + myPatchNMinerHumus.Patch[k].Value[z];
               patch01N2Oatm[z] = myPatchN2Oatm.Patch[k].Value[z];

               patch01FOMC[z] = myPatchFOMC.Patch[k].Pool[0].Value[z] + myPatchFOMC.Patch[k].Pool[1].Value[z] + myPatchFOMC.Patch[k].Pool[2].Value[z];
               patch01BiomC[z] = myPatchBiomC.Patch[k].Value[z];
               patch01HumC[z] = myPatchHumC.Patch[k].Value[z];

               patch01CO2Residues[z] = myPatchCO2Residues.Patch[k].Value[z];
               patch01CO2FOM[z] = myPatchCO2FOM.Patch[k].Value[z];
               patch01CO2Biomass[z] = myPatchCO2Biomass.Patch[k].Value[z];
               patch01CO2Humus[z] = myPatchCO2Humus.Patch[k].Value[z];
            }
         }
         else if (k == 1)
         {
            if (k &lt; myPatchCount)
            {
               patchesName[k] = myPatchName[k];
               patchesArea[k] = myPatchArea[k];
               patchesNinSediment[k] = CNPatchNinSediment[k];
               patchesCinSediment[k] = CNPatchCinSediment[k];
               for (int z = 0; z &lt; nLayers; z++)
               {
                  patch02Urea[z] = myPatchUrea.Patch[k].Value[z];
                  patch02NH4[z] = myPatchNH4.Patch[k].Value[z];
                  patch02NO3[z] = myPatchNO3.Patch[k].Value[z];
                  patch02FOMN[z] = myPatchFOMN.Patch[k].Pool[0].Value[z] + myPatchFOMN.Patch[k].Pool[1].Value[z] + myPatchFOMN.Patch[k].Pool[2].Value[z];
                  patch02BiomN[z] = myPatchBiomN.Patch[k].Value[z];
                  patch02HumN[z] = myPatchHumN.Patch[k].Value[z];

                  patch02Hydrolysis[z] = myPatchHydrolysis.Patch[k].Value[z];
                  patch02Nitrification[z] = myPatchNitrification.Patch[k].Value[z];
                  patch02Denitrification[z] = myPatchDenitrification.Patch[k].Value[z];
                  patch02NMinerResidues[z] = myPatchNMinerResidues.Patch[k].Value[z];
                  patch02NMinerSoilOM[z] = myPatchNMinerFOM.Patch[k].Value[z]+myPatchNMinerBiomass.Patch[k].Value[z]+myPatchNMinerHumus.Patch[k].Value[z];
                  patch02N2Oatm[z] = myPatchN2Oatm.Patch[k].Value[z];

                  patch02FOMC[z] = myPatchFOMC.Patch[k].Pool[0].Value[z] + myPatchFOMC.Patch[k].Pool[1].Value[z] + myPatchFOMC.Patch[k].Pool[2].Value[z];
                  patch02BiomC[z] = myPatchBiomC.Patch[k].Value[z];
                  patch02HumC[z] = myPatchHumC.Patch[k].Value[z];

                  patch02CO2Residues[z] = myPatchCO2Residues.Patch[k].Value[z];
                  patch02CO2FOM[z] = myPatchCO2FOM.Patch[k].Value[z];
                  patch02CO2Biomass[z] = myPatchCO2Biomass.Patch[k].Value[z];
                  patch02CO2Humus[z] = myPatchCO2Humus.Patch[k].Value[z];
               }
            }
            else
            {
               // clear the outputs
               patchesName[k] = "noPatch";
               patchesArea[k] = 0.0;
               patchesNinSediment[k] = 0.0;
               patchesCinSediment[k] = 0.0;

               patch02Urea = new double[nLayers];
               patch02NH4 = new double[nLayers];
               patch02NO3 = new double[nLayers];
               patch02FOMN = new double[nLayers];
               patch02BiomN = new double[nLayers];
