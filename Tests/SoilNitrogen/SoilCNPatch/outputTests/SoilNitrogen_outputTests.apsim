   [EventHandler] public void OnPost()
   {
     
      mySoilN.Get("PatchCount", out myPatchCount);
      mySoilN.Get("PatchName", out myPatchName);
      mySoilN.Get("PatchArea", out myPatchArea);
      
      // get the values from SoilNitrogen
      mySoilN.Get("PatchNLostInSediment", out CNPatchNinSediment);
      mySoilN.Get("PatchCLostInSediment", out CNPatchCinSediment);
      mySoilN.GetObject("PatchUrea", ref myPatchUrea);
      mySoilN.GetObject("PatchUrea", ref myPatchUrea);
      mySoilN.GetObject("PatchNH4", ref myPatchNH4);
      mySoilN.GetObject("PatchNO3", ref myPatchNO3);
      mySoilN.GetObject("PatchFOM_N", ref myPatchFOMN);
      mySoilN.GetObject("PatchBiom_N", ref myPatchBiomN);
      mySoilN.GetObject("PatchHum_N", ref myPatchHumN);

      mySoilN.GetObject("PatchDltUreaHydrolysis", ref myPatchHydrolysis);
      mySoilN.GetObject("PatchDltNitrification", ref myPatchNitrification);
      mySoilN.GetObject("Patch_dnit", ref myPatchDenitrification);
      mySoilN.GetObject("PatchNMineralisedFromResidues", ref myPatchNMinerResidues);
      mySoilN.GetObject("PatchNMineralisedFromFOM", ref myPatchNMinerFOM);
      mySoilN.GetObject("PatchNMineralisedFromMBiomass", ref myPatchNMinerBiomass);
      mySoilN.GetObject("PatchNMineralisedFromHumus", ref myPatchNMinerHumus);
      mySoilN.GetObject("PatchN2OLostToAtmosphere", ref myPatchN2Oatm);

      mySoilN.GetObject("PatchFOM_C", ref myPatchFOMC);
      mySoilN.GetObject("PatchBiom_C", ref myPatchBiomC);
      mySoilN.GetObject("PatchHum_C", ref myPatchHumC);

      mySoilN.GetObject("PatchDltCFromResiduesToAtmosphere", ref myPatchCO2Residues);
      mySoilN.GetObject("PatchDltCFromFOMToAtmosphere", ref myPatchCO2FOM);
      mySoilN.GetObject("PatchDltCFromMBiomassToAtmosphere", ref myPatchCO2Biomass);
      mySoilN.GetObject("PatchDltCFromHumusToAtmosphere", ref myPatchCO2Humus);

      // copy the relevant values
      for(int k = 0; k &lt; nPatchesAll;k++)
      {
         if (k == 0)
         {
            patchesName[k] = myPatchName[k];
            patchesArea[k] = myPatchArea[k];
            patchesNinSediment[k] = CNPatchNinSediment[k];
            patchesCinSediment[k] = CNPatchCinSediment[k];
            for (int z = 0; z &lt; nLayers; z++)
