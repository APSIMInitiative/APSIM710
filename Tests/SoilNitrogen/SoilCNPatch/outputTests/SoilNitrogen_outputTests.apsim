   [EventHandler] public void OnInitialised()
   {
      nPatchesAll = 2;
      nLayers = 6;
      patch01ResultName = "base";
      patch02ResultName = "patch";

      // get the link to SoilN
      foreach(Paddock myPaddock in myBlock.ChildPaddocks)
         foreach(Component Module in myPaddock.Children)
            if(Module.Name.Contains(" Nitrogen"))
            {
               mySoilName = Module.Name.Split(' ')[0];
               mySoilN = (SoilN) myPaddock.LinkByType("SoilN");
            }
           
      Console.WriteLine();
      Console.WriteLine("     Handling variables from SoilCNPatches, with " + nPatchesAll + " patches");
      
      // clear and initialise the outputs
      patchesName = new string[nPatchesAll];
      patchesArea = new double[nPatchesAll];
      patchesNinSediment = new double[nPatchesAll];
      patchesCinSediment = new double[nPatchesAll];

      // patch base
