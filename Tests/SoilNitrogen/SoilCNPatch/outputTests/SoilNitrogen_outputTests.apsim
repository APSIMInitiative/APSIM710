          <Pdk01Area type="text" description=" Relative area of first paddock [0-1]:">0.75</Pdk01Area>
          <Pdk02Name type="modulename" description=" Name of second paddock:">Paddock1-patch-LightSoil</Pdk02Name>
          <Pdk02Area type="text" description=" Relative area of second paddock [0-1]:">0.25</Pdk02Area>
        </ui>
        <text>
using System;
using ModelFramework;

public class Script 
{     

   [Param] private string Pdk01Name;
   [Param] private double Pdk01Area;
   [Param] private string Pdk02Name;
   [Param] private double Pdk02Area;
   
   [Link] Simulation myBlock;
   
   [Output][Units("mm")] public double[] dlayer_p1;
   [Output][Units("mm")] public double es_p1;
   [Output][Units("mm")] public double[] sw_uptake_p1;
   [Output][Units("mm")] public double drain_p1;
   [Output][Units("mm")] public double runoff_p1;
   [Output][Units("mm")] public double pond_p1;

   [Output][Units("kg/ha")] public double[] urea_p1;
   [Output][Units("kg/ha")] public double[] nh4_p1;
   [Output][Units("kg/ha")] public double[] no3_p1;
   [Output][Units("kg/ha")] public double[] fom_n_p1;
   [Output][Units("kg/ha")] public double[] biom_n_p1;
   [Output][Units("kg/ha")] public double[] hum_n_p1;
   [Output][Units("kg/ha")] public double surfaceom_n_p1;
   [Output][Units("kg/ha")] public double PlantN_p1;
   [Output][Units("kg/ha")] public double PlantNuptake_p1;
   [Output][Units("kg/ha")] public double Fertiliser_p1;

   [Output][Units("kg/ha")] public double[] fom_c_p1;
   [Output][Units("kg/ha")] public double[] biom_c_p1;
   [Output][Units("kg/ha")] public double[] hum_c_p1;
   [Output][Units("%")] public double[] oc_p1;
   [Output][Units("kg/ha")] public double surfaceom_c_p1;
   [Output][Units("kg/ha")] public double PlantC_p1;

   [Output][Units("kg/ha")] public double leach_no3_p1;
   [Output][Units("kg")] public double dlt_n_loss_in_sed_p1;
   [Output][Units("kg/ha")] public double[] dlt_urea_hydrol_p1;
   [Output][Units("kg/ha")] public double[] nitrification_p1;
   [Output][Units("kg/ha")] public double[] dnit_p1;
   [Output][Units("kg/ha")] public double[] dlt_n_min_res_p1;
   [Output][Units("kg/ha")] public double[] dlt_n_min_tot_p1;
   [Output][Units("kg/ha")] public double[] n2o_atm_p1;

   [Output][Units("kg/ha")] public double dlt_res_c_atm_p1;
   [Output][Units("kg/ha")] public double[] dlt_fom_c_atm_p1;
   [Output][Units("kg/ha")] public double[] dlt_biom_c_atm_p1;
   [Output][Units("kg/ha")] public double[] dlt_hum_c_atm_p1;
   [Output][Units("kg")] public double dlt_c_loss_in_sed_p1;
     
   [Output][Units("")] public string SoilName_p1;
   [Output][Units("")] string ResultName_p1;
   
   [Output] string SoilName2;

   private int nLayers;
   private SoilN[] mySoilN;
   private SoilWat[] mySoilW;
   private SurfaceOM[] mySurfOM;
   private Wheat[] myPlant;
   private AgPasture[] myPasture;
   private Fertiliser[] myFert;
   private double[] RelativeArea;
      
   [EventHandler] public void OnInitialised()
   {
      // basic input parameters
      nLayers = 6;
      ResultName_p1 = "Paddock";
      string[] PaddockNames = new string[2];
      PaddockNames[0] = Pdk01Name;
      PaddockNames[1] = Pdk02Name;
      RelativeArea = new double[2];
      RelativeArea[0] = Pdk01Area;
      RelativeArea[1] = Pdk02Area;

      // link to various modules
      mySoilN = new SoilN[2];
      mySoilW = new SoilWat[2];
      mySurfOM = new SurfaceOM[2];
      myPlant = new Wheat[2];
      myPasture = new AgPasture[2];
      myFert = new Fertiliser[2];

      Console.WriteLine("     Initialising");
      int pdk = -1;
      int PdkFound = 0;
      foreach(Paddock ThisPaddock in myBlock.ChildPaddocks)
      {
         if(ThisPaddock.Name == PaddockNames[0])
         {
            pdk = 0;
            PdkFound += 1;
            foreach(Component myModule in ThisPaddock.Children)
            {
               if(myModule.Name.Contains(" Nitrogen"))
               {
                  SoilName_p1 = myModule.Name.Split(' ')[0];
               }
            }
         }
         else if(ThisPaddock.Name == PaddockNames[1])
         {
            pdk = 1;
            PdkFound += 1;
         }

         if(pdk &gt;= 0 &amp;&amp; PdkFound &lt;= 2)
         {
            mySoilN[pdk] = (SoilN) ThisPaddock.LinkByType("SoilN");
            mySoilW[pdk] = (SoilWat) ThisPaddock.LinkByType("SoilWat");
            mySurfOM[pdk] = (SurfaceOM) ThisPaddock.LinkByType("SurfaceOM");
            myPlant[pdk] = (Wheat) ThisPaddock.LinkByName("wheat");
            myPasture[pdk] = (AgPasture) ThisPaddock.LinkByType("agpasture");
            myFert[pdk] = (Fertiliser) ThisPaddock.LinkByType("Fertiliser");
            if(PdkFound == 2)
               PdkFound += 1;
         }
      }
      if (PdkFound &lt; 2)
      {
         throw new Exception("Could not find the two paddocks to sum up");
      }
      else
      {  
         Console.WriteLine("        Summing up paddocks:");
         for(int i = 0;i&lt;2;i++)
            Console.WriteLine("         ." + mySoilN[i].FullName.Split('.')[0] + ", with area: " + RelativeArea[i]);
         
         // initialise the outputs
         dlayer_p1 = new double[nLayers];
         es_p1 = 0.0;
         sw_uptake_p1 = new double[nLayers];
         drain_p1 = 0.0;
         runoff_p1 = 0.0;
         pond_p1 = 0.0;

         urea_p1 = new double[nLayers];
         nh4_p1 = new double[nLayers];
         no3_p1 = new double[nLayers];
         fom_n_p1 = new double[nLayers];
         biom_n_p1 = new double[nLayers];
         hum_n_p1 = new double[nLayers];
         surfaceom_n_p1 = 0.0;
         PlantN_p1 = 0.0;
         PlantNuptake_p1 = 0;
         Fertiliser_p1 = 0.0;

         fom_c_p1 = new double[nLayers];
         biom_c_p1 = new double[nLayers];
         hum_c_p1 = new double[nLayers];
         oc_p1 = new double[nLayers];
         surfaceom_c_p1 = 0.0;
         PlantC_p1 = 0.0;

         leach_no3_p1 = 0.0;
         dlt_n_loss_in_sed_p1 = 0.0;
         dlt_urea_hydrol_p1 = new double[nLayers];
         nitrification_p1 = new double[nLayers];
         dnit_p1 = new double[nLayers];
         dlt_n_min_res_p1 = new double[nLayers];
         dlt_n_min_tot_p1 = new double[nLayers];
         n2o_atm_p1 = new double[nLayers];

         dlt_res_c_atm_p1 = 0.0;
         dlt_fom_c_atm_p1 = new double[nLayers];
         dlt_biom_c_atm_p1 = new double[nLayers];
         dlt_hum_c_atm_p1 = new double[nLayers];
         dlt_c_loss_in_sed_p1 = 0.0;
      }
   }
   
   [EventHandler] public void OnPost()
   {
      // clear outputs
      es_p1 = 0.0;
      drain_p1 = 0.0;
      runoff_p1 = 0.0;
      pond_p1 = 0.0;

      surfaceom_n_p1 = 0.0;
      PlantN_p1 = 0.0;
      PlantNuptake_p1 = 0;
      Fertiliser_p1 = 0.0;
      leach_no3_p1 = 0.0;
      dlt_n_loss_in_sed_p1 = 0.0;

      surfaceom_c_p1 = 0.0;
      dlt_res_c_atm_p1 = 0.0;
      PlantC_p1 = 0.0;
      dlt_c_loss_in_sed_p1 = 0.0;

      Array.Clear(dlayer_p1, 0, nLayers);
      Array.Clear(sw_uptake_p1, 0, nLayers);

      Array.Clear(urea_p1, 0, nLayers);
      Array.Clear(nh4_p1, 0, nLayers);
      Array.Clear(no3_p1, 0, nLayers);
      Array.Clear(fom_n_p1, 0, nLayers);
      Array.Clear(biom_n_p1, 0, nLayers);
      Array.Clear(hum_n_p1, 0, nLayers);

      Array.Clear(fom_c_p1, 0, nLayers);
      Array.Clear(biom_c_p1, 0, nLayers);
      Array.Clear(hum_c_p1, 0, nLayers);
      Array.Clear(oc_p1, 0, nLayers);

      Array.Clear(dlt_urea_hydrol_p1, 0, nLayers);
      Array.Clear(nitrification_p1, 0, nLayers);
      Array.Clear(dnit_p1, 0, nLayers);
      Array.Clear(dlt_n_min_res_p1, 0, nLayers);
      Array.Clear(dlt_n_min_tot_p1, 0, nLayers);
      Array.Clear(n2o_atm_p1, 0, nLayers);

      Array.Clear(dlt_fom_c_atm_p1, 0, nLayers);
      Array.Clear(dlt_biom_c_atm_p1, 0, nLayers);
      Array.Clear(dlt_hum_c_atm_p1, 0, nLayers);
      
      // Sum up the outputs
      for (int pdk = 0;pdk &lt; mySoilN.Length;pdk++)
      {
         es_p1 += mySoilW[pdk].es * RelativeArea[pdk];
         drain_p1 += mySoilW[pdk].drain * RelativeArea[pdk];
         runoff_p1 = mySoilW[pdk].runoff * RelativeArea[pdk];
         pond_p1 = mySoilW[pdk].pond * RelativeArea[pdk];
         leach_no3_p1 += mySoilW[pdk].Variable("leach_no3").ToDouble() * RelativeArea[pdk];
         surfaceom_n_p1 += mySurfOM[pdk].surfaceom_n * RelativeArea[pdk];
         Fertiliser_p1 += myFert[pdk].fertiliser * RelativeArea[pdk];
         dlt_n_loss_in_sed_p1 += mySoilN[pdk].dlt_n_loss_in_sed * RelativeArea[pdk];
         surfaceom_c_p1 += mySurfOM[pdk].surfaceom_c * RelativeArea[pdk];
         dlt_res_c_atm_p1 += mySoilN[pdk].dlt_res_c_atm * RelativeArea[pdk];
         dlt_c_loss_in_sed_p1 += mySoilN[pdk].dlt_c_loss_in_sed * RelativeArea[pdk];
         
         try
         {
            PlantNuptake_p1 += myPlant[pdk].n_uptake * RelativeArea[pdk];
            PlantN_p1 += myPlant[pdk].TotalN * RelativeArea[pdk];
            PlantC_p1 += 0.4 * myPlant[pdk].TotalWt * RelativeArea[pdk];
         }
         catch(System.Exception err)
         {
            try
            {
               PlantNuptake_p1 += myPasture[pdk].NitrogenUptake * RelativeArea[pdk];
               PlantN_p1 += myPasture[pdk].TotalPlantN * RelativeArea[pdk];
               PlantC_p1 += 0.4 * myPasture[pdk].TotalPlantWt * RelativeArea[pdk];
            }
            catch(System.Exception err2)
            {}
         }
                     
         for (int layer = 0; layer &lt; nLayers;layer++)
         {
            dlayer_p1[layer] += mySoilW[pdk].dlayer[layer] * RelativeArea[pdk];
            try
               {sw_uptake_p1[layer] += myPlant[pdk].sw_uptake[layer] * RelativeArea[pdk];}
            catch(System.Exception err)
            {
               try
                  {sw_uptake_p1[layer] += myPasture[pdk].WaterUptakeLayers[layer] * RelativeArea[pdk];}
               catch(System.Exception err1)
               {}
            }

            urea_p1[layer] += mySoilN[pdk].urea[layer] * RelativeArea[pdk];
            nh4_p1[layer] += mySoilN[pdk].nh4[layer] * RelativeArea[pdk];
            no3_p1[layer] += mySoilN[pdk].no3[layer] * RelativeArea[pdk];
            fom_n_p1[layer] += mySoilN[pdk].fom_n[layer] * RelativeArea[pdk];
            biom_n_p1[layer] += mySoilN[pdk].biom_n[layer] * RelativeArea[pdk];
            hum_n_p1[layer] += mySoilN[pdk].hum_n[layer] * RelativeArea[pdk];

            fom_c_p1[layer] += mySoilN[pdk].fom_c[layer] * RelativeArea[pdk];
            biom_c_p1[layer] += mySoilN[pdk].biom_c[layer] * RelativeArea[pdk];
            hum_c_p1[layer] += mySoilN[pdk].hum_c[layer] * RelativeArea[pdk];
            oc_p1[layer] += mySoilN[pdk].oc[layer] * RelativeArea[pdk];

            dlt_urea_hydrol_p1[layer] += mySoilN[pdk].dlt_urea_hydrol[layer] * RelativeArea[pdk];
            nitrification_p1[layer] += mySoilN[pdk].nitrification[layer] * RelativeArea[pdk];
            dnit_p1[layer] += mySoilN[pdk].dnit[layer] * RelativeArea[pdk];
            dlt_n_min_res_p1[layer] += mySoilN[pdk].dlt_n_min_res[layer] * RelativeArea[pdk];
            dlt_n_min_tot_p1[layer] += mySoilN[pdk].dlt_n_min_tot[layer] * RelativeArea[pdk];
            n2o_atm_p1[layer] += mySoilN[pdk].n2o_atm[layer] * RelativeArea[pdk];
            
            dlt_fom_c_atm_p1[layer] += mySoilN[pdk].dlt_fom_c_atm[layer] * RelativeArea[pdk];
            dlt_biom_c_atm_p1[layer] += mySoilN[pdk].dlt_biom_c_atm[layer] * RelativeArea[pdk];
            dlt_hum_c_atm_p1[layer] += mySoilN[pdk].dlt_hum_c_atm[layer] * RelativeArea[pdk];
         }
      }
   }
}
