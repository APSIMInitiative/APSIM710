    <memo name="Info">
+++++++++++++++++++++  TEST init  +++++++++++++++++++++

   These simulations are for testing the initialisation/creation 
of patches within SoilNitrogen.  The patches do not vary, i.e.
all patches are identical and thus their presence should make
no difference to the simulation (compared to having no patches)

++++++++++++++++++++++++++++++++++++++++++++++++++++</memo>
    <simulation name="Sim01-SoilCNPatch - Bare+NO3+NH4">
      <clock>
        <start_date type="date" description="start_date">01/01/2000</start_date>
        <end_date type="date" description="end_date">15/02/2000</end_date>
      </clock>
      <metfile name="met">
        <filename input="yes">%apsim%\Examples\MetFiles\MildTemperate.met</filename>
      </metfile>
      <memo name="Info">
++++++++++++++++++  TEST 01 ++++++++++++++++++

   Description:
      - Stable, artificial climate;
      - A clayey slow draining soil;
      - Bare soil;
      - Initial mineral N = 60kg as NO3 + 60kg as NH4;
      - No initial organic matter;
      - No initial surface residues;
      - No additions of residues or mineral N;

      &gt; Paddock1 has three patches, identical, added in sequence
      &gt; Paddock 2 has no patch

+++++++++++++++++++++++++++++++++++++++++++++</memo>
      <summaryfile />
      <area name="Paddock1-wPatch">
        <manager2 name="AddPatch">
          <ui>
            <DayToAdd type="ddmmmdate" description="Day of month to add patch">5</DayToAdd>
            <PatchArea type="text" description="Relative area of patch (0-1)">0.5</PatchArea>
            <UreaN type="text" description="Urea amount to add (kgN/ha)">0</UreaN>
          </ui>
          <text>
Imports System
Imports ModelFramework

Public Class Script 
   
   &lt;Param&gt; Dim DayToAdd As Integer       'Date to add urine patch
   &lt;Param&gt; Dim PatchArea As Double     'Relative area of patch (0-1)
   &lt;Param&gt; Dim UreaN As Double         'Amount of urea N to add (to first layer for now)
   
   &lt;[Event]()&gt; Public Event AddSoilCNPatch As AddSoilCNPatchDelegate

   &lt;Input&gt; Dim Today As DateTime

   Private Date2Add As DateTime
   
   &lt;EventHandler()&gt; Public Sub OnInitialised()
      
      ' Date2Add = DateTime.Parse(AddDate &amp; " 06:00:00 am")
      Console.WriteLine("   Initialised")
      
   End Sub

   &lt;EventHandler()&gt; Public Sub OnPrepare()

      If today.Date.Day = DayToAdd Then
         
         Dim NewPatch As AddSoilCNPatchType = New AddSoilCNPatchType
         Dim myUrea() As Double = { UreaN }
         NewPatch.AreaFraction = PatchArea
         NewPatch.Urea = myUrea
      
         RaiseEvent AddSoilCNPatch(NewPatch)
         
      End If
   End Sub

End Class
   </text>
