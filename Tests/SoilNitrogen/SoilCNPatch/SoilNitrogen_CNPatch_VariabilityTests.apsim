using System;
using ModelFramework;

public class Script 
{      

   [Link] Paddock myPaddock;
   
   [Input(IsOptional=true)] double[] n_demanded;
   [Input(IsOptional=true)] double n_uptake;
   [Input(IsOptional=true)] double totaln;
   [Input(IsOptional=true)] double totalwt;
   [Input(IsOptional=true)] double ep;

   [Input] double[] dlayer;
   
   [Input(IsOptional=true)] double[] NitrogenSupplyLayers;
   [Input(IsOptional=true)] double NitrogenUptake;
   [Input(IsOptional=true)] double TotalPlantN;
   [Input(IsOptional=true)] double TotalPlantWt;
   [Input(IsOptional=true)] double WaterUptake;
      
   [Output][Units("")] string SoilName;
   [Output][Units("kg/ha")] double PlantNUptake;
   [Output][Units("kg/ha")] double PlantTotalN;
   [Output][Units("kg/ha")] double PlantTotalC;
   [Output][Units("mm")] double PlantTranspiration;
   
   [EventHandler] public void OnInitialised()
   {
      Console.WriteLine("     Initialising");

      foreach(Component myModule in myPaddock.Children)
      {
         if(myModule.Name.Contains(" Nitrogen"))
         {
            SoilName = myModule.Name.Split(' ')[0];
         }
      }
   }

   [EventHandler] public void OnPost()
   {
      if (n_demanded != null)
      {
         PlantNUptake = n_uptake;
         PlantTotalN = totaln;
         PlantTotalC = 0.4 * totalwt;
         PlantTranspiration = ep;
      }
      else if (NitrogenSupplyLayers != null)
      {
         PlantNUptake = NitrogenUptake;
         PlantTotalN = TotalPlantN;
         PlantTotalC = 0.4 * TotalPlantWt;
         PlantTranspiration = WaterUptake;
      }
      else
      {
         PlantTotalN = 0.0;
         PlantTranspiration = 0.0;
      }
   }
}
