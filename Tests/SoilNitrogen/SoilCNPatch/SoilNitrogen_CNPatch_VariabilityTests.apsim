                <MergePatch_Receiver type="text" description=" Receiving patch name:">base</MergePatch_Receiver>
                <MergePatch_Merged type="text" description=" Patch being merged ( if 'last' is given, then last patch is merged to receiving):">last</MergePatch_Merged>
                <MergePatch_Interval type="text" description=" If 'last' was given, mergings can be recurrent, type the interval between attempts:">5</MergePatch_Interval>
