                <AddPatch_NumAdditions type="text" description=" Number of times to add new patches:">15</AddPatch_NumAdditions>
                <AddPatch_Interval type="text" description=" Interval between addition of new patches (days):">1</AddPatch_Interval>
                <dummy2 type="category" description="Info about the type of deposition" />
