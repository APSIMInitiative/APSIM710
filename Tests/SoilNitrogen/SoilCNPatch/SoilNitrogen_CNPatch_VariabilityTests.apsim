              <text>
using System;
using ModelFramework;

public class Script 
{      

   [Param] DateTime MergePatch_DateToAdd;
   [Param] string MergePatch_Select;
   [Param] string MergePatch_Receiver;
   [Param] string MergePatch_Merged;
   [Param] int MergePatch_Interval;
   [Param] int MergePatch_Times;
      
   [Link] private Clock myClock;
   [Link] SoilN mySoilN;

   [Event] public event MergeSoilCNPatchDelegate MergeSoilCNPatch;
   
   [Input] int PatchCount;
   [Input] string[] PatchName;
   
   private string[] PatchList;
   private int DaysAfterMerge;
   private int NumberOfMergers;

   [EventHandler] public void OnInitialised()
   {
      PatchList = new string[2];
      PatchList[0] = MergePatch_Receiver;
      PatchList[1] = MergePatch_Merged;
      
      DaysAfterMerge = 0;
      NumberOfMergers = 0;
      if (MergePatch_Select == "MergeAll")
         MergePatch_Times = 1;
      else if (MergePatch_Merged != "last")
         MergePatch_Interval = 1;
      
      Console.WriteLine();
      Console.WriteLine("     Merging of CNPatches initialised");
   }

   [EventHandler] public void OnPrepare()
   {
      if((myClock.Today &gt;= MergePatch_DateToAdd.Date) &amp;&amp; (PatchCount &gt; 1))
      {
         if ((MergePatch_Select == "MergeAll")&amp;&amp;(NumberOfMergers &lt;MergePatch_Times))
         {
            MergeSoilCNPatchType MergeInfo = new MergeSoilCNPatchType();
            MergeInfo.Sender = "manager";
            MergeInfo.MergeAll = true;
            MergeSoilCNPatch.Invoke(MergeInfo);
         }           
         if ((DaysAfterMerge &gt;= MergePatch_Interval) &amp;&amp;(NumberOfMergers &lt;MergePatch_Times))
         {
            MergeSoilCNPatchType MergeInfo = new MergeSoilCNPatchType();
            MergeInfo.Sender = "manager";
            if (MergePatch_Merged == "last")
               PatchList[1] = PatchName[PatchCount - 1];
            MergeInfo.AffectedPatches_nm = PatchList;
      
            MergeSoilCNPatch.Invoke(MergeInfo);
            NumberOfMergers += 1;
            DaysAfterMerge = 0;
         }
         else
         {
            DaysAfterMerge += 1;
         }
      }
   }
}
</text>
