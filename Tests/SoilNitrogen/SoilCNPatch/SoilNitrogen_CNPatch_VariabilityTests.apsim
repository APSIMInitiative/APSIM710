            dnit_p1[layer] += mySoilN[pdk].dnit[layer] * RelativeArea[pdk];
            dlt_n_min_res_p1[layer] += mySoilN[pdk].dlt_n_min_res[layer] * RelativeArea[pdk];
            dlt_n_min_tot_p1[layer] += mySoilN[pdk].dlt_n_min_tot[layer] * RelativeArea[pdk];
            n2o_atm_p1[layer] += mySoilN[pdk].n2o_atm[layer] * RelativeArea[pdk];
            
            dlt_fom_c_atm_p1[layer] += mySoilN[pdk].dlt_fom_c_atm[layer] * RelativeArea[pdk];
            dlt_biom_c_atm_p1[layer] += mySoilN[pdk].dlt_biom_c_atm[layer] * RelativeArea[pdk];
