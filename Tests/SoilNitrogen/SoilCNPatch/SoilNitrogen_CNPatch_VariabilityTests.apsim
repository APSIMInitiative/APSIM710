              <text>
using System;
using ModelFramework;

public class Script 
{      

   [Param] DateTime AddPatch_DateToAdd;
   [Param] int AddPatch_NumAdditions;
   [Param] int AddPatch_Interval;
   [Param] string AddPatch_DepositionType;
   [Param] string AddPatch_PatchToAdd;
   [Param] double AddPatch_NewArea;
   [Param] string AddPatch_SoluteType;
   [Param] double AddPatch_SoluteAmount;   
   
   [Link] private Paddock myPaddock;
   [Link] private Clock myClock;
   
   [Event] public event AddSoilCNPatchDelegate AddSoilCNPatch;
   
   [Input(IsOptional = true)] int numPatches;
      
   [Output] string SoilName1;

   private double[] SoluteToAdd;
   private int[] PatchToAddTo;
   private int nPatchesAdded;
   
   private int nPatches;
   private int DaysAfterAdding;
   
   [EventHandler] public void OnInitialised()
   {
      SoluteToAdd = new double[1];
      SoluteToAdd[0] = AddPatch_SoluteAmount;
      PatchToAddTo = new int[1];
      nPatchesAdded = 0;
      DaysAfterAdding = AddPatch_Interval;
      
      Console.WriteLine();
      Console.WriteLine("     Addition of CNPatches initialised");
      
   }

   [EventHandler] public void OnPrepare()
   {
      if(myClock.Today &gt;= AddPatch_DateToAdd.Date)
      {
         if ((DaysAfterAdding == AddPatch_Interval) &amp;&amp; (nPatchesAdded &lt; AddPatch_NumAdditions))
         {
            AddSoilCNPatchType NewPatch = new AddSoilCNPatchType();
            NewPatch.Sender = "manager";
            NewPatch.DepositionType = AddPatch_DepositionType;
            NewPatch.AreaFraction = AddPatch_NewArea;
            if (AddPatch_PatchToAdd == "base")
               PatchToAddTo[0] = 0;
            else
               PatchToAddTo[0] = 0; //numPatches - 1;
            NewPatch.AffectedPatches_id = PatchToAddTo;
            if (AddPatch_SoluteType == "Urea")
               NewPatch.Urea = SoluteToAdd;
            if (AddPatch_SoluteType == "NH4")
               NewPatch.NH4 = SoluteToAdd;
            if (AddPatch_SoluteType == "NO3")
               NewPatch.NO3 = SoluteToAdd;
      
            AddSoilCNPatch.Invoke(NewPatch);
            nPatchesAdded += 1;
            DaysAfterAdding = 1;
         }
         else
            DaysAfterAdding += 1;
      }
   }
      
   [EventHandler] public void OnPost()
   {

   }
}
</text>
