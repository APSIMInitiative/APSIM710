          <folder name="ManagerFolder" shortcut="/SoilNitrogen_CNPatch_VariabilityTests/SoilCNPatch-AddOnly/Test03-Bare+TypicSoil+addUreaLots/Sim03-ExplicitPatches/Paddock1-base-LightSoil/ManagerFolder">
            <manager2 name="Aux-Outputs" shortcut="/SoilNitrogen_CNPatch_VariabilityTests/SoilCNPatch-AddOnly/Test03-Bare+TypicSoil+addUreaLots/Sim03-ExplicitPatches/Paddock1-base-LightSoil/ManagerFolder/Aux-Outputs" />
            <manager2 name="AddSoilCNPatch">
              <ui>
                <dummy1 type="category" description="Basic info to add a urine patch to SoilNtrogen" />
                <AddPatch_DateToAdd type="text" description=" Day of month to add urine patch:">15-Aug-2000</AddPatch_DateToAdd>
                <AddPatch_NumAdditions type="text" description=" Number of times to add new patches:">1</AddPatch_NumAdditions>
                <AddPatch_Interval type="text" description=" Interval between addition of new patches (days):">5</AddPatch_Interval>
                <dummy2 type="category" description="Info about the type of deposition" />
                <AddPatch_DepositionType type="list" listvalues="ToAllPaddock,ToSpecificPatch,ToNewPatch,NewOverlappingPatches" description=" Choose the DepositionType:">ToNewPatch</AddPatch_DepositionType>
                <AddPatch_PatchToAdd type="list" listvalues="base,last" description="  If DepositionType is 'ToSpecificPatch' or 'ToNewPatch', choose which patch will be used">base</AddPatch_PatchToAdd>
                <AddPatch_NewArea type="text" description=" Relative area of new patch (0-1):">0.25</AddPatch_NewArea>
                <dummy3 type="category" description="Info about solute to add" />
                <AddPatch_SoluteType type="list" listvalues="None,Urea,NH4,NO3" description=" Choose the solute to be added:">Urea</AddPatch_SoluteType>
                <AddPatch_SoluteAmount type="text" description=" Amount of N to be added (kgN/ha_patch):">1000</AddPatch_SoluteAmount>
              </ui>
              <text>
using System;
using ModelFramework;

public class Script 
{      

   [Param] DateTime AddPatch_DateToAdd;
   [Param] int AddPatch_NumAdditions;
   [Param] int AddPatch_Interval;
   [Param] string AddPatch_DepositionType;
   [Param] string AddPatch_PatchToAdd;
   [Param] double AddPatch_NewArea;
   [Param] string AddPatch_SoluteType;
   [Param] double AddPatch_SoluteAmount;   
   
   [Link] private Paddock myPaddock;
   [Link] private Clock myClock;
   
   [Event] public event AddSoilCNPatchDelegate AddSoilCNPatch;
   
   [Input(IsOptional = true)] int numPatches;
      
   [Output] string SoilName1;

   private double[] SoluteToAdd;
   private int[] PatchToAddTo;
   private int nPatchesAdded;
   
   private int nPatches;
   private int DaysAfterAdding;
   
   [EventHandler] public void OnInitialised()
   {
      SoluteToAdd = new double[1];
      SoluteToAdd[0] = AddPatch_SoluteAmount;
      PatchToAddTo = new int[1];
      nPatchesAdded = 0;
      DaysAfterAdding = AddPatch_Interval;
      
      Console.WriteLine();
      Console.WriteLine("     Initialised");
      Console.WriteLine();
      
   }

   [EventHandler] public void OnPrepare()
   {
      if(myClock.Today &gt;= AddPatch_DateToAdd.Date)
      {
         if ((DaysAfterAdding == AddPatch_Interval) &amp;&amp; (nPatchesAdded &lt; AddPatch_NumAdditions))
         {
            AddSoilCNPatchType NewPatch = new AddSoilCNPatchType();
            NewPatch.Sender = "manager";
            NewPatch.DepositionType = AddPatch_DepositionType;
            NewPatch.AreaFraction = AddPatch_NewArea;
            if (AddPatch_PatchToAdd == "base")
               PatchToAddTo[0] = 0;
            else
               PatchToAddTo[0] = 0; //numPatches - 1;
            NewPatch.AffectedPatches_id = PatchToAddTo;
            if (AddPatch_SoluteType == "Urea")
               NewPatch.Urea = SoluteToAdd;
            if (AddPatch_SoluteType == "NH4")
               NewPatch.NH4 = SoluteToAdd;
            if (AddPatch_SoluteType == "NO3")
               NewPatch.NO3 = SoluteToAdd;
      
            AddSoilCNPatch.Invoke(NewPatch);
            nPatchesAdded += 1;
            DaysAfterAdding = 1;
         }
         else
            DaysAfterAdding += 1;
      }
   }
      
   [EventHandler] public void OnPost()
   {

   }
}
