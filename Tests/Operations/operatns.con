version = 7.1

[apsim.sample_operatns]
title=Operatns Sample Simulation
module = clock   operatns.par [sample] %apsim%/Model/clock.xml [standard]
module = report   operatns.par [sample] operatns.par [sample1]
module = input(met)      %apsim%/Examples/MetFiles/dalby.met[weather]
module = Operations   operatns.par [sample]
module = Irrigation
module = Fertiliser   %apsim%/Model/Fertiliser.xml [standard]
module = SoilWat   %apsim%/Examples/Soils/Soil.par [black_earth] %apsim%/Model/Soil.xml [SoilWat]
module = SoilN   %apsim%/Examples/Soils/Soil.par [black_earth] %apsim%/Model/Soil.xml [SoilN]
module = Tracker operatns.par[sample]

