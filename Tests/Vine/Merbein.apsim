        </Water>
        <SoilWater shortcut="/simulations/Merbein SpurPruned/Paddock/Soil/SoilWater" />
        <SoilOrganicMatter shortcut="/simulations/Merbein SpurPruned/Paddock/Soil/SoilOrganicMatter" />
        <Analysis shortcut="/simulations/Merbein SpurPruned/Paddock/Soil/Analysis" />
        <InitialWater name="Initial Water" shortcut="/simulations/Merbein SpurPruned/Paddock/Soil/Initial water" />
        <Sample name="Initial Nitrogen" shortcut="/simulations/Merbein SpurPruned/Paddock/Soil/Initial nitrogen" />
      </Soil>
      <surfaceom name="surface organic matter" shortcut="/simulations/Merbein SpurPruned/Paddock/surface organic matter" />
      <fertiliser shortcut="/simulations/Merbein SpurPruned/Paddock/fertiliser" />
      <irrigation name="Irrigation" shortcut="/simulations/Merbein SpurPruned/Paddock/Irrigation" />
      <folder name="Manager folder" shortcut="/simulations/Merbein SpurPruned/Paddock/Manager folder">
        <manager name="Sow on a fixed date">
          <ui>
            <category description="Sowing criteria" type="category" />
            <date type="text" description="Enter sowing date (dd-mmm) : ">13-may</date>
            <category description="Sowing parameters" type="category" />
            <crop type="crop" description="Enter name of crop to sow : ">vine</crop>
            <density type="text" description="Enter sowing density  (plants/m2) : ">0.1344</density>
            <depth type="text" description="Enter sowing depth  (mm) : ">50</depth>
            <cultivar type="cultivars" description="Enter cultivar : ">Shiraz</cultivar>
            <class type="classes" description="Enter crop growth class : ">plant</class>
            <row_spacing type="text" description="Enter row spacing (mm) : ">3100</row_spacing>
            <maxcover type="text" description="Maximum Crop Cover:">0.5</maxcover>
            <BudNo type="text" description="Bud Number (/plant):">530</BudNo>
          </ui>
          <script name="start_of_day">
            <text><![CDATA[
        if (today = date('[date]') and [crop].Plant_Status = 'out') then
!        if (today = date('[date]') ) then
              [crop] sow Population =[density], depth = [depth], cultivar = [cultivar], row_spacing = [row_spacing], crop_class = [class], maxcover=[maxcover], budnumber=[BudNo]
         endif
               ]]></text>
            <event>start_of_day</event>
          </script>
        </manager>
        <manager name="Harvesting rule" shortcut="/simulations/Merbein SpurPruned/Paddock/Manager folder/Harvesting rule" />
        <manager name="Prune on a fixed date">
          <ui>
            <category description="Sowing criteria" type="category" />
            <date type="text" description="Enter pruning date (dd-mmm) : ">12-may</date>
            <category description="Sowing parameters" type="category" />
            <crop type="crop" description="Enter name of crop to sow : ">vine</crop>
          </ui>
          <script>
            <text>
        if (today = date('[date]') and [crop].Plant_Status &lt;&gt; 'out') then
              [crop] prune budnumber = 900
         endif
               </text>
            <event>start_of_day</event>
          </script>
        </manager>
      </folder>
      <outputfile>
        <filename output="yes">Merbein MinimalPruned.out</filename>
        <title>Merbein MinimalPruned</title>
        <variables name="Variables" shortcut="/simulations/Merbein SpurPruned/Paddock/outputfile/Variables" />
        <events name="Reporting Frequency" shortcut="/simulations/Merbein SpurPruned/Paddock/outputfile/Reporting Frequency" />
      </outputfile>
      <vine shortcut="/simulations/Merbein SpurPruned/Paddock/vine" />
