        <manager name="ClimateControl">
          <script>
            <text>if DaysSinceSowing &gt; TransitionDays then
   maxt = PostMaxT
   mint = PostMinT
   Radn = PostRadn
   PhotoPeriod = PostPP
else
   maxt = PreMaxT
   mint = PreMinT
   Radn = PreRadn
   PhotoPeriod = PrePP
endif</text>
            <event>prenewmet</event>
          </script>
          <script>
            <text>DaysSinceSowing = 0</text>
            <event>Init</event>
          </script>
