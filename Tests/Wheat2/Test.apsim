            <SoilCrop name="Barley" shortcut="/simulations/Base/Wheat/paddock/Soil/Water/Barley" />
            <SoilCrop name="Chickpea" shortcut="/simulations/Base/Wheat/paddock/Soil/Water/Chickpea" />
            <SoilCrop name="Lucerne" shortcut="/simulations/Base/Wheat/paddock/Soil/Water/Lucerne" />
            <SoilCrop name="Maize" shortcut="/simulations/Base/Wheat/paddock/Soil/Water/Maize" />
