          <ModifyKL type="yesno" description="Modify KL using CL, EC or ESP if found?">yes</ModifyKL>
        </wheat>
        <folder name="Manager folder" shortcut="/simulations/Base/Wheat/paddock/Manager folder">
          <manager name="Sow on a fixed date" shortcut="/simulations/Base/Wheat/paddock/Manager folder/Sow on a fixed date" />
          <manager name="Harvesting rule" shortcut="/simulations/Base/Wheat/paddock/Manager folder/Harvesting rule" />
          <manager name="Grazing management">
            <script>
              <text>annual_lwg = 0.0
paddockState = 'empty'
</text>
              <event>init</event>
            </script>
            <script>
              <text>! Graze paddock when wheat is ready
if (today = date('1-jul') AND paddockState = 'empty') then
   graz set stocking_rate = 0.8
   graz set alw =  400.0                 ! p084
   annual_lwg = 0.0
   paddockState = 'stocked'
endif

! 'GRAZEON' stocking rate resets (%Use of TSDM for next year)
if (graz.stocking_rate &gt; 0.0 AND today = date('1-jun') then                ! p081
   graz set alw = 400.0
   avgDailyIntake = (annual_lwg/365.25 + 1.058) / 0.304
   targetUtil = 30.0 / 100.0
   annualIntake = avgDailyIntake * 365.5
   stock_equiv_ratio = targetUtil * 2000 / annualIntake

   weight_ratio = graz.alw / 200
   factor = weight_ratio ** 0.75
   beasts_ha = stock_equiv_ratio / factor
   graz set stocking_rate = beasts_ha

   annual_lwg = 0.0
endif

</text>
              <event>start_of_day</event>
            </script>
            <script>
              <text>
annual_lwg = annual_lwg + graz.lwg</text>
              <event>end_of_day</event>
            </script>
          </manager>
