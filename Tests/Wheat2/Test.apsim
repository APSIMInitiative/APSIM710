            <SoilCrop name="Sorghum" shortcut="/simulations/Base/Wheat/paddock/Soil/Water/Sorghum" />
            <SoilCrop name="Wheat" shortcut="/simulations/Base/Wheat/paddock/Soil/Water/Wheat" />
            <SoilCrop name="wheat2" shortcut="/simulations/Base/Wheat/paddock/Soil/Water/wheat2" />
          </Water>
          <SoilWater shortcut="/simulations/Base/Wheat/paddock/Soil/SoilWater" />
          <SoilOrganicMatter shortcut="/simulations/Base/Wheat/paddock/Soil/SoilOrganicMatter" />
          <Sample name="Initial nitrogen" shortcut="/simulations/Base/Wheat/paddock/Soil/Initial nitrogen" />
          <Analysis shortcut="/simulations/Base/Wheat/paddock/Soil/Analysis" />
        </Soil>
        <surfaceom name="surface organic matter" shortcut="/simulations/Base/Wheat/paddock/surface organic matter" />
