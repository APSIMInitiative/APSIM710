        <manager name="Irrigator rule">
          <ui>
            <number_of_paddocks type="text" description="Number of paddocks irrigator must rotate around">10</number_of_paddocks>
            <rain_event_trigger type="text" description="rainfall amount  (mm) to trigger irrigation pause (mm)">20</rain_event_trigger>
            <rain_event_pause_time type="text" description="number of days to pause after trigger rainfall event (d)">3</rain_event_pause_time>
            <Irrigation_amount type="text" description="Amount of irrigation applied (mm)">50</Irrigation_amount>
            <trigger_deficit type="text" description="Fraction of available water in the top layer below which irrigation is allowed (0-1)">0.5</trigger_deficit>
            <Season_start type="ddmmmdate" description="Date irrigation season opens (dd-mmm)">1-oct</Season_start>
            <Season_finish type="ddmmmdate" description="Date irrigation season closes (dd-mmm)">1-Jan</Season_finish>
            <Min_depth type="text" description="Minimum depth for calculating SWD (mm)">300</Min_depth>
          </ui>
          <script name="init">
            <text>irrigation_season = 1 !period when we forbid irrigation 
irrigation_season_open = 1 !period when we forbid irrigation
paddock = 0 !start in paddock 0
days_since_rain_event = 0 ! counter to pause irrigation after rainfall event
FASW_top = 1
depth = 0
</text>
            <event>init</event>
          </script>
          <script name="start_of_day">
            <text><![CDATA[FASW_top = (sw(1)-ll15(1))/(dul(1) - ll15(1))
depth = dlayer(1)

if depth < [Min_depth] then
   FASW_top = ((sw(1) +sw(2)) - (ll15(1) + ll15(2)))/((dul(1) + dul(2)) - (ll15(1) + ll15(2)))
   depth = depth + dlayer(2)
endif
if depth < [Min_depth] then
   FASW_top = ((sw(1) +sw(2) +sw(3)) - (ll15(1) + ll15(2) +ll15(3)))/((dul(1) + dul(2) + dul(3)) - (ll15(1) + ll15(2) +ll15(3)))
   depth = depth + dlayer(3)
endif
if depth < [Min_depth] then
   FASW_top = ((sw(1) +sw(2) +sw (3) + sw(4)) - (ll15(1) + ll15(2) +ll15(3) + 1115(4)))/((dul(1) + dul(2) + dul(3) + dul(4)) - (ll15(1) + ll15(2) +ll15(3) + ll15(4)))
   depth = depth + dlayer(4)
endif
if depth < [Min_depth] then
   FASW_top = ((sw(1) +sw(2) +sw (3) + sw(4) + sw(5)) - (ll15(1) + ll15(2) +ll15(3) + 1115(4) + ll15(5)))/((dul(1) + dul(2) =  dul(3) + dul(4) + dul(5)) - (ll15(1) + ll15(2) + ll15(3) + ll15(4) + ll15(5)))
endif

!!!!!!!!!!!Specify irrigation season start time!!!!!!!!!
irrigation_season =  (date_within('[Season_start], [Season_finish]')

if       (Irrigation_season = 0) then      !!!!This rule prevents irrigation in June and July
            irrigation_season_open = 0
            paddock = 0
elseif   (Irrigation_season = 1) and (FASW_top < [trigger_deficit]) then
            irrigation_season_open = 1
endif

!!!!!!!!!!!Count days since last rainfall event!!!!!!!!!

if        (rain >= [rain_event_trigger]) then               !!!!This rule pauses the irrigators rotation for a given number of days following a defined size of rainfall event
              days_since_rain_event = 0
else     days_since_rain_event = days_since_rain_event + 1
endif             

!!!!!!!!!!Rotate irrigator around paddocks and apply irrigation when it gets back to this paddock!!!!!!!!!!!!!

if           (irrigation_season_open = 1) and (paddock = 0) and (days_since_rain_event > [rain_event_pause_time]) then    !!!!This rule applies irrigation when the irrigator returns to this paddock if the SWD is below a given trigger
              paddock = 2                                                            !I This is 2 because it is skipping this paddock so the irrigator will return sooner than if it spend a day here irrigating
                  if  (FASW_top < [trigger_deficit]) and (paddock_is_fallow()=0) then
                         irrigation apply amount = [irrigation_amount]
                         paddock = 1                                                  !If an irrigation event is triggered in this paddock subtract 1 so that this paddock is considered in the irrigation rotation counter
                  endif
elseif  (irrigation_season_open = 1) and (paddock = [number_of_paddocks]) and (days_since_rain_event > [rain_event_pause_time]) then    !!This rule finishes the irrigators rotation and sends it back to the start (to water this paddock)
               paddock = 0 
elseif  (irrigation_season_open = 1) and (paddock > 0) and (days_since_rain_event > [rain_event_pause_time]) then    !!!!This rule moves the irrigator around one paddock in the rotation each day 
               paddock = paddock + 1
endif

]]></text>
            <event>start_of_day</event>
          </script>
          <script name="end_of_day">
            <text>
            </text>
            <event>end_of_day</event>
          </script>
