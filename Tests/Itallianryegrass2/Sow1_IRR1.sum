------- Irrigator rule Initialisation -----------------------------------------
     Manager rules:
     
     SECTION:- init
     irrigation_season = 1
     irrigation_season_open = 1
     paddock = 0
     days_since_rain_event = 0
     fasw_top = 1
     depth = 0
     
     SECTION:- start_of_day
     fasw_top = (sw(1)-ll15(1))/(dul(1) - ll15(1))
     depth = dlayer(1)
     if depth < 300 then
        fasw_top = ((sw(1) +sw(2)) - (ll15(1) + ll15(2)))/((dul(1) + dul(2)) - (ll15(1) + ll15(2)))
        depth = depth + dlayer(2)
     endif
     if depth < 300 then
        fasw_top = ((sw(1) +sw(2) +sw(3)) - (ll15(1) + ll15(2) +ll15(3)))/((dul(1) + dul(2) + dul(3)) - (ll15(1) + ll15(2) +ll15(3)))
        depth = depth + dlayer(3)
     endif
     if depth < 300 then
        fasw_top = ((sw(1) +sw(2) +sw (3) + sw(4)) - (ll15(1) + ll15(2) +ll15(3) + 1115(4)))/((dul(1) + dul(2) + dul(3) + dul(4)) - (ll15(1) + ll15(2) +ll15(3) + ll15(4)))
        depth = depth + dlayer(4)
     endif
     if depth < 300 then
        fasw_top = ((sw(1) +sw(2) +sw (3) + sw(4) + sw(5)) - (ll15(1) + ll15(2) +ll15(3) + 1115(4) + ll15(5)))/((dul(1) + dul(2) =  dul(3) + dul(4) + dul(5)) - (ll15(1) + ll15(2) + ll15(3) + ll15(4) + ll15(5)))
     endif
     irrigation_season =  (date_within('1-oct, 1-jan')
     if       (irrigation_season = 0) then
     irrigation_season_open = 0
     paddock = 0
     elseif   (irrigation_season = 1) and (fasw_top < 0.5) then
     irrigation_season_open = 1
     endif
     if        (rain >= 20) then
       days_since_rain_event = 0
     else     days_since_rain_event = days_since_rain_event + 1
     endif
     if           (irrigation_season_open = 1) and (paddock = 0) and (days_since_rain_event > 3) then
       paddock = 2
           if  (fasw_top < 0.5) and (paddock_is_fallow()=0) then
                  irrigation apply amount = 50
                  paddock = 1
           endif
     elseif  (irrigation_season_open = 1) and (paddock = 10) and (days_since_rain_event > 3) then
        paddock = 0
     elseif  (irrigation_season_open = 1) and (paddock > 0) and (days_since_rain_event > 3) then
        paddock = paddock + 1
     endif
     
     SECTION:- end_of_day
     END of rules
     
