version = 7.5
[APSIM.Dalby 600mm]
title=Dalby 600mm wet soil
module = Clock   sorghum.par [sensibility] %apsim%/Model/Clock.xml [standard]
module = Report     sorghum.par[sensibility] sorghum.par[Dalby600]
module = Input(met)        %apsim%/Tests/MetFiles/Dalby.met[weather]
module = Manager    sorghum.par[sensibility]
module = Fertiliser   %apsim%/Model/Fertiliser.xml [standard]
module = SoilWat   %apsim%/Tests/Soils/Waco.par [waco] %apsim%/Tests/Soils/Waco.par [600mm] %apsim%/Model/Soil.xml [SoilWater]
module = SurfaceOM   %apsim%/Examples/Soils/Soil.par [legume_residue] %apsim%/Model/SurfaceOM.xml [standard]
module = SoilN   %apsim%/Tests/Soils/Waco.par [waco] %apsim%/Model/Soil.xml [SoilNitrogen]
module = Sorghum   %apsim%/Tests/Soils/Waco.par [waco] %apsim%/Model/Sorghum.xml [standard]
module = Tracker    sorghum.par[sensibility]
!module = SysBal

