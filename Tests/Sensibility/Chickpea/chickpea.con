version = 7.5(2)
[APSIM.Dalby 600mm]
title=Dalby 600mm wet soil
module = Clock   chickpea.par [sensibility] %apsim%/Model/Clock.xml [standard]
module = Report     chickpea.par[sensibility] chickpea.par[Dalby600]
module = Input(met)        %apsim%/Tests/MetFiles/Dalby.met[weather]
module = Manager    chickpea.par[sensibility]
module = SoilWat   %apsim%/Tests/Soils/Waco.par [waco] %apsim%/Tests/Soils/Waco.par [600mm] %apsim%/Model/Soil.xml [SoilWater]
module = SurfaceOM   %apsim%/Examples/Soils/Soil.par [legume_residue] %apsim%/Model/SurfaceOM.xml [standard]
module = SoilN   %apsim%/Tests/Soils/Waco.par [waco] %apsim%/Model/Soil.xml [SoilNitrogen]
module = Plant(chickpea)   %apsim%/Tests/Soils/Waco.par [waco] %apsim%/Model/Chickpea.xml [standard]
module = Tracker    chickpea.par[sensibility]

