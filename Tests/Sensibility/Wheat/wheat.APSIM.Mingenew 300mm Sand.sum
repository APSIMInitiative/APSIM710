------- Manager Initialisation ------------------------------------------------
     Manager rules:
     
     SECTION:- init
     in_crop_rain = 0
     eff_in_crop_rain = 0
     
     SECTION:- start_of_day
     if day = sow_day then
        soilwat reset
        soiln reset
        surfaceom reset
        fertiliser apply amount = nrate, type = no3_n, depth = 5 (mm)
        wheat sow cultivar = cultivar, plants = 100 (/m2), sowing_depth = 40 (mm)
        sowpaw = sw_dep() - ll_dep()
        sown = no3()
        sowing_paw2n = sowpaw / sown
        in_crop_rain = 0
        eff_in_crop_rain = 0
     endif
     if today = date('15-aug') then
         fertiliser apply amount = split_nrate, type = no3_n, depth = 5 (mm)
      endif
     
     SECTION:- end_of_day
     in_crop_rain = in_crop_rain + met.rain
     eff_in_crop_rain = eff_in_crop_rain + met.rain - soilwat.drain - soilwat.runoff
     if wheat.stagename = 'harvest_ripe' or wheat.plantstatus = 'dead' then
        hi = wheat.yield/wheat.biomass
        nhi = wheat.grainn/wheat.biomass_n
        report do_output
        wheat harvest
        wheat end_crop
     endif
     
     SECTION:- init
     cultivar = 'hartog'
     
     SECTION:- init
     sow_day = 135
     
     SECTION:- init
     nrate = 10
     
     SECTION:- init
     split_nrate = 40
     END of rules
     
