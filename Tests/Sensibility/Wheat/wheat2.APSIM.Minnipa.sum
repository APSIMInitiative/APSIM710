------- Manager Initialisation ------------------------------------------------
     Manager rules:
     
     SECTION:- start_of_day
     if today = date('1-mar') then
        soilwat reset
        soiln reset
        surfaceom reset
      endif
     if date_within('15-apr,15-jun') and wheat.plantstatus = 'out' and rain[3] > 20 then
        fertiliser apply amount = nrate, type = no3_n, depth = 5 (mm)
        wheat sow cultivar = cultivar, plants = 200 (/m2), sowing_depth = 30 (mm)
        in_crop_rain = 0
        eff_in_crop_rain = 0
     endif
     if today = date('15-aug') then
         fertiliser apply amount = split_nrate, type = no3_n, depth = 5 (mm)
      endif
     
     SECTION:- end_of_day
     in_crop_rain = in_crop_rain + met.rain
     eff_in_crop_rain = eff_in_crop_rain + met.rain - soilwat.drain - soilwat.runoff
     if wheat.stagename = 'harvest_ripe' or wheat.plantstatus = 'dead' then
        if wheat.biomass > 1 then
           hi = wheat.yield/wheat.biomass
           nhi = wheat.grainn/wheat.biomass_n
           report do_output
        endif
        wheat harvest
        wheat end_crop
     endif
     
     SECTION:- init
     in_crop_rain = 0
     eff_in_crop_rain = 0
     
     SECTION:- init
     cultivar = 'yitpi'
     
     SECTION:- init
     nrate = 40
     
     SECTION:- init
     split_nrate = 0
     END of rules
     
