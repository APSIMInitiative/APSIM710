<folder version="33" name="simulations">
  <simulation name="Clock Test">
    <metfile name="met">
      <filename name="filename" input="yes">%apsim%\Examples\MetFiles\Goond.met</filename>
    </metfile>
    <clock>
      <start_date type="date" description="Enter the start date of the simulation">1/11/1940</start_date>
      <end_date type="date" description="Enter the end date of the simulation">2/01/1941</end_date>
    </clock>
    <summaryfile />
    <outputfile>
      <filename output="yes">Clock Test.out</filename>
      <title>Clock Test</title>
      <variables name="Variables">
        <variable>dd/mm/yyyy as Date</variable>
        <variable>day_of_month</variable>
        <variable>day_of_year</variable>
        <variable>is_end_week</variable>
        <variable>is_end_month</variable>
        <variable>is_end_year</variable>
        <variable>simulation_days</variable>
        <variable>month_str</variable>
      </variables>
      <events name="Reporting Frequency">
        <event>report</event>
      </events>
    </outputfile>
  </simulation>
</folder>