<folder version="28" name="simulations">
  <simulation name="Clock Test">
    <metfile name="met">
      <filename name="filename" input="yes">%apsim%\Examples\MetFiles\Goond.met</filename>
    </metfile>
    <clock>
      <start_date type="date" description="Enter the start date of the simulation">01/11/1940</start_date>
      <end_date type="date" description="Enter the end date of the simulation">02/01/1941</end_date>
    </clock>
    <summaryfile />
    <outputfile>
      <filename output="yes">Clock Test.out</filename>
      <title>Clock Test</title>
      <variables name="Variables">
        <variable name="dd/mm/yyyy as Date" />
        <variable name="day_of_month" />
        <variable name="day_of_year" />
        <variable name="is_end_week" />
        <variable name="is_end_month" />
        <variable name="is_end_year" />
        <variable name="simulation_days" />
        <variable name="month_str" />
      </variables>
      <events name="Reporting Frequency">
        <event name="report" />
      </events>
    </outputfile>
  </simulation>
</folder>