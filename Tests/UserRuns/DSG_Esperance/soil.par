[scaddan.SoilWat.parameters] 

! pale grey sandy loam duplex - depth of AB 10-15   
insoil =  2.0   
u =  3   
cn2_bare =  80
cn_red =  20 ()   
cn_cov =  0.8 ()   
salb =  0.18 !    
cona =  3.0 ! NB BAK has used 2 for sands in WA
diffus_const =  88.   
diffus_slope =  35.0   

runoff_filename =  blank   

!layer    1    2    3    4    5    6    7  
!depth   150  170  375  600  900 1200 1500 
dlayer = 150  20   205  225  300  300  300 
bd =     1.40 1.65 1.65 1.65 1.65 1.65 1.65 
ll15 =  .025  .208 .171 .171 .171 .171 .171 
dul =   .105  .285 .286 .286 .286 .286 .286 
sat =   .40  .35   .35  .35  .35  .35  .35 
swcon = 0.5  0.2   0.3  0.3  0.3  0.3  0.3 
air_dry = .012 .2 .170 .17 .17  .17  .17 

sw = .105  .285 .286 .286 .286 .286 .286 

[scaddan.SoilN.parameters]    
!layer    1    2    3    4    5    6    7  
oc =     0.9  0.3  0.2  0.1  0.1  0.1  0.1 
ph =     7.0  7.0  7.0  7.0  7.0  7.0  7.0 
nh4ppm = 0.3  0.3  0.3  0.3  0.3  0.3  0.3 
no3ppm = 0.3  0.3  0.3  0.3  0.3  0.3  0.3 
ureappm =  0    0    0    0    0    0    0 
fbiom =  0.04 0.025 0.015 0.01 0.01 0.01 0.01
finert = 0.1  0.3   0.75  0.90 0.95 0.99 0.99


soil_cn =  14.5   
enr_a_coeff =  7.4 ()   !stuff for mapping erosion
enr_b_coeff =  0.2   
profile_reduction =  on   

root_wt =  1500.   ! root residues as biomass (kg/ha)
root_cn =  30.   

[scaddan.residue2.parameters]  
pot_decomp_rate =  0.10 ()    ! Potential decomposition rate
residue_wt =  1500. (kg/ha)   ! Initial surface residue
residue_cnr =  70.0 ()        ! C:N ration of initial residue
residue_type =  wheat         ! Type if initial residue

[scaddan.soiltemp.parameters]

bound_layer_cond = 20.0  ! (J/s/m2/K)
!layer    1    2    3    4    5    6    7  
!depth   150  170  375  600  900 1200 1500 
clay =   0.05 0.45 0.45 0.45 0.45 0.45 0.45 


[scaddan.sorghum.parameters]
uptake_source =    calc   ! calculate own uptakes.
!layer    1    2    3    4    5    6    7  
!depth   150  170  375  600  900 1200 1500 
ll =    .025  .208 .171 .171 .171 .171 .171
kl =    0.07  0.07 0.05 0.05 0.04 0.04 0.04  () ! Water Extraction parameter (0-1)
xf =    1.0   0.6   0.2  0.2  0.2 0.0  0.0   ()! Root Exploration factor (0-1)

[scaddan.wheat.parameters]
uptake_source =    calc   ! calculate own uptakes.
!layer    1    2    3    4    5    6    7  
!depth   150  170  375  600  900 1200 1500 
ll =    .025  .208 .171 .171 .171 .171 .171
kl =    0.07  0.07 0.05 0.05 0.04 0.04 0.04  () ! Water Extraction parameter (0-1)
xf =    1.0   0.6   0.2  0.2  0.2 0.0  0.0   ()! Root Exploration factor (0-1)


