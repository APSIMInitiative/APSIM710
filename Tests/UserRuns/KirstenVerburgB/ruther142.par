sw_dep_180300 = sw_dep(12) + sw_dep(13) + sw_dep(14) + sw_dep(15) + sw_dep(16) + sw_dep(17)

!net flux (system) past 180 cm
sw_dep_0180    = sw_dep_10180 + sw_dep(1)
netdrain180    = met.rain - ET - (sw_dep_0180 - oldsw_dep_0180)
cumnetdrain180 = cumnetdrain180 + netdrain180
oldsw_dep_0180 = sw_dep_0180

