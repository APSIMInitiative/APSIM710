[*contents]

[run%.soiln2.parameters]

   amp               =   11.9   (oC)  	!temperature amplitude (oC) -
                                        !difference between highest and 
                                        !lowest mean monthly air temperatures                       
   tav               =   25.9   (oC)    ! mean annual air temperature (oC)                           
   root_cn           =   45.0   ()      !                                                            
   root_wt           =  500.0   (kg/ha) !                                                            
   soil_cn           =   7.82   ()      !soil humic pool C:N ratio                                               
                                        !(JVDK information)                  

   enr_a_coeff       =    7.4   ()
   enr_b_coeff       =    0.2   ()
