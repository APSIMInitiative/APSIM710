ll =#LL
kl =  0.07    0.07    0.07    0.07    0.07    0.07    0.04    0.04    0.04   0.04 ()
xf =  1.00    1.00    1.00    1.00    1.00    1.00    1.00    1.00    1.00   1.00 ()


[run%.cowpea.parameters]

