   tt_end_grain_to_maturity  = 60          (oCd)     ! 60
   tt_maturity_to_ripe      =  40          (oCd)     ! 40
 
   x_stem_wt                = 0  390   (g/m3)    ! (measured final: 303g/m2) used in legnew_canopy_height
   y_height                 = 0  800   (mm)      ! measured 615, see icarda annual rep. 1987: 800-850cm



