   x_pp_init_to_flower      =   1   24     (h)
   y_tt_init_to_flower      =   35  35     (oCd)
   
   x_pp_flower_to_start_grain  =  1    24  (h)
   y_tt_flower_to_start_grain  =  230  230 (oCd)     !215 both
   
