[all.report.parameters]
!APSIM_progress_form = on ! off

! report parameters for excel
                                                  

variable = clock.year
variable = clock.day
variable = chickpea.DaysAfterSowing
variable = manager.tt_since_emerg
variable = chickpea.PhenologyStage
variable = chickpea.LAI
variable = chickpea.biomass
variable = chickpea.GrainWt as GrainWt
variable = chickpea.LeafLiveWt
variable = chickpea.StemLiveWt
variable = chickpea.biomass_n
variable = chickpea.GrainN
variable = chickpea.RootDepth
variable = chickpea.StemLiveN
variable = SoilWat.sw(1-10)
variable = SoilN.no3ppm(1-6)
variable = SoilN.nh4ppm(1-6)
variable = chickpea.LeafLiveNConc
variable = chickpea.StemLiveNConc
variable = chickpea.n_conc_grain

!----------------------------------------------------------------------------------
[g2w1_00.report.parameters]
Outputfile   =g2_00_w1.out


[g2w0_00.report.parameters]
Outputfile   =g2_00_w0.out


!----------------------------------------------------------------------------------
[g2rf_99.report.parameters]
Outputfile   =g2_99_w0.out
[g2w1_00.SummaryFile.parameters]
summaryfile = g2_00_w1.sum
[g2w0_00.SummaryFile.parameters]
summaryfile = g2_00_w0.sum
[g2rf_99.SummaryFile.parameters]
summaryfile = g2_99_w0.sum
