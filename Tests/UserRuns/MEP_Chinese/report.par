! XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

[wheat.report.parameters]  

variable = clock.day
variable = clock.year
variable = clock.dd/mm/yyyy as date
variable = manager.event
variable = manager.flag
variable = tracker1.sum@met_rain as rain
variable = tracker1.sum@wheat_ep as w_ep
variable = SoilWat.sw as sw
variable = tracker1.sum@soilwat2_es as es
variable = tracker1.sum@soilwat2_runoff as runoff
variable = tracker1.sum@soilwat2_drain as drain
variable = SoilWat.sw_dep() as sw_dep
variable = SoilN.nh4()
variable = SoilN.no3() as tot_no3
variable = SoilN.no3
variable = tracker1.sum@soiln2_dnit[] as dnit
variable = SoilN.hum_c(1)
variable = SoilN.biom_c(1)
variable = SoilN.fom_c(1)
variable = surfaceom.surfaceom_n
variable = surfaceom.surfaceom_wt
variable = wheat.lai as w_lai
variable = wheat.yield as w_yield
variable = wheat.biomass as w_biomass
variable = wheat.RootGreenWt as w_root_wt
variable = wheat.root_depth as w_root_dep
variable = wheat.biomass_n as w_biom_n
variable = wheat.maturity_date as w_mat
variable = tracker1.avg@wheat_nfact_photo as n_stress
variable = tracker1.avg@wheat_swdef_photo as w_stress
variable = tracker1.sum@soilwat2_eo as eo
