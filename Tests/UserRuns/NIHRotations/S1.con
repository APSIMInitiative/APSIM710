version = 7.2

!!!!!!!!!!!!!!!!!!!!!!!! warialda lg1 Nfert=50 system=s1  Root = deep sowing = AvgWindow !!!!!!!!!!!!!!!!!!!!!!!


[APSIM.warialda_lg1_50_s1_deep_AvgWindow]
Title = site=warialda; soil=lg1; treatment=s1; Nrate=50; RootDepth=deep; Sowing = AvgWindow
module = clock   PRule.par [all] %apsim%/Model/clock.xml [standard]
module = report      report_files.par[warialda_lg1_50_s1_deep_AvgWindow]      report.par [whtsrg]
module = input(met)         warialda.met [weather]
module = manager     prule.man [Fert_50]   prule.man [s1] prule.man [s] prule.man [all] prule.man [AvgWindow]
module = Fertiliser   %apsim%/Model/Fertiliser.xml [standard]
module = SoilWat   soil.par [lg1] %apsim%/Model/Soil.xml [SoilWat]
module = SurfaceOM   PRule.par [all] %apsim%/Model/SurfaceOM.xml [standard]
module = SoilN   soil.par [all] %apsim%/Model/Soil.xml [SoilN]
module = plant(wheat)   soil.par [lg1deep] %apsim%/Model/wheat.xml [standard]
module = plant(chickpea)   soil.par [lg1deep] %apsim%/Model/chickpea.xml [standard]
module = sorghum   soil.par [lg1deep] %apsim%/Model/sorghum.xml [standard]
module = tracker   report.par[all]
!module = sysbal

