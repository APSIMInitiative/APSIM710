using System;
using ModelFramework;
using LayerCanopyPhotosynthesis;
 
public class Script 
{      
   [Link]  public Simulation MySimulation;
   [Link] Paddock MyPaddock; // Can be used to dynamically get access to simulation structure and variables
   [Input] DateTime Today;   // Equates to the value of the current simulation date - value comes from CLOCK
   [Output] public double[] dcapst = new double[5];
   
   //Additional Outputs
   [Output] public double BIOtotalDAY;
   [Output] public double BIOshootDAY;
   [Output] public double RootShoot;
   [Output] public double EcanDemand; 
   [Output] public double EcanSupply;
   [Output] public double RUE;
   [Output] public double TE; 
   [Output] public double RadIntDcapst; 
   [Output] public double BIOshootDAYPot;
   
   public PhotosynthesisModel PM;
   
   public string Shift2PsModel = "dcapst"; //The photosynthesis module to which you want to witch when LAI&gt;LAITrigger
   public double LAITrigger = 0.5; //reduced from 1 (AW, 2019.09.12)
   public double PsiFactor = 1.0; //psiFactor-Psi Reduction Factor  
   
   public bool DCaPSTTriggered = false;
   public string PsModelName1;
   
   int DOY = 0;
   int iter = 0;
   double latitude = 0;
   double maxT = 0;
   double minT = 0;
   double radn = 0;
   double RootShootRatio = 0;
   double SLN = 0;
   double SWAvailable = 0;
   double SWAvailablePrev = 0;
   double lai = 0;   
   
   // (Ben, 2019.07.20): Set this to TRUE to use APSIM internal CO2
   bool APSIMCO2 = false;
   public double CO2 = 370;
      
   // The following event handler will be called once at the beginning of the simulation
   [EventHandler] public void OnInitialised()
   {
     
      PM = new PhotosynthesisModelC3();
      PM.photoPathway = PhotosynthesisModel.PhotoPathway.C3;
      PM.Initialised = false;
      PM.Canopy.NLayers = 1;

      //Set the parameters
      PM.EnvModel.Initialised = false;
      
      PM.Canopy.U0 = 1.5;     //U0 - wind speed just above the canopy

      PM.EnvModel.RPAR = 0.5;     //RPAR - Fraction of PAR energy to that of the total solar

      if (APSIMCO2) { MyPaddock.Get("CO2", out CO2); }
      
      PM.Canopy.Ca = CO2;     //Ca - Atmsopheric CO2 partial pressure
      PM.Canopy.OxygenPartialPressure = 210000;     //OxygenPartialPressure - Atmsopheric O2 partial pressure
      PM.EnvModel.ATM = 1.01325;     //Air pressure - Air pressure

      //////// Canopy level ////////
      PM.B = 0.409;     //BiomassConversionCoefficient - CO2-to-biomass conversion efficiency

      PM.Canopy.LeafAngle = 60;     //LeafAngle - Leaf angle (with respect to the horizontal plane)
      PM.Canopy.CPath.SLNRatioTop = 1.3;     //SLNRatioTop - Ratio of the specific leaf nitrogen at the top of the canopy to that of the canopy average
      PM.Canopy.CPath.StructuralN = 14;     //StructuralN - Specific leaf nitrogen below which Vcamx, Jmax and Vpmax are zero
      PM.Canopy.DiffuseExtCoeff = 0.78;     //DiffuseExtCoeff - 
      PM.Canopy.DiffuseExtCoeffNIR = 0.8;     //DiffuseExtCoeffNIR - 
      PM.Canopy.LeafScatteringCoeff = 0.15;     //LeafScatteringCoeff - leaf-level scattering coefficient for PAR
      PM.Canopy.LeafScatteringCoeffNIR = 0.8;     //LeafScatteringCoeffNIR - leaf-level scattering coefficient for NIR
      PM.Canopy.DiffuseReflectionCoeff = 0.036;     //DiffuseReflectionCoeff - canopy-level reflection coefficient for diffuse radiation for PAR
      PM.Canopy.DiffuseReflectionCoeffNIR = 0.389;     //DiffuseReflectionCoeffNIR - canopy-level reflection coefficient for diffuse radiation for NIR
      PM.Canopy.LeafWidth = 0.05;     //LeafWidth - Leaf width
      PM.Canopy.Ku = 1.5;     //Ku - canopy wind speed profile coefficient

      //////// Photosynthesis ////////
      PM.Canopy.CPath.PsiVc = 1.1;     //PsiVc - Slope of linear relationship between Vcmax per leaf area at 25°C and specific leaf nitrogen
      PM.Canopy.CPath.PsiJ = 1.85;     //PsiJ - Slope of linear relationship between Jmax per leaf area at 25°C and specific leaf nitrogen
      PM.Canopy.CPath.PsiRd = 0;     //PsiRd - Slope of linear relationship between Rd per leaf area at 25°C and specific leaf nitrogen

      PM.Canopy.Vpr_l = 0;     //Vpr_l - PEP regeneration rate per unit leaf
      PM.Canopy.Theta = 0.7;     //theta - Empirical curvature factor
      PM.Canopy.F = 0.15;     //F - Spectral correction factor
      PM.Canopy.Alpha = 0;     //Alpha - Fraction of PS II activity in the bundle sheath
      PM.Canopy.CPath.Phi = 2;     //Phi - extra energy (ATP) cost required from processes other than the C3 cycle
      PM.Canopy.CPath.X = 0.4;     //X - Fraction of electron transport partitioned to mesophyll chloroplasts
      PM.Canopy.Constant = 0.047;     //Constant - 

      //////// Leaf gas diffusion ////////
      PM.Canopy.CPath.CiCaRatio = 0.7;     //CiCaRatio - ratio of intercellular to air CO2 partial pressure
      PM.Canopy.CPath.PsiGm = 0.00412;     //PsiGm - Slope of linear relationship between gm per leaf area at 25°C and specific leaf nitrogen
      //reduced to 0.00412 (AW, 2019.09.12)
      PM.Canopy.Gbs_CO2 = 0.003;     //Gbs_CO2 - Bundle-sheath conductance for CO2 per leaf area

      //////// Temperature response ////////
      PM.Canopy.CPath.KcP25 = 273.422964228666;     //KcP25
      PM.Canopy.CPath.KcTEa = 93720;     //KcEa
      PM.Canopy.CPath.KoP25 = 165824.064155384;     //KoP25
      PM.Canopy.CPath.KoTEa = 33600;     //KoEa

      PM.Canopy.CPath.VcTEa = 65330;     //VcmaxEa
      PM.Canopy.CPath.VcMax_VoMaxP25 = 4.59217066521612;     //Vcmax_VomaxP25
      PM.Canopy.CPath.VcMax_VoMaxTEa = 35713.1987127717;     //Vcmax_VomaxEa

      PM.Canopy.CPath.RdTEa = 46390;     //RdEa

      PM.Canopy.CPath.JTMin = 0;     //JTMin
      PM.Canopy.CPath.JTOpt = 30;     //JTOpt
      PM.Canopy.CPath.JTMax = 45;     //JTMax
      PM.Canopy.CPath.JMaxC = 0.911017958600129;     //JMaxC
      PM.Canopy.CPath.JBeta = 1;     //Jbeta

      PM.Canopy.CPath.GmTMin = 0;     //GmTMin
      PM.Canopy.CPath.GmTOpt = 29.2338417788683;     //GmTOpt
      PM.Canopy.CPath.GmTMax = 45;     //GmTMax
      PM.Canopy.CPath.GmC = 0.875790608584141;     //GmMaxC
      PM.Canopy.CPath.GmBeta = 1;     //Gmbeta

      //////// Constants ////////
      PM.Canopy.Sigma = 0.0000000567;     //
      PM.Canopy.Rcp = 1200;     //Volumetric heat capacity of air

      PM.Canopy.G = 0.066;     //Psychrometric constant
      PM.Canopy.Lambda = 2447000;     //Latent heat of vapourisation of water

      //Set the Psi values using PsiFactor
      PM.Canopy.CPath.PsiVc *= PsiFactor;
      PM.Canopy.CPath.PsiJ *= PsiFactor;
      PM.Canopy.CPath.PsiRd *= PsiFactor;
      PM.Canopy.CPath.PsiVp *= PsiFactor;

      PM.EnvModel.Initialised = true;
      PM.Initialised = true;

      //Set the LAI trigger and keep initial Ps model name    
      MyPaddock.Set("DCaPSTTriggerLAI", LAITrigger);
      MyPaddock.Get("PsModelName1", out PsModelName1);       
   }
   
   [EventHandler] public void OnPrepare()
   {    
      RootShoot = 0;
      BIOshootDAY = 0;
      BIOtotalDAY = 0;
      EcanDemand = 0; 
      EcanSupply = 0;
      RadIntDcapst = 0;
      RUE = 0;
      TE = 0;
      BIOshootDAYPot = 0;
      iter = 0;
      
      for(int i = 0; i &lt; 5; i++) { dcapst[i] = -1.0f;}      
              
      if (!DCaPSTTriggered) {
         if (Shift2PsModel == "dcapstold") {
            DCaPSTTriggered = true;
            MyPaddock.Set("PsModelName", Shift2PsModel); 
         
         } else {
         
            MyPaddock.Get("lai", out lai);          
            if (lai &gt;= LAITrigger) {
               DCaPSTTriggered = true;
               MyPaddock.Set("PsModelName", Shift2PsModel); 
            }           
         }  
      }
   }
   
   // This routine is called when the plant model wants us to do the calculation
   [EventHandler] public void Ondodcapst() {
      //      ApsimC3PhotoLink p = new ApsimC3PhotoLink();
     
      MyPaddock.Get("latitude", out latitude);
      MyPaddock.Get("maxT", out maxT);
      MyPaddock.Get("minT", out minT);
      MyPaddock.Get("radn", out radn);
      MyPaddock.Get("lai", out lai); 
      
      if (Shift2PsModel == "dcapst") {
         MyPaddock.Get("DCAPSTDOY", out DOY);      
         MyPaddock.Get("DCAPSTsln", out SLN);
         MyPaddock.Get("DCAPSTRootShootRatio", out RootShootRatio);
         MyPaddock.Get("DCAPSTswAvail", out SWAvailable);
         
      } else {
         MyPaddock.Get("DCAPSTOldDOY", out DOY);      
         MyPaddock.Get("DCAPSTOldsln", out SLN);
         MyPaddock.Get("DCAPSTOldRootShootRatio", out RootShootRatio);
         MyPaddock.Get("DCAPSTOldswAvail", out SWAvailable);
      }
 
              
      //Check
      //Console.WriteLine("DCaps Input Manager--------------");
      //Console.WriteLine("DOY = " + DOY.ToString());
      //Console.WriteLine("latitude = " + latitude.ToString());
      //Console.WriteLine("maxT = " + maxT.ToString());
      //Console.WriteLine("minT = " + minT.ToString());
      //Console.WriteLine("radn = " + radn.ToString());
      //Console.WriteLine("RootShootRatio = " + RootShootRatio.ToString());
      //Console.WriteLine("SLN = " + SLN.ToString());
      //Console.WriteLine("SWAvailable = " + SWAvailable.ToString());
      //Console.WriteLine("lai = " + lai.ToString());
      //Console.WriteLine("DCaps Input Manager--------------");      
        
      dcapst = PM.RunApsim(DOY, latitude, maxT, minT, radn, lai, SLN, SWAvailable, RootShootRatio);
      
      //Additional outputs
      RootShoot = RootShootRatio;
      BIOshootDAY = dcapst[0];
      BIOtotalDAY = BIOshootDAY * (1 + RootShoot);
      EcanDemand = dcapst[1]; 
      EcanSupply = dcapst[2];
      RadIntDcapst = dcapst[3];
      RUE = (RadIntDcapst == 0 ? 0 : BIOshootDAY / RadIntDcapst);
      TE = (EcanSupply == 0 ? 0 : BIOshootDAY / EcanSupply); 
      BIOshootDAYPot = dcapst[4];
      
      //Check Outputs
      //      Console.WriteLine("DCapst Out Manager--------------");
      //      Console.WriteLine("1 = " + dcapst[0].ToString());
      //      Console.WriteLine("2 = " + dcapst[1].ToString());
      //      Console.WriteLine("3 = " + dcapst[2].ToString());
      //      Console.WriteLine("4 = " + dcapst[3].ToString());
      //      Console.WriteLine("DCapst Out Manager--------------");
      //      
   }      
   
   [EventHandler] public void OnSowing() {
      DCaPSTTriggered = false;
      MyPaddock.Set("PsModelName", PsModelName1); 
   }
}
