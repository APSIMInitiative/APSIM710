------- Sow WEEDS using a variable rule with intercropping Initialisation -----
     Manager rules:
     
     SECTION:- init
              next = 1
              crop_in = 0
              blank = 'fallow'
              next_crop = blank
              crop_current = blank
              crop_current1 = blank
              crop_current2 = blank
              crop_sow = blank
              ready_for_next = 1
              ready_to_fert_pre_sow = 0
              days_last_year = 365
              fert_day_pre_sow = 0
              ready_to_till_pre_sow = 0
              till_day_pre_sow = 0
              days_after_weed_kill = 0
     
     SECTION:- process
        if (next_crop = 'weed' or next_crop = blank or 'yes' = 'yes') then
           if (today = date('30-apr')) then
              if ('yes' = 'no') then
                 ready_for_next = 1
              endif
           endif
           if (wd_sow_opp_num > 0) then
              wd_sow_opp_days = wd_sow_opp_days + 1
           else
           endif
           if (crop_in = 0 or ('intercrop' = 'intercrop'
               and weed.plant_status = 'out')
               and days_after_weed_kill > 0) then
                 if (date_within('01-oct, 30-apr') = 1) then
                    crop_sow = 'waiting'
                    surface_moist = (sw(1) - ll15(1)) / (dul(1)- ll15(1))
                    if ((rain[5] >= 10  and  esw >= 5 and
                          (surface_moist <= 2 and surface_moist >= 0)) or
                        ('no' = 'yes' and today = date('30-apr'))) then
                       if (wd_sow_opp_days >= 5 or wd_sow_opp_num = 0) then
                          wd_sow_opp_num = wd_sow_opp_num + 1
                          wd_sow_opp_days = 0
                       endif
                       if ((wd_sow_opp_num >= 1) or ('no' = 'yes' and today = date('30-apr')) then
                          if (date_within('01-oct, 1-jul') = 1) then
                             weed sow crop_class =  summer_grass, plants = 5 (plants/m2), sowing_depth = 15, cultivar =  early, row_spacing = 350
                          else
                             weed sow crop_class =  summer_grass, plants = 5 (plants/m2), sowing_depth = 15, cultivar = late, row_spacing = 350
                          endif
                          crop_in = crop_in + 1
                          if ('yes' = 'no') then
                             ready_for_next = 1
                          endif
                          crop_current2 = crop_current1
                          crop_current1 = crop_current
                          crop_current = 'weed'
                          crop_sow = 'weed'
                       else
                       endif
                 endif
              endif
           endif
        endif
     
     SECTION:- end_of_day
       days_this_year = 364 + (date('1-mar') - date('28-feb'))
        days_after_weed_kill = days_after_weed_kill + 1
        if (next_crop = 'weed' or next_crop = blank or 'yes' = 'yes') then
           if ( 0 > 0) then
              till_day_pre_sow = date('01-oct') - 0
              if (till_day_pre_sow < today) then
                 till_day_pre_sow = days_this_year + till_day_pre_sow
              endif
              if (today = till_day_pre_sow) then
                 ready_to_till_pre_sow = 1
              endif
              if ((crop_in = 0 or 'no' = 'yes') and (ready_to_till_pre_sow = 1)) then
                 if (weed.plant_status <> 'out') then
                    weed harvest, remove=0
                    weed kill_crop
                    weed end_crop
                    crop_in = crop_in - 1
                   if (crop_current = 'weed') then
                       crop_current = crop_current1
                       crop_current1 = crop_current2
                       crop_current2 = blank
                    elseif (crop_current1 = 'weed') then
                       crop_current1 = crop_current2
                       crop_current2 = blank
                    elseif (crop_current2 = 'weed') then
                       crop_current2 = blank
                    endif
                 endif
              endif
           endif
        endif
        if (date_within('01-oct, 30-apr') = 1) then
        else
           wd_sow_opp_num = 0
           wd_sow_opp_days = 0
        endif
     END of rules
     
     Manager creating a new local real variable : fert_day_pre_sow = 0.00000

------- Output at harvesting for intercropping Initialisation -----------------
     Manager rules:
     
     SECTION:- end_of_day
              if (maize_to_harvest = 1) then
     'outputfile' do_output
              endif
     END of rules
     

------- Harvesting rule for intercropping Initialisation ----------------------
     Manager rules:
     
     SECTION:- init
               maize_to_harvest = 0
     
     SECTION:- end_of_day
              if (maize_to_harvest = 1) then
     maize harvest, remove=0.95
     maize kill_crop
     maize end_crop
           crop_in = crop_in - 1
     maize_to_harvest = 0
     fert_tot = 0
     sow_year = 0
     if (crop_current = 'maize') then
        crop_current = crop_current1
        crop_current1 = crop_current2
        crop_current2 = blank
     elseif (crop_current1 = 'maize') then
        crop_current1 = crop_current2
        crop_current2 = blank
     elseif (crop_current2 = 'maize') then
        crop_current2 = blank
     endif
              endif
              if (maize.stagename = 'harvest_ripe' or maize.plant_status = 'dead') then
     maize_to_harvest = 1
              endif
     END of rules
     
     Manager creating a new local real variable : maize_to_harvest = 0.00000

------- Weeding at threshold weed biomass or maximum days Initialisation ------
     Manager rules:
     
     SECTION:- init
               daemerge = 0
     
     SECTION:- end_of_day
              if ((crop_in >= 1)
     and (weed.plant_status <> 'out')) then
     if (weed.stage >= 3.0) then
        daemerge = daemerge + 1
     endif
     if (weed.biomass > 5000 or daemerge >= 35) then
        if ((crop_in > 1 and 1 > weed_count_in) or (crop_in = 1 and 0 > weed_count_out)) then
           if (crop_in > 1) then
              weed_count_in = weed_count_in + 1
           elseif (crop_in = 1) then
              weed_count_out = weed_count_out + 1
           endif
           weed harvest, remove = 0
           weed kill_crop plants_kill_fraction = 1
           if (1 = 1.0) then
              weed end_crop
              days_after_weed_kill = 0
          else
           endif
           'surface organic matter' tillage type = user_defined, f_incorp = 0.1, tillage_depth = 25
           if (1 = 1.0) then
             crop_in = crop_in - 1
             if (crop_current = 'weed') then
                 crop_current = crop_current1
                 crop_current1 = crop_current2
                 crop_current2 = blank
              elseif (crop_current1 = 'weed') then
                 crop_current1 = crop_current2
                 crop_current2 = blank
              elseif (crop_current2 = 'weed') then
                 crop_current2 = blank
              endif
              daemerge = 0
           else
           endif
        else
        endif
     else
     endif
              else
     daemerge = 0
              endif
              if (((crop_in = 1)
     and (weed.plant_status <> 'out'))
     or (crop_in = 0)) then
     weed_count_in = 0
              else
     weed_count_out = 0
              endif
     END of rules
     
     Manager creating a new local real variable : daemerge = 0.00000

------- Remove all residues on a fixed date Initialisation --------------------
