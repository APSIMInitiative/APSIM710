     --->Section: init

onFlowering<-function (args) {
   cat ("RLink: On flowering called\n")
}

seen <- FALSE
onNewMet<-function (args) {
   if (!seen) {
     cat ("RLink: new met event\n")
     for (n in names(args)) {
       cat (n,args[[n]],"\n")
     }
     seen <<- TRUE
   }
}

apsim$subscribe("flowering", "Null", "onFlowering")

apsim$subscribe("newmet", "NewMet", "onNewMet")

     --->Section: post

if (apsim$get("wheat.StageName") == "harvest_ripe" || apsim$get("wheat.plant_status") == "dead") {
   apsim$publish("wheat.harvest",  "Harvest", list(Remove=1.0))
   apsim$publish("wheat.end_crop", "Null", list())  
}

     --->Section: prepare

# getting and setting simple apsim variables:
year<-apsim$get("year")

today<-as.Date(paste(apsim$get("dd_mmm"), year, sep="_"), "%d_%b_%Y")
if (as.Date("10-jul", "%d-%b") < as.Date("15-may", "%d-%b")) {
  w1 <- as.Date(paste("15-may",year,sep="-"), "%d-%b-%Y")
  w2 <- as.Date(paste("10-jul",year+1,sep="-"), "%d-%b-%Y")
} else {
  w1 <- as.Date(paste("15-may",year,sep="-"), "%d-%b-%Y")
  w2 <- as.Date(paste("10-jul",year,sep="-"), "%d-%b-%Y")
}

# Publishing apsim events. NB named list items are case sensitive.
if (apsim$get("wheat.plant_status") == "out" && today >= w1 && today <= w2) {
   if ((apsim$get("rain[7]") > 25 && apsim$get("esw") >= 100) ||
       ("yes" == "yes" && today == w2)) {
      apsim$publish("wheat.sow", "Sow", list(plants=100, sowing_depth=30, Cultivar ="hartog", row_spacing = 250, crop_class = "plant"))
      apsim$publish("fertiliser.apply", "FertiliserApplication", list(Amount = 150, Depth = 50, Type = "urea_N"))
   } 
} 
            
     --->End
