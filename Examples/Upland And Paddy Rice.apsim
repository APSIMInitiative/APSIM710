        <manager name="Rice sowing rule">
          <script name="init">
            <text>
!Inital variables
Irrigation_amount = 0
evap_amount = 0</text>
            <event>init</event>
          </script>
          <script name="start_of_day">
            <text>!Upland Aus
                                      ! plant 25th Mar (Mar-July)
if (day = 84) then
    rice sow cultivar = local, establishment = direct-seed, nplds = 180 (p/m2)
endif
</text>
            <event>start_of_day</event>
          </script>
          <script name="end_of_day">
            <text>
if (rice.plant_status = 'dead') then
  rice end_crop
  Irrigation_amount = 0                                                   !reset  counters
  evap_amount = 0
                                                                                           !remove stubble
  SurfaceOM tillage type = burn, remove = 0.95
endif


                                                                                           !counters
 Irrigation_amount = Irrigation_amount + irrig_tot
 evap_amount = evap_amount + es

</text>
            <event>end_of_day</event>
          </script>
        </manager>
        <manager name="Fertilise_sowing">
          <ui>
            <category description="When should fertiliser be applied" />
            <fert_date type="ddmmmdate" description="Enter fertiliser date (dd-mmm) : ">15-aug</fert_date>
            <fert_criteria type="text" description="Don't add fertiliser if N in top 2 layers exceeds (kg/ha) : ">1000</fert_criteria>
            <category description="Fertiliser application details" />
