        <manager name="Rice sowing rule">
          <script name="init">
            <text>
!Inital variables
Irrigation_amount = 0
evap_amount = 0</text>
            <event>init</event>
          </script>
          <script name="start_of_day">
            <text>!Transplanted Aman
                                      ! plant 1st Aug (July-Dec)
! Transplant
if (day = 213) then                                                                    !sbdur= duration of seedbed
                                                                                                    !nplh= number of plants on hills
                                                                                                    !nh= number of hills
    rice sow cultivar = BR23, establishment = transplant, sbdur = 22, nplh = 4 , nh = 28, nplsb = 112
endif

</text>
            <event>start_of_day</event>
          </script>
          <script name="end_of_day">
            <text>
if (rice.plant_status = 'dead') then
  rice end_crop
  Irrigation_amount = 0                                                   !reset  counters
  evap_amount = 0
                                                                                           !remove stubble
  SurfaceOM tillage type = burn, remove = 0.95
endif


                                                                                           !counters
 Irrigation_amount = Irrigation_amount + irrig_tot
 evap_amount = evap_amount + es

</text>
            <event>end_of_day</event>
          </script>
        </manager>
