          <script name="start_of_day">
            <text>!Transplanted Aus
                                      ! plant 25th Apr (Apr-Aug)
! Transplant
if (day = 91) then                                                                    !sbdur= duration of seedbed
                                                                                                    !nplh= number of plants on hills
                                                                                                    !nh= number of hills
          rice sow cultivar = BR3, establishment = transplant, sbdur = 45, nplh = 25 , nh = 7, nplsb = 180
endif

!Transplanted Aman
                                      ! plant 20th Aug (July-Dec)
! Transplant
if (day = 213) then                                                                    !sbdur= duration of seedbed
                                                                                                    !nplh= number of plants on hills
                                                                                                    !nh= number of hills
      rice sow cultivar = BR23, establishment = transplant, sbdur = 44, nplh = 4 , nh = 28, nplsb = 112
endif

</text>
