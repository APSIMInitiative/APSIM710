      <manager name="Watering">
        <script name="init">
          <text>

max_pond = 0.0
pond_depth = 0.0
top_dress = 0
          </text>
          <event>init</event>
        </script>
        <script name="start_of_day">
          <text><![CDATA[!

if day = 1 then 
'Soil Water' set max_pond = 50
endif


if rice.plant_status = 'alive' and pond_depth < 50 then
irrig_amount = 50-pond_depth
Irrigation apply amount = irrig_amount
tot_irrig = tot_irrig + irrig_amount
!fertiliser apply amount = 10, type=urea_n
endif

if (day = 288) then
  rice sow cultivar = riverina, establishment = direct-seed, nplds = 180 (p/m2)
fertiliser apply amount = 200, type=urea_n
endif

if rice.dvs >0.3 and top_dress = 0 then
  fertiliser apply amount = 800, type = no3_n
  top_dress = 1
endif

!if (day = 288) then
!  rice sow cultivar = riverina, establishment = transplant, sbdur = 50, nplh = 10 , nh = 2, nplsb = 100
!endif

if (rice.plant_status = 'dead') then
  rice end_crop
  tot_irrig = 0
  top_dress = 0
endif
]]></text>
          <event>start_of_day</event>
        </script>
        <script name="end_of_day">
          <text>

pond_depth = pond
</text>
          <event>end_of_day</event>
        </script>
