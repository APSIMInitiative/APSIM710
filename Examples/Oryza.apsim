        <manager name="Rice sowing rule">
          <script name="init">
            <text>
</text>
            <event>init</event>
          </script>
          <script name="start_of_day">
            <text>if (day = 288) then
  rice sow cultivar = experimental, establishment = direct-seed, nplds = 180 (p/m2)
endif
</text>
            <event>start_of_day</event>
          </script>
          <script name="end_of_day">
            <text>
if (rice.plant_status = 'dead') then
  rice end_crop
endif</text>
            <event>end_of_day</event>
          </script>
        </manager>
