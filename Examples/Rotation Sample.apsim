          <text>using System;
using ModelFramework;

// This script accumulates yields and biomass over a period. It is useful when 
// wanting to accumulate the total biomass produced across a sequence of crops 
// or the total transpiration within a phase of a rotation. 
// It's not very useful for intercrops.

public class Script 
   {
   [Link()] public Paddock MyPaddock;  
   [Output, Description("Crop Yield"), Units("kg/ha")] public double yld_kgha {
      get{
         double yld2 = 0;
         foreach (Component crop in MyPaddock.Crops)
         {
            double yld = 0;
            crop.Get("yield", out yld);
            yld2 = yld2 + yld;
         }
         return yld2 ;}
   }
  
   [Output, Description("Crop Biomass"), Units("kg/ha")] public double biomass_kgha {
      get{
         double bio2 = 0;
         foreach (Component crop in MyPaddock.Crops)
         {
            double bio = 0;
            crop.Get("biomass", out bio);
            bio2 = bio2 + bio;
         }
         return bio2 ;}
   }

   [Output, Description("Plant Transpiration"), Units("mm")] public double SEp {
      get{
         double EpSum = 0;
         foreach (Component crop in MyPaddock.Crops)
         {
            double Ep = 0.0;
            crop.Get("Ep", out Ep);
            EpSum = EpSum + Ep;
         }
         return EpSum ;}
   }

   [Output, Description("Grain Protein"), Units("%")] public double protein {
   get{
      double pro = 0;
      double pro2 = 0;
      foreach (Component crop in MyPaddock.Crops)
      {
         crop.Get("grain_protein", out pro);
         pro2 = pro2 + pro;
      }
        
      return pro2 ;}
   }
   
   [Output, Description("Grain N"), Units("g/m2")] public double GrainN_gm2 {
      get{
         double GN2 = 0;
         foreach (Component crop in MyPaddock.Crops)
         {
            double GN = 0;
            if (crop.Get("grain_N", out GN))
               GN2 = GN2 + GN;
            else if (crop.Get("GrainGreenN", out GN))
               GN2 = GN2 + GN;
         }
         return GN2 ;}
   }
}
