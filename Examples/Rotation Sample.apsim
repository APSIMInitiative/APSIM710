          <text>using System; 
using ModelFramework; 
using CSGeneral;

// This captures and stores the inital state of sw_dep and NO3 at the begining of a 
// transition within a rotation, this allows the start and end conditions 
// to be reported on a single report line. 
 
public class Script 
{ 
   [Link()] public Paddock myPaddock;   
 
   [Output, Description("The soil water on the first day of a rotation"), Units("mm")] public double swStart = 0.0;
   [Output, Description("The soil water on the last day of a rotation"), Units("mm")] public double swYesterday = 0.0;    //this is a way of capturing the day befores soil water to use in a water balance calculation
   [Output, Description("The soil NO3 on the first day of a rotation"), Units("kg/ha")] public double no3Start = 0.0;     //this is the soil nitrogen on the final day of a transition
   [Output, Description("The soil NO3 on the last day of a rotation"), Units("kg/ha")] public double no3Yesterday = 0.0; //this is a way of capturing the day befores soil nitrogen to use in a nitrogen balance calculation
   [Output, Description("The number of days in a rotation"), Units("")] public int daysInPhase = 0;
   
   [EventHandler] public void OnStart_Simulation() { 
      double [] swdep = new double[15];
      double [] no3dep = new double[15];
      myPaddock.Get("sw_dep()", out swdep);
      myPaddock.Get("no3()", out no3dep);
      swYesterday = swStart = MathUtility.Sum(swdep);  
      no3Yesterday = no3Start = MathUtility.Sum(no3dep);
   } 
   [EventHandler] public void OnTransition() {
      swStart = swYesterday; no3Start = no3Yesterday; 
      daysInPhase = 0;
   } 
   [EventHandler] public void OnPost() { 
      double [] swdep = new double[15];
      myPaddock.Get("sw_dep()", out swdep);
      swYesterday = MathUtility.Sum(swdep);
      double [] no3dep = new double[15];
      myPaddock.Get("no3()", out no3dep);
      no3Yesterday = MathUtility.Sum(no3dep);
      daysInPhase++;
   } 
} 
</text>
