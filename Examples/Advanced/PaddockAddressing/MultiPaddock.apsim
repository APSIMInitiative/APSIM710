      <ui />
      <text>Imports System
Imports ModelFramework

Public Class Script
   
   &lt;Link()&gt; Dim MySimulation As Simulation
   
   &lt;EventHandler()&gt; Public Sub OnPost()

      ' Loop through all child paddocks and change the soil water in each.
      For Each SubPaddock As Paddock In MySimulation.ChildPaddocks
         Dim SoilWater As SoilWat = SubPaddock.LinkByType("SoilWat")
         Dim sw As Single() = SoilWater.sw
         Console.WriteLine(SubPaddock.FullName + "-" + sw(0).ToString())
         SoilWater.sw = sw
      Next

      ' Loop through all child paddocks and write the names and LAIs of all crops.
      For Each SubPaddock As Paddock In MySimulation.ChildPaddocks
         For Each Crop As Component In SubPaddock.Crops
            Console.WriteLine(Crop.Name)
         Next
      Next

      ' Loop through all child paddocks and apply fertilier
      ' And irrigation to each paddock.
      For Each SubPaddock As Paddock In MySimulation.ChildPaddocks
         Dim Fertiliser As Fertiliser = SubPaddock.LinkByType("Fertiliser")
         Dim Irrigation As Irrigation = SubPaddock.LinkByType("Irrigation")
         Dim fert As FertiliserApplicationType = New FertiliserApplicationType()
         fert.Amount = 50
         fert.Depth = 10
         fert.Type = "NO3_N"
         Fertiliser.Apply(fert)
         Dim irrig As IrrigationApplicationType = New IrrigationApplicationType()
         irrig.Amount = 50
         irrig.Duration = 0
         irrig.time = ""
         ReDim irrig.source(-1)
         irrig.Crop_Area = 0
         irrig.NO3 = 0
         irrig.NH4 = 0
         irrig.CL = 0
         Irrigation.Apply(irrig)
      Next
   End Sub


End Class
    </text>
