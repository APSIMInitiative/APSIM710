proc doPrepareAnimals {} {
   global animals
   
#####Culls   
   if {[dateIs 15-aug]} {
     set animals(culls) [animalsIn 100 culls 500]    ;# 100 head @ 500kg
     moveStockTo $animals(culls) p11 buffel          ;# move onto p11 buffel
   }
   
   if {[dateIs 15-dec] && 
       $animals(culls) != {} && 
       [animalWeight $animals(culls)] > 550} {
     animalsOut $animals(culls) all
     set animals(culls) {}
   }

   if {[dateIs 15-jun] && $animals(culls) != {}} {
     animalsOut $animals(culls) all
     set animals(culls) {}
   }
   
##### Weaners on oats for 6 mths
   if {[dateIs 15-aug] } {

     set animals(steers1) [animalsIn 275 steers 200]   ;# 275 head @ 200Kg
     if {[OatsIsPlanted p10]} {
       moveStockTo $animals(steers1) p10 oats           ;# move onto oats in p10
     } else { 
       puts "Couldn't put weaners on oats in p10"
       moveStockTo $animals(steers1) p13 buffel         ;# move onto buffel in p13
       setSupplementsOn $animals(steers1)
     }

	 set animals(steers3) [animalsIn 200 steers 200]   ;# 275 head @ 200Kg
     if {[OatsIsPlanted p1]} {
       moveStockTo $animals(steers3) p1 oats           ;# move onto oats in p10
     } else { 
       puts "Couldn't put weaners on oats in p1"
       moveStockTo $animals(steers3) p13 buffel         ;# move onto buffel in p13
       setSupplementsOn $animals(steers3)
     }

     set animals(heifers) [animalsIn 275 heifers 200]
     if { [OatsIsPlanted p20]} {
       moveStockTo $animals(heifers) p20 oats
     } else { 
       puts "Couldn't put weaners on oats in p20"
       moveStockTo $animals(heifers) p13 buffel         ;# move onto buffel in p13 (shared with steers)
       setSupplementsOn $animals(heifers)
     }
   }
   
   # Oats are flogged, but sorghum isn't ready 
   if {$animals(steers1) != {} &&
       [animalWhat $animals(steers1)] == "oats" && 
       ([apsimGet p10.oats.stage] > 8 || [apsimGet p10.oats.biomass] < 500) &&
       [apsimGet .masterpm.p16.SweetSorghum.biomass] < 1000} {
       moveStockTo $animals(steers1) p13 buffel
       setSupplementsOn $animals(steers1)
   }     
   if {$animals(steers3) != {} &&
       [animalWhat $animals(steers3)] == "oats" && 
       ([apsimGet p1.oats.stage] > 8 || [apsimGet p1.oats.biomass] < 500) &&
       [apsimGet .masterpm.p2.SweetSorghum.biomass] < 1000} {
       moveStockTo $animals(steers3) p13 buffel
       setSupplementsOn $animals(steers3)
   }     
   if {$animals(heifers) != {} &&
       [animalWhat $animals(heifers)] == "oats" && 
       ([apsimGet p20.oats.stage] > 8 || [apsimGet p20.oats.biomass] < 500) &&
       [apsimGet .masterpm.p18.SweetSorghum.biomass] < 1000} {
       moveStockTo $animals(heifers) p13 buffel
       setSupplementsOn $animals(heifers)
   }     


### mid-jan - split weaners and sell if feed scarce
   if {$animals(steers1) != {} && $animals(steers2) == {} &&
       (([animalWhat $animals(steers1)] == "buffel" &&
         [apsimGet .masterpm.p16.SweetSorghum.biomass] > 1000) ||
        [dateIs 15-jan])} {

        moveStockTo $animals(steers1) p16 sorghum
        setSupplementsOff $animals(steers1)
        set animals(steers2) [splitMob $animals(steers1) 135 p19 sorghum]


        if {![ForageSorghumIsPlanted p16] || [apsimGet .masterpm.p16.SweetSorghum.biomass] < 800} {
           animalsOut $animals(steers1) all
           set animals(steers1) {}
        }

        if {![ForageSorghumIsPlanted p19] || [apsimGet .masterpm.p19.SweetSorghum.biomass] < 800} {
           animalsOut $animals(steers2) all
           set animals(steers2) {}
        }
   }

   if {$animals(steers3) != {} &&
       (([animalWhat $animals(steers3)] == "buffel" &&
         [apsimGet .masterpm.p2.SweetSorghum.biomass] > 1000) ||
        [dateIs 15-jan])} {

        moveStockTo $animals(steers3) p2 sorghum
        setSupplementsOff $animals(steers3)

        if {![ForageSorghumIsPlanted p2] || [apsimGet .masterpm.p2.SweetSorghum.biomass] < 800} {
           animalsOut $animals(steers3) all
           set animals(steers3) {}
        }

   }
   
   if {$animals(heifers) != {} && $animals(breeders) == {} && 
       (([animalWhat $animals(heifers)] == "buffel" &&
         [apsimGet .masterpm.p18.SweetSorghum.biomass] > 1000) ||
        [dateIs 15-jan])} {

       moveStockTo $animals(heifers) p18 sorghum
       setSupplementsOff $animals(heifers)
       set animals(breeders) [splitMob $animals(heifers) 120 p12 buffel]

puts "[apsimGet dd/mm/yyyy]:animals:heifers sorgwt = [apsimGetOptional .masterpm.p18.SweetSorghum.biomass]"
       if {![ForageSorghumIsPlanted p18] || [apsimGet .masterpm.p18.SweetSorghum.biomass] < 800} {
           animalsOut $animals(heifers) all
           set animals(heifers) {}
       }
   }

   if {$animals(breeders) != {}} {
      if {[animalWhat $animals(breeders)] == "buffel" &&
          [apsimGet p12.buffel.tsdm] < 500} {
         animalsOut $animals(breeders) all
         set animals(breeders) {}
      }  
   }

   
   # Off to the feedlot
   if {$animals(steers1) != {} && 
       [animalWhat $animals(steers1)] == "sorghum" &&
       ([animalWeight $animals(steers1)] > 450 || [dateIs 1-apr])} {
       moveStockTo $animals(steers1) "" feedlot
   }

   if {$animals(steers2) != {} && 
       [animalWhat $animals(steers2)] == "sorghum" &&
       ([animalWeight $animals(steers2)] > 450 || [dateIs 1-apr])} {
       moveStockTo $animals(steers2) "" feedlot
   }

   if {$animals(steers3) != {} && 
       [animalWhat $animals(steers3)] == "sorghum" &&
       ([animalWeight $animals(steers3)] > 450 || [dateIs 1-apr])} {
       moveStockTo $animals(steers3) "" feedlot
   }
   
   if {$animals(heifers) != {} && 
       [animalWhat $animals(heifers)] == "sorghum" &&
       ([animalWeight $animals(heifers)] > 450 || [dateIs 1-apr])} {
       moveStockTo $animals(heifers) "" feedlot
   }
   
   # Off the farm after 
   foreach a {steers1 steers2 steers3 heifers} {
      if {$animals($a) != {} && 
          ([animalDaysIn $animals($a) "feedlot"] == 70 || [dateIs 1-aug])} {
        animalsOut $animals($a) all
        set animals($a) {}
      }
   }

   if {$animals(breeders) != {} && [dateIs 1-aug]} {
     animalsOut $animals(breeders) all
     set animals(breeders) {}
   }
   
   # Work out feedlot costs
   set nInFeedlot 0
   foreach a {steers1 steers2 steers3 heifers} {
      if {$animals($a) != {} && 
          [animalWhat $animals($a)] == "feedlot"} {
         set nInFeedlot [expr $nInFeedlot + [animalNumber $animals($a)]]
      }
   }

   if {$nInFeedlot > 0} {
      apsimSendMessage economics expenditure {paddock livestock}\
             "cost [expr $nInFeedlot * $animals(dailyFeedlotCost)]" \
             [list comment "feedlot costs for $nInFeedlot beasts"]
   }

   # Work out supplement costs
   set nSupplemented 0
   foreach a {steers1 steers2 steers3 heifers} {
      if {$animals($a) != {} && 
          $animals($animals($a),supplemented)} {
         set nSupplemented [expr $nSupplemented + [animalNumber $animals($a)]]
      }
   }

   if {$nSupplemented > 0} {
      apsimSendMessage economics expenditure {paddock livestock}\
             "cost [expr $nSupplemented * $animals(dailySupplementCost)]" \
             [list comment "supplement costs for $nSupplemented beasts"]
      puts "animals: Supplementary feeding for $nSupplemented beasts"
   }
   
}

]]></text>
