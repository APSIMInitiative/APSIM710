proc canPlant_ForageSorghum {paddock} {
   global config rainList
   if {[info exists config($paddock,HarvestedForageSorghumDay)]} {
     if {$config($paddock,HarvestedForageSorghumDay) == [apsimGet day]} {return 0}
   }
   
   if {"no" == "yes" &amp;&amp; [dateIs 15-dec ]} {return 1}

