        <rule name="Kilmore Forage Sorghum Sowing Rule.out - Sowing rule - initialisation" condition="init" invisible="yes"><![CDATA[
# Support procedures
# Can we plant a crop today?
proc canPlant_ForageSorghum {paddock} {
   global config rainList
   if {[info exists config($paddock,HarvestedForageSorghumDay)]} {
     if {$config($paddock,HarvestedForageSorghumDay) == [apsimGet day]} {return 0}
   }
   
   if {"no" == "yes" && [dateIs 15-dec ]} {return 1}

   set dateOK [dateWithin 1-oct 15-dec ]

   set rainOK [expr [sumLastRain 4] >= 20]

   set esw [expr [apsimGet $config($paddock,watBal).sw_dep()] - \
                 [apsimGet $config($paddock,watBal).ll15_dep()]]

   set eswOK [expr $esw >= 60]

   if {"tractor_1" != "<none>"} {
     set machOK [machineryAvailable tractor=tractor_1,implement=planter]
   } else {
     set machOK 1
   }
 
   return [expr $dateOK && $rainOK && $eswOK]
}

# Plant a crop now
proc sow_ForageSorghum {paddock} {
   global config

   apsimSendMessage .masterpm.$paddock.SweetSorghum sow \
           {plants 10} {sowing_depth 50} \
           {cultivar sugargraze} {row_spacing 500} \
           {skiprow 0} 

   if {"tractor_1" != "<none>"} {
     # machinery operation
     apsimSendMessage economics operate \
                   "area $config($paddock,area)" \
                   "tractor tractor_1"\
                   "implement planter"\
                   "paddock $paddock"\
                   "costtype incrop_cost"
   }

   # Sowing costs
   apsimSendMessage economics expenditure \
                   "cost [expr 140.0 * $config($paddock,area)]" \
                   "paddock $paddock" \
                   "costtype incrop_cost" \
                   [list comment "Sowing costs for ForageSorghum in $paddock"]
}


proc ForageSorghumFinished {paddock} {
   if {[apsimGet .masterpm.$paddock.SweetSorghum.StageName] == "harvest_ripe" ||
       [apsimGet .masterpm.$paddock.SweetSorghum.plant_status] == "dead"} {
      return 1
   }
   return 0
}

proc finishForageSorghum {paddock} {
  apsimSendMessage .masterpm.$paddock.SweetSorghum end_crop
global config
set config($paddock,HarvestedForageSorghumDay) [apsimGet day]
}

proc ForageSorghumIsPlanted {paddock} {
   set status [apsimGetOptional .masterpm.$paddock.SweetSorghum.plant_status]
   return [string match "$status" "alive"]
}

]]></rule>
