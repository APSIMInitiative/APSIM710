                   "paddock $paddock"\
                   "costtype incrop_cost"
   }

   # Sowing costs
   apsimSendMessage economics expenditure \
                   "cost [expr [sowing_costs] * $config($paddock,area)]" \
                   "paddock $paddock" \
                   "costtype incrop_cost" \
                   [list comment "Sowing costs for [state] in $paddock"]
}


proc [state]Finished {paddock} {
   if {[apsimGet .masterpm.$paddock.[crop].StageName] == "harvest_ripe" ||
       [apsimGet .masterpm.$paddock.[crop].plant_status] == "dead"} {
      return 1
   }
   return 0
}

proc finish[state] {paddock} {

  apsimSendMessage .masterpm.$paddock.[crop] kill_crop
  apsimSendMessage .masterpm.$paddock.[crop] end_crop
}

proc [state]IsPlanted {paddock} {
   set status [apsimGetOptional .masterpm.$paddock.[crop].plant_status]
   return [string match "$status" "alive"]
}










