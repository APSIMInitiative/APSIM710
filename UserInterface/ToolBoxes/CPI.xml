  <CPIManager>
    <initsection>
      <init name="published_events" array="T" />
      <init name="subscribed_events" array="T" />
      <init name="driver_connections" array="T" />
      <init name="rules" kind="string" array="T">
        <val>!This is an example script that is used in AusFarm                                         </val>
        <val />
        <val>define real    stocking_rate = 10.0  ! wethers/ha                                          </val>
        <val />
        <val>! Replacement                                                                              </val>
        <val />
        <val>define real    no_to_buy                                                                   </val>
        <val>define integer i                                                                           </val>
        <val />
        <val>each 1 Jan                                                                                 </val>
        <val>{ for i = 1 to stock.no_groups                                                             </val>
        <val>    stock.sell group=i, number=0.25*stock.number[i]                                        </val>
        <val />
        <val>  set no_to_buy = stocking_rate * paddock1.area - stock.number_all                         </val>
        <val>  stock.buy genotype='Small Merino', number=no_to_buy, sex='wethers', age=12.0, weight=45.0</val>
        <val>  stock.tag group=i, value=i+1                                                             </val>
        <val>  stock.move group=i, paddock='paddock1'                                                   </val>
        <val>}                                                                                          </val>
        <val />
        <val />
        <val>! Shearing                                                                                 </val>
        <val />
        <val>each 15 Dec                                                                                </val>
        <val>  stock.shear                                                                              </val>
        <val />
        <val>! Supplementary feeding                                                                    </val>
        <val />
        <val>if stock.cond_score_all &lt; 1.0                                                              </val>
        <val>  supplement.feed supplement='wheat', amount=0.40*stock.number_all, paddock='paddock1'     </val>
      </init>
      <init name="logfile" kind="string">
        <val />
      </init>
      <init name="log_set" kind="boolean">
        <val>false</val>
      </init>
      <init name="text" kind="string">
        <val />
      </init>
    </initsection>
  </CPIManager>
