<PlugIn>
   <toolbox>%apsim%\UserInterface\ToolBoxes\Standard.xml</toolbox>
   <toolbox>%apsim%\UserInterface\ToolBoxes\Graph.xml</toolbox>
   <toolbox>%apsim%\UserInterface\ToolBoxes\Sugar.xml</toolbox>
   <toolbox>%apsim%\UserInterface\ToolBoxes\Developers.xml</toolbox>
   <Type name="simulation">
      <MetaData>
         <UItype>VBUserInterface.AreaUI</UItype>
         <ShowInMainTree>Yes</ShowInMainTree>
         <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
         <LargeIcon>%apsim%\UserInterface\Images\document32.png</LargeIcon>
         <SmallIcon>%apsim%\UserInterface\Images\document16.png</SmallIcon>
         <drops>
            <drop>folder</drop>
            <drop>simulations</drop>
         </drops>
         <ApsimToSim>
            <simulation name="[simulation.name]" executable="%apsim%\Model\ProtocolManager.dll">
               <executable>%apsim%\Model\ProtocolManager.dll</executable>
               <title>[simulation.name]</title>
               [Children]
            </simulation>
         </ApsimToSim>
      </MetaData>
   </Type>
   <Type name="area">
      <MetaData>
         <UItype>VBUserInterface.AreaUI</UItype>
         <ShowInMainTree>Yes</ShowInMainTree>
         <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
         <LargeIcon>%apsim%\UserInterface\Images\layout_center32.png</LargeIcon>
         <SmallIcon>%apsim%\UserInterface\Images\layout_center16.png</SmallIcon>
         <drops>
            <drop>toolbox</drop>
            <drop>simulation</drop>
            <drop>folder</drop>
         </drops>
         <ApsimToSim>
            <system name="[area.name]" executable="%apsim%\Model\ProtocolManager.dll">
               <executable>%apsim%\Model\ProtocolManager.dll</executable>
               <component name="accum" executable="%apsim%\Model\Accum.dll">
                  <initdata>
                     <accum_variables>rain[15]</accum_variables>
                  </initdata>
               </component>
               [Children]
            </system>
         </ApsimToSim>
      </MetaData>
   </Type>
   <Type name="folder">
      <MetaData>
         <UItype>VBUserInterface.AreaUI</UItype>
         <ShowInMainTree>Yes</ShowInMainTree>
         <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
         <LargeIcon>%apsim%\UserInterface\Images\folder_closed32.png</LargeIcon>
         <SmallIcon>%apsim%\UserInterface\Images\folder_closed16.png</SmallIcon>
         <drops>
            <drop>area</drop>
            <drop>folder</drop>
            <drop>simulations</drop>
            <drop>toolbox</drop>
         </drops>
         <ApsimToSim>
            [Children]
         </ApsimToSim>
      </MetaData>
   </Type>
