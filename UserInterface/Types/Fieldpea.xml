   <fieldpea>
      <UItype>VBUserInterface.EmptyUI</UItype>
      <IsCrop>Yes</IsCrop>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\fieldpea.jpg</Image>
      <Description>Most APSIM crop or plant modules do not require extra user input.  Crop specific parameters are either provided for your chosen soil, or are captured as part of the standard model validation process.</Description>
      <Documentation name="Fieldpea science documentation">../apsim/fieldpea/docs/Working science document for fieldpea module.htm</Documentation>
      <Documentation name="Generic plant" >../apsim/plant/docs/plant_science.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\leaf32.bmp</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\leaf16.bmp</SmallIcon>
      <drops>
         <drop name="toolbox"/>
         <drop name="area"/>
         <drop name="simulation"/>
         <drop name="folder"/>
     </drops>
      <class    name="plant"/>
      <cultivar name="parvie"/>
      <cultivar name="excel"/>
      <cultivar name="parafield"/>
      <cultivar name="kaspa"/>
      <cultivar name="mutka"/>
      <variables link="%apsim%\Documentation\ModelInfo\fieldpea.xml"/>
      <events link="%apsim%\Documentation\ModelInfo\fieldpea.xml"/>
      <ApsimToSim>
         <type>component</type>
         <dll>%apsim%\Model\Plant.dll</dll>
         <ini>%apsim%\Model\FieldPea.xml</ini>
         <calldll>
            <class>Actions.SoilActions</class>
            <method>WriteCropSim</method>
         </calldll>
      </ApsimToSim>
   </fieldpea>