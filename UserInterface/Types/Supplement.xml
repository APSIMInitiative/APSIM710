   <supplement>
      <UItype>VBUserInterface.GenericUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\banner2.jpg</Image>
      <Documentation name="Component description">../apsim/supplement/docs/supplement Component Description.htm</Documentation>
      <Documentation name="Component drivers">../apsim/supplement/docs/supplement Component Drivers.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\herbage_green32.gif</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\herbage_green16.gif</SmallIcon>
      <drops>
         <drop name="toolbox"/>
         <drop name="area"/>
         <drop name="simulation"/>
         <drop name="folder"/>
     </drops>
     <variables link="%apsim%\Documentation\ModelInfo\supplement.xml"/>
      <events link="%apsim%\Documentation\ModelInfo\supplement.xml"/>
      <ApsimToSim>
         <type>xml</type>
         <dll>c:\program files\ausfarm\supplement.dll</dll>
         <sim>
  <component name="[supplement.name]" executable="c:\program files\ausfarm\supplement.dll">
    <executable>c:\program files\ausfarm\supplement.dll</executable>
    <initdata><![CDATA[
             <init name="spoilage_time" kind="double" unit="d">
               <val>0</val>
             </init>
               <init name="stores" array="T">
             </init>
           </initsection>

            ]]></initdata>
  </component>

    <component name="[supplement.name]ScienceConverter" executable="%apsim%\Model\supplementscienceconverter.dll">
    <executable>%apsim%\Model\supplementscienceconverter.dll</executable>
    <initdata>
      <debug type="list" listvalues="on,off" description="debug">off</debug>
      <supplement_module type="modulename" description="supplement_module">[supplement.name]</supplement_module>
    </initdata>
  </component>
       </sim>
      </ApsimToSim>
   </supplement>