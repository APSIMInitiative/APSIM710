   <sugar>
      <UItype>VBUserInterface.EmptyUI</UItype>
      <IsCrop>Yes</IsCrop>
      <ShowInMainTree>Yes</ShowInMainTree>
      <Image>%apsim%\UserInterface\Images\sugar.jpg</Image>
      <Documentation name="Sugar science documentation">../apsim/sugar/docs/sugar_science.htm</Documentation>
      <LargeIcon>%apsim%\UserInterface\Images\leaf32.bmp</LargeIcon>
      <SmallIcon>%apsim%\UserInterface\Images\leaf16.bmp</SmallIcon>
      <drops>
         <drop name="toolbox"/>
         <drop name="area"/>
         <drop name="simulation"/>
         <drop name="folder"/>
     </drops>
      <class    name="plant"/>
      <cultivar name="q117"/>
      <cultivar name="q117_ratoon"/>
      <cultivar name="q117_fum"/>
      <cultivar name="q117_fum_ratoon"/>
      <cultivar name="q96"/>
      <cultivar name="q96_ratoon"/>
      <cultivar name="q96_fum"/>
      <cultivar name="q96_fum_ratoon"/>
      <cultivar name="q138"/>
      <cultivar name="q138_ratoon"/>
      <cultivar name="q138_fum"/>
      <cultivar name="q138_fum_ratoon"/>
      <cultivar name="ts65-28"/>
      <cultivar name="ts65-28_ratoon"/>
      <cultivar name="ts65-28_fum"/>
      <cultivar name="ts65-28_fum_ratoon"/>
      <cultivar name="h73"/>
      <cultivar name="h73_ratoon"/>
      <cultivar name="q141"/>
      <cultivar name="q141_ratoon"/>
      <cultivar name="nco376"/>
      <cultivar name="nco376_ratoon"/>
      <cultivar name="n12"/>
      <cultivar name="n12_ratoon"/>
      <cultivar name="n14"/>
      <cultivar name="n14_ratoon"/>
      <cultivar name="CP51"/>
      <cultivar name="CP51_ratoon"/>
      <cultivar name="R570"/>
      <cultivar name="R570_ratoon"/>
      <cultivar name="M1356"/>
      <cultivar name="M1356_ratoon"/>
      <cultivar name="M55560"/>
      <cultivar name="M55560_ratoon"/>
      <cultivar name="q124"/>
      <cultivar name="q124_ratoon"/>
      <variables>
         <variable name="stage_name" description ="Sugar Stage name"/>
         <variable name="crop_status" description ="Sugar Crop status (dead,alive,out)"/>
         <variable name="ratoon_no" description ="Sugar Ratoon number"/>
         <variable name="ep" description ="Sugar daily crop water uptake (mm)"/>
         <variable name="dm_senesced" array="T" description ="Sugar senesced plant material (trash) (g/m2)"/>
         <variable name="n_senesced" array="T" description ="Sugar N in trash (g/m2)"/>
         <variable name="rlv" description ="Sugar root length density for each layer (mm/mm3)"/>
         <variable name="cep" description ="Sugar cumulative crop water uptake (mm)"/>
         <variable name="biomass" description ="Sugar total above-ground biomass (green + trash) (g/m2)"/>
         <variable name="green_biomass" description ="Sugar green biomass (g/m2)"/>
         <variable name="biomass_n" description ="Sugar total N in biomass (green + trash) (g/m2)"/>
         <variable name="cane_wt" description ="Sugar cane dry weight (g/m2)"/>
         <variable name="canefw" description ="Sugar cane fresh weight (t/ha)"/>
         <variable name="n_conc_cab" description ="Sugar N concentration in cabbage (g/g)"/>
         <variable name="n_conc_cane" description ="Sugar N concentration in cane (g/g)"/>
         <variable name="n_conc_leaf" description ="Sugar N concentration in green leaf (g/g)"/>
         <variable name="stage" description ="Sugar current phenological stage"/>
         <variable name="dlt_dm" description ="Sugar daily biomass production (g/m2)"/>
         <variable name="leaf_wt" description ="Sugar leaf weight (g/m2)"/>
         <variable name="root_wt" description ="Sugar root weight (g/m2)"/>
         <variable name="sstem_wt" description ="Sugar structural stem weight (g/m2)"/>
         <variable name="sucrose_wt" description ="Sugar sucrose weight (g/m2)"/>
         <variable name="cabbage_wt" description ="Sugar cabbage weight (g/m2)"/>
         <variable name="lai" description ="Sugar green leaf area index"/>
         <variable name="tlai" description ="Sugar total leaf area index (green and senesced)"/>
         <variable name="root_depth" description ="Sugar depth of roots (mm)"/>
         <variable name="slai" description ="Sugar senesced leaf area index"/>
         <variable name="sw_demand" description ="Sugar crop demand for water (potential uptake) (mm)"/>
         <variable name="green_biomass_n" description ="Sugar green biomass N (g/m2)"/>
         <variable name="n_demand" description ="Sugar nitrogen demand (g/m2)"/>
         <variable name="n_green" description ="Sugar green N for all parts (g/m2)"/>
         <variable name="nfact_photo" description ="Sugar Nitrogen stress factor on photosynthesis (0-1)"/>
         <variable name="nfact_expan" description ="Sugar Nitrogen stress factor on cell expansion (0-1)"/>
         <variable name="swdef_pheno" description ="Sugar water stress factor on phenology (0-1)"/>
         <variable name="swdef_photo" description ="Sugar water stress factor on photosynthesis (0-1)"/>
         <variable name="swdef_expan" description ="Sugar water stress factor on cell expansion (0-1)"/>
         <variable name="das" description ="Sugar days after sowing"/>
         <variable name="ep" description ="Sugar daily crop water uptake, Ep (mm)"/>
      </variables>
      <events>
         <event name = "harvesting"/>
         <event name = "sowing"/>
      </events>
      <ApsimToSim>
         <type>component</type>
         <dll>%apsim%\Model\Sugar.dll</dll>
         <ini>%apsim%\Model\Sugar.xml</ini>
         <sim>
            <uptake_source>calc</uptake_source>
            <rlv>0.04  0.02  0.01  .005  0.00  0.00 0.00</rlv>
         </sim>
         <calldll>
            <class>Actions.SoilActions</class>
            <method>WriteCropSim</method>
         </calldll>
      </ApsimToSim>
   </sugar>